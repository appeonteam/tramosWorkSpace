 retrieve="  SELECT VETT_REJECT.REJECTDATE,   
         VETT_REJECT.VESSELIMO,   
         VETT_REJECT.COMP_ID,   
         VETT_REJECT.RSNID,   
			VETT_REJREASON.REASON,
			VETT_REJREASON.VETT_USED,
         VETT_REJECT.DETAILS,   
         VETT_REJECT.INSP_ID,   
         VETT_REJECT.ACCEPTDATE,   
         VETT_REJECT.USERID,   
         VETT_COMP.NAME,   
         VETT_REJECT.REJECTID  
    FROM VETT_REJECT inner join VETT_COMP on VETT_REJECT.COMP_ID = VETT_COMP.COMP_ID
	INNER JOIN VETT_REJREASON On VETT_REJECT.RSNID=VETT_REJREASON.RSNID  
   WHERE VETT_REJECT.VESSELIMO = :VesselIMO
" update="VETT_REJECT" updatewhere=0 updatekeyinplace=no arguments=(("VesselIMO", number))  sort="rejectdate D " )
