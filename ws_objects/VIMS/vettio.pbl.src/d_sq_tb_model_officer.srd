 retrieve="  SELECT VETT_OBJ.OBJ_ID,
         VETT_OBJ.OBJNUM,   
         VETT_OBJ.OBJTYPE,   
         VETT_TEXT.TEXTDATA,   
         VETT_OBJ.DEFRISK,   
         VETT_OBJ.RISKRATING,
         VETT_OBJ.REQTYPE,
			   VETT_OBJ.EXTNUM,
         GUIDE.NOTE_TEXT as GUIDE,
         UNOTE.NOTE_TEXT as UNOTE,
         MJ.TEXTDATA as MAJOR,
         MJN.NOTE_TEXT as MAJORNOTE,
         MN.TEXTDATA as MINOR,
         MNN.NOTE_TEXT as MINORNOTE
    FROM (((((((VETT_OBJ INNER JOIN VETT_TEXT ON VETT_TEXT.TEXT_ID = VETT_OBJ.OBJTEXT)
          LEFT OUTER JOIN VETT_NOTES GUIDE ON VETT_OBJ.OBJNOTE = GUIDE.NOTES_ID)
          LEFT OUTER JOIN VETT_NOTES UNOTE ON VETT_OBJ.USERNOTE = UNOTE.NOTES_ID)
          LEFT OUTER JOIN VETT_TEXT MJ ON MJ.TEXT_ID = VETT_OBJ.SECTMAIN)
          LEFT OUTER JOIN VETT_NOTES MJN ON MJN.NOTES_ID = VETT_OBJ.SECTMAINNOTE)
          LEFT OUTER JOIN VETT_TEXT MN ON MN.TEXT_ID = VETT_OBJ.SECTHEAD)
          LEFT OUTER JOIN VETT_NOTES MNN ON MNN.NOTES_ID = VETT_OBJ.SECTHEADNOTE)
   WHERE (VETT_OBJ.PARENT = :Parent) and (VETT_OBJ.OBJTYPE >= :ObjType or VETT_OBJ.OBJTYPE = 4) and ((VETT_OBJ.OBJTYPE - :ObjType) < 2)
			and ((VETT_OBJ.OBJTYPE<3) or (:Officer=0) or (-EXTNUM/10000 = :Officer) or ((-EXTNUM/100)%100 = :Officer) or (-EXTNUM%100 = :Officer))
   ORDER BY OBJNUM
" arguments=(("Parent", number),("ObjType", number),("ParentNum", string),("Officer", number)) )
