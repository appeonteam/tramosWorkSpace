table(column=(type=decimal(0) updatewhereclause=yes key=yes identity=yes name=vessels_vessel_id dbname="VESSEL_ID" )
 column=(type=char(58) updatewhereclause=yes name=vslname dbname="VSLNAME" )
 column=(type=char(20) updatewhereclause=yes name=resp dbname="VETT_RESP" )
 column=(type=datetime updatewhereclause=yes name=lastsire dbname="LASTSIRE" )
 column=(type=datetime updatewhereclause=yes name=siredue dbname="SIREDUE" )
 column=(type=long updatewhereclause=yes name=overdue dbname="OVERDUE" )
 column=(type=long update=yes updatewhereclause=yes name=sireduestatus dbname="VETT_SIREDUESTATUS" )
 retrieve="Select VESSEL_ID,
	VESSEL_REF_NR + ' - ' + VESSEL_NAME VSLNAME, VETT_RESP,
	Max(INSPDATE) as LASTSIRE,
	Max(DateAdd(mm, INTERVAL, INSPDATE)) as SIREDUE,
	DateDiff(dd,Max(DateAdd(mm, INTERVAL, INSPDATE)), getdate()) OVERDUE, VETT_SIREDUESTATUS
	From VESSELS Inner Join VETT_VSLTYPE On VESSELS.VETT_TYPE = VETT_VSLTYPE.TYPE_ID
Left Outer Join VETT_INSP On VESSELS.IMO_NUMBER = VETT_INSP.VESSELIMO 
Left Outer Join VETT_INSPMODEL On VETT_INSPMODEL.IM_ID = VETT_INSP.IM_ID 
		
Where VESSEL_ACTIVE=1 and TYPE_NAME not like '%T/C%' and TYPE_NAME not like '%3P%' and SHORTNAME='SIRE' and VETT_INVIMS=1

Group By VESSEL_REF_NR, VESSEL_NAME, VETT_RESP, TYPE_NAME,VETT_SIREDUESTATUS
" update="VESSELS" updatewhere=0 updatekeyinplace=no )
