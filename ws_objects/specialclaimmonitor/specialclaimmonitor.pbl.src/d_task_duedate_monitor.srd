 column=(type=long updatewhereclause=yes name=agent_nr dbname="AGENT_NR" )
 column=(type=long updatewhereclause=yes name=broker_nr dbname="BROKER_NR" )
 column=(type=char(70) updatewhereclause=yes name=third_party_name dbname="THIRD_PARTY_NAME" )
 column=(type=char(100) updatewhereclause=yes name=special_claim_type dbname="SPECIAL_CLAIM_TYPE" )
 column=(type=char(800) updatewhereclause=yes name=action_description dbname="DESCRIPTION" )
 column=(type=datetime updatewhereclause=yes name=action_due_date dbname="DUE_DATE" )
 column=(type=char(20) updatewhereclause=yes name=action_assigned_to dbname="ASSIGNED_TO" )
 retrieve="  SELECT VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,   
         SPECIAL_CLAIM.DEBTOR_CREDITOR,   
         SPECIAL_CLAIM.CHART_NR,   
         SPECIAL_CLAIM.TCOWNER_NR,   
         SPECIAL_CLAIM.AGENT_NR,   
         SPECIAL_CLAIM.BROKER_NR,   
         SPECIAL_CLAIM.THIRD_PARTY_NAME,   
         SPECIAL_CLAIM.SPECIAL_CLAIM_TYPE,   
         SPECIAL_CLAIM_ACTION.DESCRIPTION,   
         SPECIAL_CLAIM_ACTION.DUE_DATE,   
         SPECIAL_CLAIM_ACTION.ASSIGNED_TO  
    FROM SPECIAL_CLAIM,   
         VESSELS,   
         SPECIAL_CLAIM_ACTION,
			(select USERID from USERS where DELETED = 0) ACTIVE_USERS 
   WHERE ( VESSELS.VESSEL_NR = SPECIAL_CLAIM.VESSEL_NR ) and  
         ( SPECIAL_CLAIM.SPECIAL_CLAIM_ID = SPECIAL_CLAIM_ACTION.SPECIAL_CLAIM_ID ) and  
			( SPECIAL_CLAIM_ACTION.ASSIGNED_TO = ACTIVE_USERS.USERID) and 
         ( ( SPECIAL_CLAIM_ACTION.USER_ACTION = 1 ) AND  
         ( SPECIAL_CLAIM_ACTION.DUE_DATE = convert(date, dateadd(dd, 1, getdate())) ) AND  
         ( SPECIAL_CLAIM_ACTION.FINISHED = 0 ) )    
" )
