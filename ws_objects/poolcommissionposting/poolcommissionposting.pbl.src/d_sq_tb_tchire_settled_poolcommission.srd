 column=(type=decimal(0) updatewhereclause=yes name=non_port_id dbname="NTC_COMMISSION.NON_PORT_ID" dbalias=".NON_PORT_ID" )
 column=(type=char(50) updatewhereclause=yes name=type_desc dbname="TYPE_DESC" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" dbalias=".VESSEL_REF_NR" )
 column=(type=number updatewhereclause=yes name=brokers_broker_pool_manager dbname="BROKERS.BROKER_POOL_MANAGER" dbalias=".BROKER_POOL_MANAGER" )
 column=(type=decimal(6) updatewhereclause=yes name=ntc_payment_ex_rate_usd dbname="NTC_PAYMENT.EX_RATE_USD" dbalias=".EX_RATE_USD" )
 column=(type=decimal(2) updatewhereclause=yes name=diff_amount dbname="DIFF_AMOUNT" )
 retrieve="  SELECT NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         NTC_TC_CONTRACT.TC_HIRE_CP_DATE,   
         NTC_TC_CONTRACT.CURR_CODE,   
         NTC_COMMISSION.INV_NR,   
        NTC_COMMISSION.AMOUNT,   
         NTC_COMMISSION.COMM_SETTLE_DATE,   
         NTC_COMMISSION.NTC_COMM_ID,   
         NTC_COMMISSION.PAYMENT_ID,   
         NTC_COMMISSION.BROKER_NR,   
         NTC_COMMISSION.VOYAGE_NR,
		(SELECT MIN(PERIODE_START) FROM NTC_PAYMENT_DETAIL WHERE PAYMENT_ID = NTC_COMMISSION.PAYMENT_ID) as PERIOD_START,
		(SELECT MAX(PERIODE_END) FROM NTC_PAYMENT_DETAIL WHERE PAYMENT_ID = NTC_COMMISSION.PAYMENT_ID) as PERIOD_END,
         NTC_COMMISSION.NON_PORT_ID, 
		NULL as TYPE_DESC,
		VESSELS.VESSEL_REF_NR,
		BROKERS.BROKER_POOL_MANAGER,
		NTC_PAYMENT.EX_RATE_USD,
		
		(select sum( N2.AMOUNT) from NTC_COMMISSION as N2
			where N2.NTC_COMM_ID =  NTC_COMMISSION.NTC_COMM_ID )  as DIFF_AMOUNT

    FROM NTC_COMMISSION,   
         NTC_PAYMENT,   
         NTC_TC_CONTRACT,   
         VESSELS, 
		BROKERS
   WHERE NTC_PAYMENT.PAYMENT_ID = NTC_COMMISSION.PAYMENT_ID and  
         NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID and  
         VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR and
		NTC_COMMISSION.COMM_SETTLE_DATE is not null and
		NTC_COMMISSION.BROKER_NR = BROKERS.BROKER_NR and
		BROKERS.BROKER_POOL_MANAGER = 1 and
		year((SELECT MIN(PAY1.PERIODE_START) FROM NTC_PAYMENT_DETAIL as PAY1 
				WHERE PAY1.PAYMENT_ID = NTC_COMMISSION.PAYMENT_ID )) > 2008 and
		NTC_PAYMENT.PAYMENT_STATUS<>5 and
		(select sum( N1.AMOUNT) from NTC_COMMISSION as N1
			where N1.NTC_COMM_ID =  NTC_COMMISSION.NTC_COMM_ID
				and  NTC_COMMISSION.COMM_SETTLE_DATE is not null  ) > 0 


UNION


  SELECT NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         NTC_TC_CONTRACT.TC_HIRE_CP_DATE,
	   	NTC_TC_CONTRACT.CURR_CODE,   
         NTC_COMMISSION.INV_NR,   
         NTC_COMMISSION.AMOUNT,   
         NTC_COMMISSION.COMM_SETTLE_DATE,   
         NTC_COMMISSION.NTC_COMM_ID,   
         NTC_COMMISSION.PAYMENT_ID,   
         NTC_COMMISSION.BROKER_NR,   
         NTC_COMMISSION.VOYAGE_NR,
		NULL as PERIOD_START,
		NULL as PERIOD_END,
         NTC_COMMISSION.NON_PORT_ID, 
		NTC_EXP_TYPE.TYPE_DESC as TYPE_DESC,
		VESSELS.VESSEL_REF_NR,
		BROKERS.BROKER_POOL_MANAGER,
		NTC_PAYMENT.EX_RATE_USD,
		(select sum( N2.AMOUNT) from NTC_COMMISSION as N2
			where N2.NTC_COMM_ID =  NTC_COMMISSION.NTC_COMM_ID )  as DIFF_AMOUNT

    FROM NTC_COMMISSION,   
         NTC_PAYMENT, 
	   	NTC_NON_PORT_EXP,
         NTC_TC_CONTRACT,
   	   	NTC_EXP_TYPE,
         VESSELS,
		BROKERS  
   WHERE NTC_NON_PORT_EXP.NON_PORT_ID = NTC_COMMISSION.NON_PORT_ID and
		NTC_PAYMENT.PAYMENT_ID = NTC_NON_PORT_EXP.PAYMENT_ID and  
         NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID and
	   	NTC_NON_PORT_EXP.EXP_TYPE_ID = NTC_EXP_TYPE.EXP_TYPE_ID and  
         VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR and  
		NTC_COMMISSION.COMM_SETTLE_DATE is not null and
		NTC_COMMISSION.BROKER_NR = BROKERS.BROKER_NR and
		BROKERS.BROKER_POOL_MANAGER = 1 AND
		year(NTC_NON_PORT_EXP.ACTIVITY_PERIOD)>2008 and
		NTC_PAYMENT.PAYMENT_STATUS<>5 and
		(select sum( N1.AMOUNT) from NTC_COMMISSION as N1
			where N1.NTC_COMM_ID =  NTC_COMMISSION.NTC_COMM_ID
				and  NTC_COMMISSION.COMM_SETTLE_DATE is not null  ) > 0 
	 " update="NTC_COMMISSION" updatewhere=0 updatekeyinplace=yes  sort="vessel_nr A cp_date A period_start A " )
