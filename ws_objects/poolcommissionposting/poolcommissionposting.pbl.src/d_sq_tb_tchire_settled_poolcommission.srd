 column=(type=datetime updatewhereclause=yes name=cp_date dbname="NTC_TC_CONTRACT.TC_HIRE_CP_DATE" dbalias=".TC_HIRE_CP_DATE" )
 column=(type=char(3) updatewhereclause=yes name=curr_code dbname="NTC_TC_CONTRACT.CURR_CODE" dbalias=".CURR_CODE" )
 column=(type=char(32) update=yes updatewhereclause=yes name=inv_nr dbname="NTC_COMMISSION.INV_NR" dbalias=".INV_NR" )
 column=(type=decimal(2) update=yes updatewhereclause=yes name=amount dbname="NTC_COMMISSION.AMOUNT" dbalias=".AMOUNT" )
 column=(type=datetime update=yes updatewhereclause=yes name=settle_date dbname="NTC_COMMISSION.COMM_SETTLE_DATE" dbalias=".COMM_SETTLE_DATE" values="	0/	NULL" )
 column=(type=decimal(0) updatewhereclause=yes key=yes identity=yes name=ntc_commission_ntc_comm_id dbname="NTC_COMMISSION.NTC_COMM_ID" dbalias=".NTC_COMM_ID" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=payment_id dbname="NTC_COMMISSION.PAYMENT_ID" dbalias=".PAYMENT_ID" )
 column=(type=long update=yes updatewhereclause=yes name=broker_nr dbname="NTC_COMMISSION.BROKER_NR" dbalias=".BROKER_NR" )
