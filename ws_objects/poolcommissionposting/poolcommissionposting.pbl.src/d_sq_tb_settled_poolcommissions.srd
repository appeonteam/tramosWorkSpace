table(column=(type=long update=yes updatewhereclause=yes name=commissions_vessel_nr dbname="COMMISSIONS.VESSEL_NR" dbalias=".VESSEL_NR" )
 column=(type=char(7) update=yes updatewhereclause=yes name=commissions_voyage_nr dbname="COMMISSIONS.VOYAGE_NR" dbalias=".VOYAGE_NR" )
 column=(type=long update=yes updatewhereclause=yes name=commissions_claim_nr dbname="COMMISSIONS.CLAIM_NR" dbalias=".CLAIM_NR" )
 column=(type=char(4) updatewhereclause=yes name=claims_claim_type dbname="CLAIMS.CLAIM_TYPE" dbalias=".CLAIM_TYPE" )
 column=(type=char(3) updatewhereclause=yes name=claims_curr_code dbname="CLAIMS.CURR_CODE" dbalias=".CURR_CODE" )
 column=(type=decimal(2) updatewhereclause=yes name=diff_amount_local dbname="DIFF_AMOUNT_LOCAL" )
 column=(type=char(32) updatewhereclause=yes name=max_invoice_nr dbname="MAX_INVOICE_NR" )
 column=(type=long update=yes updatewhereclause=yes name=commissions_chart_nr dbname="COMMISSIONS.CHART_NR" dbalias=".CHART_NR" )
 column=(type=long update=yes updatewhereclause=yes name=commissions_broker_nr dbname="COMMISSIONS.BROKER_NR" dbalias=".BROKER_NR" )
 column=(type=char(50) updatewhereclause=yes name=vessels_vessel_name dbname="VESSELS.VESSEL_NAME" dbalias=".VESSEL_NAME" )
 column=(type=long updatewhereclause=yes name=voyages_voyage_type dbname="VOYAGES.VOYAGE_TYPE" dbalias=".VOYAGE_TYPE" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" dbalias=".VESSEL_REF_NR" )
 retrieve="  SELECT distinct COMMISSIONS.VESSEL_NR,   
         COMMISSIONS.VOYAGE_NR,   
         COMMISSIONS.CLAIM_NR,   
         CLAIMS.CLAIM_TYPE,   
         CLAIMS.CURR_CODE, 
		(SELECT SUM(TABCOM.COMM_AMOUNT_LOCAL_CURR) FROM COMMISSIONS AS TABCOM
		WHERE TABCOM.BROKER_NR = COMMISSIONS.BROKER_NR AND
			TABCOM.CHART_NR = COMMISSIONS.CHART_NR AND 
			TABCOM.VESSEL_NR = COMMISSIONS.VESSEL_NR AND
			TABCOM.VOYAGE_NR = COMMISSIONS.VOYAGE_NR AND
			TABCOM.CLAIM_NR = COMMISSIONS.CLAIM_NR) as DIFF_AMOUNT_LOCAL ,

	(SELECT MAX(TABCOM.INVOICE_NR) FROM COMMISSIONS AS TABCOM
		WHERE TABCOM.BROKER_NR = COMMISSIONS.BROKER_NR AND
			TABCOM.CHART_NR = COMMISSIONS.CHART_NR AND 
			TABCOM.VESSEL_NR = COMMISSIONS.VESSEL_NR AND
			TABCOM.VOYAGE_NR = COMMISSIONS.VOYAGE_NR AND
			TABCOM.CLAIM_NR = COMMISSIONS.CLAIM_NR) as MAX_INVOICE_NR ,
        
		COMMISSIONS.CHART_NR,   
         COMMISSIONS.BROKER_NR,   
         VESSELS.VESSEL_NAME,   
         VOYAGES.VOYAGE_TYPE,   
         VESSELS.VESSEL_REF_NR  
    FROM COMMISSIONS,   
         CLAIMS,   
         VESSELS,   
         VOYAGES, BROKERS
   WHERE ( CLAIMS.CHART_NR = COMMISSIONS.CHART_NR ) and  
         ( CLAIMS.VESSEL_NR = COMMISSIONS.VESSEL_NR ) and  
         ( CLAIMS.VOYAGE_NR = COMMISSIONS.VOYAGE_NR ) and  
         ( CLAIMS.CLAIM_NR = COMMISSIONS.CLAIM_NR ) and  
         ( CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR ) and  
         ( VOYAGES.VESSEL_NR = CLAIMS.VESSEL_NR ) and  
         ( VOYAGES.VOYAGE_NR = CLAIMS.VOYAGE_NR ) and  
         COMMISSIONS.BROKER_NR = BROKERS.BROKER_NR and
		BROKERS.BROKER_POOL_MANAGER = 1 and
         ( COMMISSIONS.COMM_SETTLED = 1 ) AND  
		COMMISSIONS.VOYAGE_NR is not null and
		CONVERT(NUMERIC(4,0), LEFT(COMMISSIONS.VOYAGE_NR,2)) >8 and
		CONVERT(NUMERIC(4,0), LEFT(COMMISSIONS.VOYAGE_NR,2)) <50 and
		COMMISSIONS.COMM_AMOUNT > 0 and

	     CLAIMS.CLAIM_AMOUNT_USD - ( 
			SELECT ISNULL(SUM(CC.C_TRANS_AMOUNT_USD),0) FROM CLAIM_TRANSACTION as CC
				WHERE CC.VESSEL_NR =  CLAIMS.VESSEL_NR
					AND CC.VOYAGE_NR = CLAIMS.VOYAGE_NR
					AND CC.CHART_NR = CLAIMS.CHART_NR
					AND CC.CLAIM_NR = CLAIMS.CLAIM_NR 
					
		)>:maxbalance 
		AND
		(SELECT SUM(TABCOM.COMM_AMOUNT_LOCAL_CURR) FROM COMMISSIONS AS TABCOM
		WHERE TABCOM.BROKER_NR = COMMISSIONS.BROKER_NR AND
			TABCOM.CHART_NR = COMMISSIONS.CHART_NR AND 
			TABCOM.VESSEL_NR = COMMISSIONS.VESSEL_NR AND
			TABCOM.VOYAGE_NR = COMMISSIONS.VOYAGE_NR AND
			TABCOM.CLAIM_NR = COMMISSIONS.CLAIM_NR and
			TABCOM.COMM_SETTLED = 1
		) >0

ORDER BY COMMISSIONS.VESSEL_NR,COMMISSIONS.VOYAGE_NR, COMMISSIONS.CLAIM_NR, COMMISSIONS.BROKER_NR, COMMISSIONS.CHART_NR  ASC   
" arguments=(("maxbalance", number)) )
