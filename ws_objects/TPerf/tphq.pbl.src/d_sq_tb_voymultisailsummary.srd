 column=(type=decimal(1) updatewhereclause=yes name=lmeaecons dbname="LMEAECONS" )
 column=(type=decimal(1) updatewhereclause=yes name=bmeaecons dbname="BMEAECONS" )
 column=(type=decimal(1) updatewhereclause=yes name=lmecons dbname="LMECONS" )
 column=(type=decimal(1) updatewhereclause=yes name=bmecons dbname="BMECONS" )
 column=(type=long updatewhereclause=yes name=wrrtype dbname="WRRTYPE" )
 retrieve="  SELECT TPERF_SAILDATA.TYPE,
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 0 THEN TPERF_SAILDATA.PERIOD ELSE 0 END) as LPERIOD,   
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 1 THEN TPERF_SAILDATA.PERIOD ELSE 0 END) as BPERIOD,   
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 0 THEN TPERF_SAILDATA.DIST ELSE 0 END) as LDIST,   
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 1 THEN TPERF_SAILDATA.DIST ELSE 0 END) as BDIST,
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 0 THEN TPERF_SAILDATA.MEHFO + TPERF_SAILDATA.MELSHFO + TPERF_SAILDATA.MEDO + TPERF_SAILDATA.MEGO + TPERF_REPCON.QTY_HFO + TPERF_REPCON.QTY_LSHFO + TPERF_REPCON.QTY_DO + TPERF_REPCON.QTY_GO ELSE 0 END) as LMEAECONS,
         SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 1 THEN TPERF_SAILDATA.MEHFO + TPERF_SAILDATA.MELSHFO + TPERF_SAILDATA.MEDO + TPERF_SAILDATA.MEGO + TPERF_REPCON.QTY_HFO + TPERF_REPCON.QTY_LSHFO + TPERF_REPCON.QTY_DO + TPERF_REPCON.QTY_GO ELSE 0 END) as BMEAECONS,
			SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 0 THEN TPERF_SAILDATA.MEHFO + TPERF_SAILDATA.MELSHFO + TPERF_SAILDATA.MEDO + TPERF_SAILDATA.MEGO ELSE 0 END) AS LMECONS,
			SUM(CASE WHEN TPERF_VOY.VOY_TYPE = 1 THEN TPERF_SAILDATA.MEHFO + TPERF_SAILDATA.MELSHFO + TPERF_SAILDATA.MEDO + TPERF_SAILDATA.MEGO ELSE 0 END) AS BMECONS,
         MAX(VESSELS.TPERF_WRRTYPE) as WRRTYPE
    FROM ((((TPERF_REPCON RIGHT OUTER JOIN TPERF_REPORTS ON TPERF_REPCON.REP_ID = TPERF_REPORTS.REP_ID)
         INNER JOIN TPERF_SAILDATA ON TPERF_SAILDATA.REP_ID = TPERF_REPORTS.REP_ID)
         INNER JOIN TPERF_VOY ON TPERF_VOY.VOY_ID = TPERF_REPORTS.VOY_ID)
         INNER JOIN VESSELS ON TPERF_VOY.VESSEL_ID = VESSELS.VESSEL_ID)
   WHERE ( VESSELS.VESSEL_ID = :VesselID ) AND
         ( TPERF_REPCON.CONTYPE = TPERF_SAILDATA.TYPE) AND
         ( TPERF_VOY.EXCLUDE = 0 ) AND
         ( CharIndex(~"#~" + Cast(TPERF_VOY.VOY_ID as VarChar) + ~"#~", :VoyIDString) > 0 )
GROUP BY TPERF_SAILDATA.TYPE
ORDER BY TPERF_SAILDATA.TYPE ASC   
" arguments=(("VesselID", number),("VoyIDString", string)) )
