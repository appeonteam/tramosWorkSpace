 column=(type=decimal(2) updatewhereclause=yes name=lperiod dbname="LPERIOD" )
 column=(type=decimal(2) updatewhereclause=yes name=bperiod dbname="BPERIOD" )
 column=(type=decimal(1) updatewhereclause=yes name=ldist dbname="LDIST" )
