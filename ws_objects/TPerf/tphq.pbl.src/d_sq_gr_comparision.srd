table(column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" dbalias=".VESSEL_REF_NR" )
 column=(type=char(50) updatewhereclause=yes name=vessels_vessel_name dbname="VESSELS.VESSEL_NAME" dbalias=".VESSEL_NAME" )
 column=(type=decimal(1) updatewhereclause=yes name=lddis dbname="LdDis" )
 column=(type=decimal(2) updatewhereclause=yes name=ldper dbname="LdPer" )
 column=(type=decimal(1) updatewhereclause=yes name=bldis dbname="BlDis" )
 column=(type=decimal(2) updatewhereclause=yes name=blper dbname="BlPer" )
 column=(type=decimal(1) updatewhereclause=yes name=ldcon dbname="LdCon" )
 column=(type=decimal(1) updatewhereclause=yes name=blcon dbname="BlCon" )
 retrieve="  SELECT VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,   
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 0 THEN TPERF_SAILDATA.DIST ELSE 0 END) as LdDis,    
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 0 THEN TPERF_SAILDATA.PERIOD ELSE 0 END) as LdPer,   
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 1 THEN TPERF_SAILDATA.DIST ELSE 0 END) as BlDis,    
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 1 THEN TPERF_SAILDATA.PERIOD ELSE 0 END) as BlPer,
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 0 THEN (TPERF_SAILDATA.MEHFO+TPERF_SAILDATA.MEDO+TPERF_SAILDATA.MELSHFO+TPERF_SAILDATA.MEGO) ELSE 0 END) as LdCon,
			Sum(CASE TPERF_VOY.VOY_TYPE WHEN 1 THEN (TPERF_SAILDATA.MEHFO+TPERF_SAILDATA.MEDO+TPERF_SAILDATA.MELSHFO+TPERF_SAILDATA.MEGO) ELSE 0 END) as BlCon   
   from  TPERF_REPORTS,   
         TPERF_SAILDATA,   
         TPERF_VOY,   
         VESSELS  
   WHERE ( TPERF_SAILDATA.REP_ID = TPERF_REPORTS.REP_ID ) and  
         ( TPERF_VOY.VOY_ID = TPERF_REPORTS.VOY_ID ) and  
         ( TPERF_VOY.VESSEL_ID = VESSELS.VESSEL_ID ) and
         ( VESSELS.VESSEL_ACTIVE = 1 ) and
         ( TPERF_VOY.EXCLUDE = 0) and
         ( VESSELS.TCOWNER_NR IN ( :Owner ) or VESSELS.TCOWNER_NR is Null) and 
         ( VESSELS.CAL_VEST_TYPE_ID IN ( :shipType )) and
         ( VESSELS.YARD_ID IN (:Yard ) or VESSELS.YARD_ID is Null) and
         ( TPERF_SAILDATA.TYPE IN ( :Sail )) and
				 ( TPERF_REPORTS.UTC >= :FromDate) and
         ( TPERF_REPORTS.UTC <= :ToDate) and
         ( VESSELS.PC_NR IN (Select PC_NR from USERS_PROFITCENTER Where USERID = :UserID))
GROUP BY VESSELS.VESSEL_REF_NR   
" arguments=(("shiptype", numberlist),("owner", numberlist),("yard", numberlist),("sail", numberlist),("UserID", string),("FromDate", date),("ToDate", date)) )
