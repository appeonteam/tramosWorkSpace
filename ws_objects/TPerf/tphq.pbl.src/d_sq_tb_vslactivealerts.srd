 retrieve="  SELECT VESSELS.VESSEL_REF_NR + ' - ' + VESSELS.VESSEL_NAME as VSLNAME,
         TPERF_VOY.VOY_NUM,   
         TPERF_ALERTS.REP_SERIAL,   
         TPERF_ALERTLIST.TITLE,
         TPERF_REPORTS.TYPE  
    FROM VESSELS Inner Join TPERF_VOY On VESSELS.VESSEL_ID = TPERF_VOY.VESSEL_ID
         Inner Join TPERF_ALERTS On TPERF_VOY.VOY_ID = TPERF_ALERTS.VOY_ID
         Inner Join TPERF_ALERTLIST On TPERF_ALERTS.ALERT_TYPEID = TPERF_ALERTLIST.TYPE_ID
         Left Outer Join TPERF_REPORTS On TPERF_REPORTS.VOY_ID = TPERF_ALERTS.VOY_ID and TPERF_REPORTS.SERIAL = TPERF_ALERTS.REP_SERIAL
   WHERE TPERF_ALERTS.ACK = 0 and
         VESSELS.VESSEL_ID = :VesselID
" arguments=(("VesselID", number))  sort="voy_num A tperf_alerts_rep_serial A " )
