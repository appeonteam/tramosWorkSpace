table(column=(type=char(150) updatewhereclause=yes name=yardname dbname="YARD.YARDNAME" )
 column=(type=decimal(0) updatewhereclause=yes name=yard_id dbname="YARD.YARD_ID" )
 column=(type=long updatewhereclause=yes name=voycount dbname="VOYCOUNT" )
 retrieve="  SELECT YARD.YARDNAME,   
         YARD.YARD_ID,
         Count(VOY_ID) as VOYCOUNT
    FROM YARD inner join VESSELS on YARD.YARD_ID = VESSELS.YARD_ID
     left outer join TPERF_VOY on TPERF_VOY.VESSEL_ID = VESSELS.VESSEL_ID
  WHERE (VESSELS.TCOWNER_NR in (:OwnerList) or VESSELS.TCOWNER_NR is null)and
         (VESSELS.PC_NR in (Select PC_NR from USERS_PROFITCENTER Where USERID=:UserID))
  GROUP BY YARD.YARDNAME, YARD.YARD_ID  
" arguments=(("OwnerList", numberlist),("UserID", string)) )
