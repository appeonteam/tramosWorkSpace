    FROM TPERF_HRBR,   
         TPERF_HRBRCON,   
         TPERF_VOY  
   WHERE ( TPERF_HRBRCON.H_ID = TPERF_HRBR.H_ID ) and  
         ( TPERF_VOY.VOY_ID = TPERF_HRBR.VOY_ID ) and  
         ( TPERF_VOY.VESSEL_ID = :VesselID ) AND  
         ( CharIndex('#' + Cast(TPERF_VOY.VOY_ID as VarChar) + '#', :VoyIDString) > 0) AND  
