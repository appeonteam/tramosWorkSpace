 column=(type=number updatewhereclause=yes name=cal_cerp_id dbname="CAL_CERP_ID" )
 retrieve="SELECT 
	CL1.VOYAGE_NR, 
	CL1.VESSEL_NR, 
	CL1.CHART_NR, 
	CL1.CLAIM_NR, 
	CL1.CLAIM_TYPE, 
	CL1.CURR_CODE,
	CLAIM_AMOUNT, 
	CLAIM_AMOUNT_USD,
	isnull(CL1.CLAIM_PERCENTAGE, 0) CLAIM_PERCENTAGE,
	(SELECT 
		ISNULL(sum(round(ISNULL(TR1.C_TRANS_AMOUNT,0),2)),0)  
	FROM 
		CLAIM_TRANSACTION TR1 
	WHERE 
		CL1.VESSEL_NR = TR1 .VESSEL_NR AND  
		CL1.VOYAGE_NR = TR1.VOYAGE_NR AND 
		CL1.CHART_NR = TR1.CHART_NR AND 
		CL1.CLAIM_NR = TR1.CLAIM_NR)  
	AS TRANS_AMT, 
	(SELECT 
		ISNULL(sum(ISNULL(TR2.C_TRANS_AMOUNT_USD,0)),0)  
	FROM 
		CLAIM_TRANSACTION TR2 
	WHERE 
		CL1.VESSEL_NR = TR2.VESSEL_NR AND 
		CL1.VOYAGE_NR = TR2.VOYAGE_NR AND  
		CL1.CHART_NR = TR2.CHART_NR AND
		CL1.CLAIM_NR = TR2.CLAIM_NR)  
		AS TRANS_AMT_USD,
	CL1.CAL_CERP_ID
FROM 
	CLAIMS CL1
WHERE 
	CL1.VOYAGE_NR=:voyage_nr AND
	CL1.VESSEL_NR=:vessel_nr AND
	CL1.CHART_NR=:chart_nr AND
	CL1.CLAIM_NR=:claim_nr
" update="CLAIMS" updatewhere=0 updatekeyinplace=yes arguments=(("vessel_nr", number),("voyage_nr", string),("chart_nr", number),("claim_nr", number)) )
