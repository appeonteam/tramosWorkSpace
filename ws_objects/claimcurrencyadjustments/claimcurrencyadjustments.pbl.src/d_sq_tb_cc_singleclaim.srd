 column=(type=long updatewhereclause=yes key=yes name=vessel_nr dbname="CLAIMS.VESSEL_NR" )
 column=(type=long updatewhereclause=yes key=yes name=chart_nr dbname="CLAIMS.CHART_NR" )
 column=(type=long updatewhereclause=yes key=yes name=claim_nr dbname="CLAIMS.CLAIM_NR" )
 column=(type=char(4) updatewhereclause=yes name=claim_type dbname="CLAIMS.CLAIM_TYPE" )
 column=(type=char(3) updatewhereclause=yes name=curr_code dbname="CLAIMS.CURR_CODE" )
 column=(type=number updatewhereclause=yes name=claimamount_local dbname="CLAIMS.CLAIM_AMOUNT" )
 column=(type=number update=yes updatewhereclause=yes name=claimamount_usd dbname="CLAIMS.CLAIM_AMOUNT_USD" )
