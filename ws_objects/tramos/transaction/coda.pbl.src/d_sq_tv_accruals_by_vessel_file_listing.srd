		   VOYAGE_DOCUMENT.VOYAGE_NR,
			~"(~" + VESSELS.VESSEL_REF_NR + ~") ~" + VESSELS.VESSEL_NAME as VESSEL_FULLNAME,
		VESSELS.PC_NR,
		PROFIT_C.PC_NAME,
		VOYAGE_DOCUMENT.DB_NAME
