 column=(type=long updatewhereclause=yes name=buy_sell dbname="BP_DETAILS.BUY_SELL" dbalias=".BUY_SELL" )
 column=(type=char(5) updatewhereclause=yes name=tc_type dbname="TC_TYPE" )
 retrieve="	SELECT ISNULL(sum(round(BP_DETAILS.LIFTED_HFO * isnull(BP_DETAILS.PRICE_HFO,0) * isnull(BP_DETAILS.EX_RATE_TO_TC, 0) /100, 2))   
			 + sum(round(BP_DETAILS.LIFTED_DO * isnull(BP_DETAILS.PRICE_DO,0) * isnull(BP_DETAILS.EX_RATE_TO_TC, 0) /100, 2))   
			 + sum(round(BP_DETAILS.LIFTED_GO * isnull(BP_DETAILS.PRICE_GO,0) * isnull(BP_DETAILS.EX_RATE_TO_TC, 0) /100, 2))   
			 + sum(round(BP_DETAILS.LIFTED_LSHFO * isnull(BP_DETAILS.PRICE_LSHFO,0) * isnull(BP_DETAILS.EX_RATE_TO_TC, 0) /100, 2)),0) AS AMOUNT_LOCAL,
			 sum(BP_DETAILS.LIFTED_HFO) + sum(BP_DETAILS.LIFTED_DO) + sum(BP_DETAILS.LIFTED_GO) + sum(BP_DETAILS.LIFTED_LSHFO) AS BUNKER_TON,
            BP_DETAILS.BUY_SELL,
            CASE WHEN VOYAGES.VOYAGE_TYPE = 2 THEN 'TCOUT' ELSE 'TCIN' END AS TC_TYPE
		FROM BP_DETAILS,VOYAGES  
		WHERE BP_DETAILS.VESSEL_NR = VOYAGES.VESSEL_NR 
        AND BP_DETAILS.VOYAGE_NR = VOYAGES.VOYAGE_NR  
        AND BP_DETAILS.PAYMENT_ID = :ai_paymentid
		AND (BP_DETAILS.OWNER_POOL_PURCHASE_OR_CHART = 2 AND VOYAGES.VOYAGE_TYPE = 2 OR BP_DETAILS.OWNER_POOL_PURCHASE_OR_CHART = 0 AND VOYAGES.VOYAGE_TYPE <> 2)
    GROUP BY BP_DETAILS.PAYMENT_ID, BP_DETAILS.OWNER_POOL_PURCHASE_OR_CHART,VOYAGE_TYPE, BP_DETAILS.BUY_SELL
" arguments=(("ai_paymentid", number))  sort="buy_sell A " )
