 column=(type=char(70) updatewhereclause=yes name=chart_chart_n_1 dbname="CHART.CHART_N_1" )
 column=(type=char(35) updatewhereclause=yes name=tcowners_tcowner_n_1 dbname="TCOWNERS.TCOWNER_N_1" )
 column=(type=char(3) updatewhereclause=yes name=ntc_tc_contract_curr_code dbname="NTC_TC_CONTRACT.CURR_CODE" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=payment_id dbname="NTC_PAYMENT.PAYMENT_ID" )
 column=(type=long updatewhereclause=yes name=vessel_nr dbname="NTC_TC_CONTRACT.VESSEL_NR" )
 column=(type=char(50) updatewhereclause=yes name=vessel_name dbname="VESSELS.VESSEL_NAME" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=contract_id dbname="NTC_TC_CONTRACT.CONTRACT_ID" )
 column=(type=number updatewhereclause=yes name=tc_hire_in dbname="NTC_TC_CONTRACT.TC_HIRE_IN" )
 column=(type=long updatewhereclause=yes name=vessels_pc_nr dbname="VESSELS.PC_NR" )
 column=(type=decimal(2) updatewhereclause=yes name=ntc_payment_transfer_to_next dbname="TRANSFER_TO_NEXT" )
 column=(type=decimal(2) updatewhereclause=yes name=payment_balance dbname="PAYMENT_BALANCE" )
 column=(type=decimal(0) updatewhereclause=yes name=min_id dbname="MIN_ID" )
 column=(type=long updatewhereclause=yes name=trans_to_coda dbname="NTC_PAYMENT.TRANS_TO_CODA" values="	1/	0" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 column=(type=number updatewhereclause=yes name=ntc_payment_locked dbname="NTC_PAYMENT.LOCKED" values="	1/	0" )
 column=(type=char(200) updatewhereclause=yes name=ntc_payment_ax_invoice_text dbname="NTC_PAYMENT.AX_INVOICE_TEXT" )
 retrieve="  SELECT NTC_PAYMENT.INCOME,   
         NTC_PAYMENT.PAYMENT_STATUS,   
         NTC_PAYMENT.EST_DUE_DATE,   
         isnull(NTC_PAYMENT.ADR_COMM,0) as ADR_COMM,   
         NTC_PAYMENT.SETTLE_DATE,   
         NTC_PAYMENT.EX_RATE_USD, 
			NTC_PAYMENT.PRINT_DATE,
			NTC_PAYMENT.INVOICE_NR,  
         (SELECT isnull(SUM(NTC_PAYMENT_DETAIL.QUANTITY * NTC_PAYMENT_DETAIL.RATE),0) 
					FROM  NTC_PAYMENT_DETAIL
					WHERE NTC_PAYMENT_DETAIL.PAYMENT_ID =  NTC_PAYMENT.PAYMENT_ID) AS SUM_DETAIL_AMOUNT,
         CHART.CHART_N_1,   
         TCOWNERS.TCOWNER_N_1,   
         NTC_TC_CONTRACT.CURR_CODE,   
         NTC_PAYMENT.PAYMENT_ID,   
         NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         NTC_TC_CONTRACT.CONTRACT_ID,   
         NTC_TC_CONTRACT.TC_HIRE_IN,
			VESSELS.PC_NR,
			isnull(NTC_PAYMENT.TRANSFER_TO_NEXT,0) as TRANSFER_TO_NEXT,
			isnull(NTC_PAYMENT.PAYMENT_BALANCE,0) as PAYMENT_BALANCE,
			(SELECT MIN(P2.PAYMENT_ID) FROM NTC_PAYMENT P2 WHERE P2.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID) AS MIN_ID,
		   NTC_PAYMENT.TRANS_TO_CODA,
			VESSELS.VESSEL_REF_NR,
			NTC_PAYMENT.LOCKED,
			NTC_PAYMENT.AX_INVOICE_TEXT
    FROM NTC_PAYMENT,   
         CHART,   
         NTC_TC_CONTRACT,   
         TCOWNERS,   
         VESSELS  
   WHERE NTC_TC_CONTRACT.CHART_NR *= CHART.CHART_NR and  
         TCOWNERS.TCOWNER_NR =* NTC_TC_CONTRACT.TCOWNER_NR and  
         NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID and  
         VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR and  
         NTC_TC_CONTRACT.CONTRACT_ID = :contract_id  and
			( NTC_PAYMENT.PAYMENT_STATUS in (:ra_payment_status) or (NTC_PAYMENT.PAYMENT_STATUS = 5 and NTC_PAYMENT.PAYMENT_BALANCE <> 0)) 
ORDER BY NTC_PAYMENT.EST_DUE_DATE ASC   
" arguments=(("contract_id", number),("ra_payment_status", numberlist))  sort="est_due_date A " )
