 retrieve="  SELECT NTC_PORT_EXP.CURR_CODE,   
         NTC_PORT_EXP.EXP_DESC,   
         NTC_PORT_EXP.EXP_AMOUNT,   
         NTC_PORT_EXP.EX_RATE_TC,   
         NTC_PORT_EXP.EXP_FOR_OA,   
         NTC_PORT_EXP.USE_IN_VAS,   
         NTC_PAYMENT.SETTLE_DATE,   
         NTC_PORT_EXP.PORT_EXP_ID,   
         NTC_PORT_EXP.PAYMENT_ID,   
         NTC_PORT_EXP.MATCH_PORT_EXP_ID,   
         NTC_TC_CONTRACT.CONTRACT_ID,   
         NTC_TC_CONTRACT.CURR_CODE,   
         isNull(NTC_PAYMENT.PAYMENT_STATUS,0) AS PAYMENT_STATUS,   
         NTC_PORT_EXP.DISB_INVOICE_NR,   
         NTC_PORT_EXP.STATEMENT_DESCRIPTION  
    FROM NTC_PORT_EXP,   
         NTC_PAYMENT,   
         NTC_TC_CONTRACT  
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_PORT_EXP.PAYMENT_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( ( NTC_TC_CONTRACT.CONTRACT_ID = :contract_id ) )   
ORDER BY NTC_PORT_EXP.EXP_AMOUNT ASC   
" update="NTC_PORT_EXP" updatewhere=0 updatekeyinplace=no arguments=(("contract_id", number)) )
