 column=(type=long updatewhereclause=yes name=ntc_tc_contract_vessel_nr dbname="NTC_TC_CONTRACT.VESSEL_NR" )
 retrieve="SELECT  NTC_TC_CONTRACT.CONTRACT_ID ,
           NTC_TC_CONTRACT.TC_HIRE_CP_DATE ,
           CHART.CHART_N_1 ,
           TCOWNERS.TCOWNER_N_1 ,
           NTC_TC_CONTRACT.TC_HIRE_IN ,
           NTC_TC_CONTRACT.IN_POOL,
           NTC_TC_CONTRACT.BAREBOAT,
			  NTC_TC_CONTRACT.DELIVERY,
				NTC_TC_CONTRACT.VESSEL_NR
        FROM NTC_TC_CONTRACT ,
           CHART ,
           TCOWNERS     
        WHERE ( NTC_TC_CONTRACT.TCOWNER_NR *= TCOWNERS.TCOWNER_NR) and
          ( CHART.CHART_NR =* NTC_TC_CONTRACT.CHART_NR) and 
         ( ( NTC_TC_CONTRACT.VESSEL_NR = :ai_vessel ) )
			ORDER BY NTC_TC_CONTRACT.DELIVERY  " arguments=(("ai_vessel", number))  sort="delivery A " )
