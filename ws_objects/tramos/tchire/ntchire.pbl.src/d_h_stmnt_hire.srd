 column=(type=decimal(2) updatewhereclause=yes name=adr_comm dbname="ADR_COMM" )
 column=(type=decimal(2) updatewhereclause=yes name=broker_commission dbname="BROKER_COMMISSION" )
 column=(type=decimal(2) updatewhereclause=yes name=base_amount dbname="BASE_AMOUNT" )
