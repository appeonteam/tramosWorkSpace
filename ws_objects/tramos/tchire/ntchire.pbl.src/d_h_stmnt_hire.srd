 retrieve="  SELECT P1.EST_DUE_DATE,   
         PD1.PERIODE_START,   
         PD1.PERIODE_END,   
         PD1.MONTHLY_RATE,   
         PD1.QUANTITY,   
         PD1.RATE,   
 	      P1.PAYMENT_STATUS,   
         P1.PAYMENT_ID,
         isnull(P1.ADR_COMM_PCT,0) as ADR_COMM_PCT, 
  			isnull(P1.BROKER_COMMISSION_PCT,0) as BROKER_COMMISSION_PCT,   
         isnull(P1.ADR_COMM,0) as ADR_COMM,   
         isnull(P1.BROKER_COMMISSION,0) as BROKER_COMMISSION,   
         0.00 as BASE_AMOUNT,
			isnull(P1.BROKER_COMMISSION_PER_DAY,0) as BROKER_COMMISSION_PER_DAY,
			0.0000 as BASE_DAYS  
    FROM NTC_PAYMENT P1,   
         NTC_PAYMENT_DETAIL PD1
    WHERE ( PD1.PAYMENT_ID = P1.PAYMENT_ID ) and  
			( P1.PAYMENT_ID = :paymentid OR 
         ( P1.CONTRACT_ID = :contractid AND 
					P1.PAYMENT_ID >= :paymentid_low AND 
         		P1.PAYMENT_ID <= :paymentid ))	
   ORDER BY P1.EST_DUE_DATE   


  " arguments=(("paymentid", number),("paymentid_low", number),("contractid", number)) )
