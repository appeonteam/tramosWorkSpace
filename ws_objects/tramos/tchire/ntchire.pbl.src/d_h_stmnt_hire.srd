 column=(type=long update=yes updatewhereclause=yes name=payment_status dbname="NTC_PAYMENT.PAYMENT_STATUS" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=payment_id dbname="NTC_PAYMENT.PAYMENT_ID" )
 column=(type=decimal(2) updatewhereclause=yes name=adr_comm_pct dbname="ADR_COMM_PCT" )
