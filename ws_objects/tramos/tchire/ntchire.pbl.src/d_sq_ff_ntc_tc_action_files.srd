 retrieve="  SELECT ATTACHMENTS.FILE_ID,   
         ATTACHMENTS.FILE_NAME,   
         ATTACHMENTS.DESCRIPTION,   
         ATTACHMENTS.CREATED_BY,   
         ATTACHMENTS.CREATED_DATE,   
         ATTACHMENTS.UPDATED_BY,   
         ATTACHMENTS.UPDATED_DATE,   
         ATTACHMENTS.MODULE_NAME,   
         ATTACHMENTS.DB_NAME,   
         NTC_TC_ACTION.CONTRACT_ID,   
         NTC_TC_ACTION.ACTION_ASSIGNED_TO,   
         NTC_TC_ACTION.ACTION_FINISHED,   
         NTC_TC_ACTION.ACTION_DUE_DATE,   
          0 as insert_index,    
         NTC_TC_ACTION.ACTION_DATE  
    FROM ATTACHMENTS,   
         NTC_TC_ACTION  
   WHERE ( NTC_TC_ACTION.FILE_ID = ATTACHMENTS.FILE_ID ) and  
         ( ( NTC_TC_ACTION.CONTRACT_ID = :contract_id ) )    
