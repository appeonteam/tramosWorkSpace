 retrieve="SELECT  CHART.CHART_N_1 ,
           TCOWNERS.TCOWNER_N_1 ,
           NTC_TC_CONTRACT.CONTRACT_ID ,
           NTC_PAYMENT.PAYMENT_ID ,
           NTC_PAYMENT.INCOME ,
           NTC_PAYMENT.EST_DUE_DATE ,
           NTC_PAYMENT.PAYMENT_STATUS ,
           isnull(NTC_PAYMENT.BROKER_COMMISSION,0) as BROKER_COMMISSION ,
           isnull(NTC_PAYMENT.ADR_COMM,0) as ADR_COMM ,
           NTC_PAYMENT.SETTLE_DATE ,
           NTC_PAYMENT.EX_RATE_USD ,
           NTC_PAYMENT_DETAIL.PERIODE_START ,
           NTC_PAYMENT_DETAIL.PERIODE_END ,
           NTC_PAYMENT_DETAIL.MONTHLY_RATE ,
           NTC_PAYMENT_DETAIL.QUANTITY ,
           NTC_PAYMENT_DETAIL.RATE ,
           NTC_TC_CONTRACT.VESSEL_NR ,
           NTC_TC_CONTRACT.CURR_CODE ,
           VESSELS.VESSEL_NAME ,
           NTC_TC_CONTRACT.TC_HIRE_IN ,
           VESSELS.PC_NR ,
           NTC_TC_CONTRACT.TC_HIRE_CP_DATE,
			  isnull(NTC_PAYMENT.TRANSFER_TO_NEXT,0) as TRANSFER_TO_NEXT,
			  isnull(NTC_PAYMENT.PAYMENT_BALANCE,0) as PAYMENT_BALANCE,
			  VESSELS.VESSEL_REF_NR     
        FROM CHART ,
           NTC_PAYMENT ,
           NTC_PAYMENT_DETAIL ,
           NTC_TC_CONTRACT ,
           TCOWNERS ,
           VESSELS     
        WHERE ( NTC_PAYMENT_DETAIL.PAYMENT_ID =* NTC_PAYMENT.PAYMENT_ID) and          ( NTC_TC_CONTRACT.CHART_NR *= CHART.CHART_NR) and          ( TCOWNERS.TCOWNER_NR =* NTC_TC_CONTRACT.TCOWNER_NR) and          ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and          ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and          ( ( NTC_PAYMENT.PAYMENT_ID = :payment_id ) )  " arguments=(("payment_id", number)) )
