 retrieve="  SELECT NTC_EXP_TYPE.TYPE_DESC,   
         NTC_OFS_DEPENDENT_CONTRACT_EXP.DEDUCTABLE_AMOUNT,   
         NTC_OFF_SERVICE.START_DATE,
	   NTC_OFF_SERVICE.END_DATE  
    FROM NTC_OFS_DEPENDENT_CONTRACT_EXP,
	   NTC_OFF_SERVICE,   
         NTC_PAYMENT,   
         NTC_EXP_TYPE  
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID ) and
	   ( NTC_OFF_SERVICE.OFF_SERVICE_ID = NTC_OFS_DEPENDENT_CONTRACT_EXP.OFF_SERVICE_ID ) and  
         ( NTC_EXP_TYPE.EXP_TYPE_ID = NTC_OFS_DEPENDENT_CONTRACT_EXP.EXP_TYPE_ID ) and  
         ( ( NTC_PAYMENT.CONTRACT_ID = :contract_id ) AND  
         ( NTC_PAYMENT.PAYMENT_ID >= :payment_id_low ) AND  
         ( NTC_PAYMENT.PAYMENT_ID <= :payment_id ) )   
ORDER BY NTC_OFF_SERVICE.START_DATE ASC   
" arguments=(("payment_id", number),("payment_id_low", number),("contract_id", number)) )
