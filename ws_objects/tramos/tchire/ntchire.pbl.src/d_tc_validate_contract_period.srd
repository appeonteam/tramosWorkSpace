 column=(type=decimal(1) updatewhereclause=yes name=del_lt_to_utc_difference dbname="DEL_LT_TO_UTC_DIFFERENCE" )
 column=(type=datetime updatewhereclause=yes name=redelivery dbname="REDELIVERY" )
 column=(type=decimal(1) updatewhereclause=yes name=red_lt_to_utc_difference dbname="RED_LT_TO_UTC_DIFFERENCE" )
 retrieve="  SELECT NTC_TC_CONTRACT.CONTRACT_ID,   
         NTC_TC_CONTRACT.DELIVERY,
			(CASE WHEN NTC_TC_CONTRACT.LOCAL_TIME = 1 THEN 0
				  ELSE ISNULL(POC_DEL.LT_TO_UTC_DIFFERENCE, 0) END) AS DEL_LT_TO_UTC_DIFFERENCE,
    		(SELECT MAX(NTC_TC_PERIOD.PERIODE_END) FROM NTC_TC_PERIOD WHERE NTC_TC_PERIOD.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID) as REDELIVERY,
			(CASE WHEN NTC_TC_CONTRACT.LOCAL_TIME = 1 THEN 0
					ELSE ISNULL(POC_RED.LT_TO_UTC_DIFFERENCE, 0) END) AS RED_LT_TO_UTC_DIFFERENCE
     FROM NTC_TC_CONTRACT LEFT OUTER JOIN POC POC_DEL ON NTC_TC_CONTRACT.CONTRACT_ID = POC_DEL.CONTRACT_ID AND POC_DEL.PURPOSE_CODE = 'DEL'
          					  LEFT OUTER JOIN POC POC_RED ON NTC_TC_CONTRACT.CONTRACT_ID = POC_RED.CONTRACT_ID AND POC_RED.PURPOSE_CODE = 'RED' 
   WHERE ( NTC_TC_CONTRACT.VESSEL_NR = :vessel ) AND  
         ( NTC_TC_CONTRACT.TC_HIRE_IN = :tc_hire_in)    
" update="NTC_TC_CONTRACT" updatewhere=1 updatekeyinplace=no arguments=(("vessel", number),("tc_hire_in", number)) )
