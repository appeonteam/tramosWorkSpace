 retrieve="  SELECT DISTINCT NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,
			VESSELS.PC_NR,   
         NTC_TC_CONTRACT.TC_HIRE_CP_DATE,   
         HEADOWNER.TCOWNER_N_1 HEADOWNER_N_1,   
         CHART.CHART_N_1,   
         NTC_TC_CONTRACT.CURR_CODE,   
         NTC_TC_CONTRACT.DELIVERY,
			VESSELS.VESSEL_NR,
			NTC_PAYMENT.EST_DUE_DATE,
			isnull(CHART.NOM_ACC_NR, ~"N/A~") AS CHART_NOM_ACC_NR ,
			NTC_TC_CONTRACT.LOCAL_TIME,   
         (select max(NTC_TC_PERIOD.PERIODE_END) from NTC_TC_PERIOD where NTC_TC_PERIOD.CONTRACT_ID = :contract_id) AS redelivery ,
			NTC_TC_CONTRACT.TC_HIRE_IN,
			NTC_TC_CONTRACT.IN_POOL,
			isnull(HEADOWNER.NOM_ACC_NR, ~"N/A~") as HEADOWNER_NOM_ACC_NR,
			NTC_PAYMENT.PAYMENT_BALANCE,
         TCOWNER.TCOWNER_N_1 TCOWNER_N_1,
			VESSELS.VESSEL_REF_NR   
    FROM CHART,   
         NTC_TC_CONTRACT,   
         TCOWNERS HEADOWNER, 
			TCOWNERS TCOWNER,  
         VESSELS,   
         NTC_PAYMENT,   
         NTC_TC_PERIOD  
   WHERE ( NTC_TC_CONTRACT.CHART_NR *= CHART.CHART_NR) and  
         ( HEADOWNER.TCOWNER_NR =* NTC_TC_CONTRACT.TCOWNER_NR) and  
         ( TCOWNER.TCOWNER_NR =* NTC_TC_CONTRACT.CONTRACT_TCOWNER_NR) and  
         ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and  
         ( NTC_PAYMENT.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID ) and  
         ( NTC_TC_PERIOD.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID ) and  
         ( NTC_PAYMENT.PAYMENT_ID = :payment_id )   
" arguments=(("payment_id", number),("contract_id", number)) )
