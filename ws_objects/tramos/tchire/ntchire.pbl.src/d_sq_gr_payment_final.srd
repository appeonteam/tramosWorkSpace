 retrieve="  SELECT TOP 1 NTC_PAYMENT.CONTRACT_ID,   
         NTC_PAYMENT.PAYMENT_ID,   
         NTC_PAYMENT.EST_DUE_DATE,
         NTC_PAYMENT.LOCKED       
    FROM NTC_PAYMENT,   
         NTC_TC_CONTRACT  
   WHERE NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID  AND
         NTC_PAYMENT.PAYMENT_STATUS = 3    AND
         NTC_TC_CONTRACT.TC_HIRE_IN = 0    AND
         NTC_TC_CONTRACT.CONTRACT_ID = :contract_id
   ORDER BY NTC_PAYMENT.EST_DUE_DATE DESC  
" arguments=(("contract_id", number)) )
