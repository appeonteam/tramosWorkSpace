 retrieve="  SELECT PROFIT_C.BANK_DETAIL_1,   
         PROFIT_C.BANK_DETAIL_2,   
         PROFIT_C.BANK_DETAIL_3,   
         PROFIT_C.BANK_DETAIL_4,   
         PROFIT_C.BANK_DETAIL_5,   
         PROFIT_C.BANK_DETAIL_6,   
         VESSELS.VESSEL_NR,   
         NTC_PAYMENT.EST_DUE_DATE,   
         CHART.NOM_ACC_NR,   
         TCOWNERS.TCOWNER_B_N_1,   
         TCOWNERS.TCOWNER_B_NR,   
         NTC_TC_CONTRACT.TC_HIRE_IN,   
         VESSELS.APM_ACCOUNT_NR,   
         CHART.CHART_B_N_1,   
         CHART.CHART_B_NR,   
         NTC_PAYMENT.PAYMENT_BALANCE,   
         TCOWNERS.NOM_ACC_NR,   
         CHART.CHART_VAT_NR,   
         TCOWNERS.TCOWNER_VAT_NR,   
         CHART.CHART_APL_VAT_RATE,   
         TCOWNERS.TCOWNER_APL_VAT_RATE,   
         VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,   
         NTC_TC_CONTRACT.CURR_CODE,
			NTC_TC_CONTRACT.BANK_ACCOUNT_ID,
			BANK_ACCOUNTS.BANK_NAME, 
			BANK_ACCOUNTS.BANK_ADDRESS,
			case when isnull(BANK_ACCOUNTS.BANK_ACCOUNT_SWIFT,'') = '' then '' else 'Swift code: ' + BANK_ACCOUNTS.BANK_ACCOUNT_SWIFT end as BANK_ACCOUNT_SWIFT,
			case when isnull(BANK_ACCOUNTS.BANK_ACCOUNT_IBAN,'') = '' then '' else 'IBAN: ' + BANK_ACCOUNTS.BANK_ACCOUNT_IBAN end + 
			case when isnull(BANK_ACCOUNTS.BANK_ACCOUNT_NR,'') = '' then '' else ' Account Nr: ' + BANK_ACCOUNTS.BANK_ACCOUNT_NR end as BANK_ACCOUNT_IBAN, 
			case when isnull(BANK_ACCOUNTS.BANK_ACCOUNT_BENEFICIARY,'') = '' then '' else 'Beneficiary: ' + BANK_ACCOUNTS.BANK_ACCOUNT_BENEFICIARY end AS BANK_ACCOUNT_BENEFICIARY  
    FROM CHART RIGHT OUTER JOIN NTC_TC_CONTRACT ON CHART.CHART_NR = NTC_TC_CONTRACT.CHART_NR 
					LEFT OUTER JOIN TCOWNERS ON NTC_TC_CONTRACT.TCOWNER_NR = TCOWNERS.TCOWNER_NR
					LEFT OUTER JOIN BANK_ACCOUNTS ON NTC_TC_CONTRACT.BANK_ACCOUNT_ID = BANK_ACCOUNTS.BANK_ACCOUNT_ID,   
         PROFIT_C,   
         VESSELS,   
         NTC_PAYMENT  
   WHERE ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and  
         ( VESSELS.PC_NR = PROFIT_C.PC_NR ) and  
         ( NTC_PAYMENT.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID ) and  
         ( ( NTC_TC_CONTRACT.CONTRACT_ID = :contract_id ) AND  
         ( NTC_PAYMENT.PAYMENT_ID = :payment_id ) )    
" arguments=(("contract_id", number),("payment_id", number)) )
