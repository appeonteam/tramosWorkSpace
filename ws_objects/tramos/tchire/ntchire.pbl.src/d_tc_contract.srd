 retrieve="SELECT  NTC_TC_CONTRACT.CONTRACT_ID ,
		  NTC_TC_CONTRACT.OFFICE_NR ,
		  NTC_TC_CONTRACT.TCOWNER_NR ,
		  NTC_TC_CONTRACT.FINISH_USER_ID ,
		  NTC_TC_CONTRACT.CHART_NR ,
		  NTC_TC_CONTRACT.VESSEL_NR ,
		  NTC_TC_CONTRACT.CURR_CODE ,
		  NTC_TC_CONTRACT.FIXTURE_USER_ID ,
		  NTC_TC_CONTRACT.TC_HIRE_IN ,
		  NTC_TC_CONTRACT.IN_POOL ,
		  NTC_TC_CONTRACT.BAREBOAT ,
		  NTC_TC_CONTRACT.TC_HIRE_CP_DATE ,
		  NTC_TC_CONTRACT.TC_HIRE_CP_TEXT ,
		  NTC_TC_CONTRACT.FIX_DATE ,
		  NTC_TC_CONTRACT.FINISH_DATE ,
		  NTC_TC_CONTRACT.FIX_NOTE ,
		  NTC_TC_CONTRACT.DELIVERY ,
		  NTC_TC_CONTRACT.LOCAL_TIME ,
		  NTC_TC_CONTRACT.PAYMENT ,
		  NTC_TC_CONTRACT.PAYMENT_TYPE ,
		  NTC_TC_CONTRACT.MONTHLY_RATE,
		  NTC_TC_CONTRACT.OPTION_PERIOD ,
		  NTC_TC_CONTRACT.ADR_COMM ,
		  NTC_TC_CONTRACT.PERF_GRNTE ,
		  NTC_TC_CONTRACT.COMMENTS ,
		  NTC_TC_CONTRACT.EXPIRING_DAYS,
		  CHART.CHART_N_1 ,
		  OFFICES.OFFICE_NAME ,
		  TCOWNERS.TCOWNER_N_1,
		  getdate() as REDELIVERY,
		  0 as CALCULATED_DAYS,
		  CHART.NOM_ACC_NR,
		  TCOWNERS.NOM_ACC_NR,
		  (SELECT MAX(PAYMENT_STATUS) FROM NTC_PAYMENT WHERE CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID) AS PAYMENT_STATUS,
		  (SELECT COUNT(*) FROM NTC_SHARE_MEMBER WHERE CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID) AS SHARE_MEMBER,
			NTC_TC_CONTRACT.CONTRACT_TCOWNER_NR,
		  TCOWNERS_2.TCOWNER_N_1,
		  TCOWNERS_2.NOM_ACC_NR,
		  NTC_TC_CONTRACT.STATEMENT_LOGO,
		  NTC_TC_CONTRACT.STATEMENT_LOGO_TEXT,
		  NTC_TC_CONTRACT.PAYMENT_DUE_DAYS,
		  NTC_TC_CONTRACT.HIRED_BY_POOL,
		  NTC_TC_CONTRACT.PURE_MANAGEMENT,
		  NTC_TC_CONTRACT.OPSA_SETUP,
		  NTC_TC_CONTRACT.BANK_ACCOUNT_ID,
		  NTC_TC_CONTRACT.PC_NR,
		  isnull(cast(PC_NR as varchar(2)),'') + CAST(STATEMENT_LOGO AS VARCHAR(2)) + ltrim(rtrim(lower(isnull(case when STATEMENT_LOGO IN(1,3) AND PC_NR IS NOT NULL THEN STATEMENT_LOGO_TEXT ELSE '' END,'')))) AS PCNRLOGO
	FROM CHART ,
		  NTC_TC_CONTRACT ,
		  OFFICES ,
		  TCOWNERS ,
		  TCOWNERS TCOWNERS_2
 WHERE (NTC_TC_CONTRACT.CHART_NR *= CHART.CHART_NR) 
	and (OFFICES.OFFICE_NR =* NTC_TC_CONTRACT.OFFICE_NR) 
	and (NTC_TC_CONTRACT.TCOWNER_NR *= TCOWNERS.TCOWNER_NR) 
	and (NTC_TC_CONTRACT.CONTRACT_TCOWNER_NR *= TCOWNERS_2.TCOWNER_NR) 
	and ( ( NTC_TC_CONTRACT.CONTRACT_ID = :al_contract_id ) )  " update="NTC_TC_CONTRACT" updatewhere=0 updatekeyinplace=yes arguments=(("al_contract_id", number)) )
