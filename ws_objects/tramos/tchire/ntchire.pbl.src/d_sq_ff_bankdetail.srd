 column=(type=char(80) updatewhereclause=yes name=bank_address dbname="BANK_ADDRESS" )
 column=(type=char(52) updatewhereclause=yes name=bank_account_swift dbname="BANK_ACCOUNT_SWIFT" )
 column=(type=char(91) updatewhereclause=yes name=bank_account_iban dbname="BANK_ACCOUNT_IBAN" )
 column=(type=char(93) updatewhereclause=yes name=bank_account_beneficiary dbname="BANK_ACCOUNT_BENEFICIARY" )
 retrieve="SELECT 'Default' AS PCBANK_ACCOUNT_DESC,
       BANK_DETAIL_1,
       BANK_DETAIL_2,
       BANK_DETAIL_3,
       ISNULL(BANK_DETAIL_4,'') + ISNULL(APM_ACCOUNT_NR,'') AS BANK_DETAIL_4,
       BANK_DETAIL_5, 
       ISNULL(BANK_DETAIL_6,'') + ' ' + VESSEL_NAME + ' (' + VESSEL_REF_NR + ' - TC Hire)' AS BANK_DETAIL_6,
       A.BANK_ACCOUNT_DESC,
       A.BANK_NAME,
       A.BANK_ADDRESS,
       A.BANK_ACCOUNT_SWIFT,
       A.BANK_ACCOUNT_IBAN,
       A.BANK_ACCOUNT_BENEFICIARY
FROM PROFIT_C, VESSELS LEFT OUTER JOIN 
    (select BANK_ACCOUNT_DESC,
			   BANK_NAME, 
			   BANK_ADDRESS,
			  case when isnull(BANK_ACCOUNT_SWIFT,'') = '' then '' else 'Swift code: ' + BANK_ACCOUNT_SWIFT end as BANK_ACCOUNT_SWIFT,
			  case when isnull(BANK_ACCOUNT_IBAN,'') = '' then '' else 'IBAN: ' + BANK_ACCOUNT_IBAN end + 
			  case when isnull(BANK_ACCOUNT_NR,'') = '' then '' else ' Acc. Nr: ' + BANK_ACCOUNT_NR end as BANK_ACCOUNT_IBAN, 
			  case when isnull(BANK_ACCOUNT_BENEFICIARY,'') = '' then '' else 'Beneficiary: ' + BANK_ACCOUNT_BENEFICIARY end AS BANK_ACCOUNT_BENEFICIARY
	from BANK_ACCOUNTS
	where BANK_ACCOUNT_ID = :ai_bankaccountid) A on 1 = 1 
WHERE VESSELS.VESSEL_NR = :ai_vesselnr
     AND PROFIT_C.PC_NR = VESSELS.PC_NR" arguments=(("ai_bankaccountid", number),("ai_vesselnr", number)) )
