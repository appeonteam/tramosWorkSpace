 retrieve="  SELECT NTC_OFF_SERVICE.START_DATE,   
         NTC_OFF_SERVICE.END_DATE,   
         NTC_OFF_SERVICE.HOURS,   
         NTC_OFF_SERVICE.MINUTES,   
         NTC_OFF_SERVICE.HFO_TON,   
         NTC_OFF_SERVICE.HFO_PRICE,   
         NTC_OFF_SERVICE.DO_TON,   
         NTC_OFF_SERVICE.DO_PRICE,   
         NTC_OFF_SERVICE.GO_PRICE,   
         NTC_OFF_SERVICE.GO_TON,   
         NTC_OFF_SERVICE.LSHFO_PRICE,   
         NTC_OFF_SERVICE.LSHFO_TON,   
         NTC_OFF_SERVICE_DETAIL.START_DATE,   
         NTC_OFF_SERVICE_DETAIL.END_DATE,   
         NTC_OFF_SERVICE_DETAIL.DAYS,   
         NTC_OFF_SERVICE_DETAIL.RATE,   
         NTC_OFF_SERVICE.EXCHANGE_RATE_USD,   
         NTC_OFF_SERVICE.OFF_SERVICE_ID,   
			(SELECT COUNT(*) FROM NTC_OFF_SERVICE_DETAIL WHERE NTC_OFF_SERVICE_DETAIL.OFF_SERVICE_ID = NTC_OFF_SERVICE.OFF_SERVICE_ID) AS NO_OF_DETAIL,
			OFF_SERVICES.OFF_DESCRIPTION ,   
         NTC_OFF_SERVICE.HFO_STOCK_START,   
         NTC_OFF_SERVICE.HFO_STOCK_END,   
         NTC_OFF_SERVICE.DO_STOCK_START,   
         NTC_OFF_SERVICE.DO_STOCK_END,   
         NTC_OFF_SERVICE.GO_STOCK_START,   
         NTC_OFF_SERVICE.GO_STOCK_END,   
         NTC_OFF_SERVICE.LSHFO_STOCK_START,   
         NTC_OFF_SERVICE.LSHFO_STOCK_END,
         isnull(NTC_PAYMENT.ADR_COMM_PCT,0) as ADR_COMM_PCT,   
         isnull(NTC_PAYMENT.BROKER_COMMISSION_PCT,0) as BROKER_COMMISSION_PCT,   
         isnull(NTC_PAYMENT.ADR_COMM,0) as ADR_COMM,   
         isnull(NTC_PAYMENT.BROKER_COMMISSION,0) as BROKER_COMMISSION,   
         NTC_PAYMENT.PAYMENT_ID,
			0.00 as BASE_AMOUNT,
			(select count(*) from NTC_OFF_SERVICE where NTC_OFF_SERVICE.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID) AS NO_OF_OFFSERVICE, 
			isnull(NTC_PAYMENT.BROKER_COMMISSION_PER_DAY,0) as BROKER_COMMISSION_PER_DAY,
			0.0000 as BASE_DAYS   
    FROM NTC_OFF_SERVICE,   
         NTC_OFF_SERVICE_DETAIL,   
         NTC_PAYMENT,
			OFF_SERVICES  
   WHERE ( NTC_OFF_SERVICE_DETAIL.OFF_SERVICE_ID = NTC_OFF_SERVICE.OFF_SERVICE_ID ) and  
         ( NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID ) and  
        
         (NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID ) AND
         ( ( NTC_PAYMENT.PAYMENT_ID = :paymentid ) OR 
         ( NTC_PAYMENT.CONTRACT_ID = :contractid AND 
					NTC_PAYMENT.PAYMENT_ID >= :paymentid_low AND 
         		NTC_PAYMENT.PAYMENT_ID <= :paymentid )
          ) 

   

 " arguments=(("paymentid", number),("paymentid_low", number),("contractid", number))  sort="ntc_off_service_start_date A ntc_off_service_off_service_id A " )
