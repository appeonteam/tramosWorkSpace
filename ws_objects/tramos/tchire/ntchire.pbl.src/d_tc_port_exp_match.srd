 column=(type=decimal(0) updatewhereclause=yes name=match_port_exp_id dbname="NTC_PORT_EXP.MATCH_PORT_EXP_ID" dbalias=".MATCH_PORT_EXP_ID" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=contract_id dbname="NTC_TC_CONTRACT.CONTRACT_ID" dbalias=".CONTRACT_ID" )
 retrieve="  SELECT NTC_PORT_EXP.PORT_EXP_ID,   
         NTC_PORT_EXP.PAYMENT_ID,   
         NTC_PORT_EXP.CURR_CODE,   
         NTC_PORT_EXP.EXP_DESC,   
         NTC_PORT_EXP.EXP_AMOUNT,   
         NTC_PORT_EXP.EX_RATE_TC,   
         NTC_PORT_EXP.EXP_FOR_OA,   
         NTC_PORT_EXP.USE_IN_VAS,   
         NTC_PORT_EXP.MATCH_PORT_EXP_ID,   
         NTC_TC_CONTRACT.CONTRACT_ID  
    FROM NTC_PAYMENT,   
         NTC_PORT_EXP,   
         NTC_TC_CONTRACT  
   WHERE ( NTC_PORT_EXP.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID )   
  HAVING ( NTC_TC_CONTRACT.CONTRACT_ID = :contract_id ) And  
         ( NTC_PORT_EXP.EXP_AMOUNT*(-1) = :exp_amount ) and  
         ( NTC_PORT_EXP.PORT_EXP_ID <> :port_exp_id ) and  
         ( NTC_PORT_EXP.MATCH_PORT_EXP_ID is NULL ) and  
         ( NTC_PORT_EXP.CURR_CODE = :curr_code ) and
			( NTC_PAYMENT.PAYMENT_STATUS < 3)   
" arguments=(("curr_code", string),("contract_id", number),("exp_amount", number),("port_exp_id", number)) )
