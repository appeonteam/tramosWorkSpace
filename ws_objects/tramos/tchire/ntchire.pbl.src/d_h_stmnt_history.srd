 retrieve="  SELECT NTC_PAYMENT_SETTLED_AMOUNT.SETTLE_DATE,   
         NTC_PAYMENT_SETTLED_AMOUNT.SETTLE_AMOUNT,
			NTC_PAYMENT.EST_DUE_DATE  
    FROM NTC_PAYMENT, NTC_PAYMENT_SETTLED_AMOUNT  
   WHERE (NTC_PAYMENT.PAYMENT_ID = NTC_PAYMENT_SETTLED_AMOUNT.PAYMENT_ID) AND
         ( NTC_PAYMENT_SETTLED_AMOUNT.PAYMENT_ID = :payment_id  
          OR ( NTC_PAYMENT.CONTRACT_ID = :contract_id AND 
					NTC_PAYMENT_SETTLED_AMOUNT.PAYMENT_ID >= :payment_id_low AND 
         		NTC_PAYMENT_SETTLED_AMOUNT.PAYMENT_ID <= :payment_id ))     
	ORDER BY NTC_PAYMENT.EST_DUE_DATE
" arguments=(("contract_id", number),("payment_id", number),("payment_id_low", number)) )
