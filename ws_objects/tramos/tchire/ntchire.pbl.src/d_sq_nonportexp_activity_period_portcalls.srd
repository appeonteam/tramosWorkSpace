table(column=(type=char(7) updatewhereclause=yes name=poc_voyage_nr dbname="POC.VOYAGE_NR" )
 column=(type=char(10) updatewhereclause=yes name=poc_port_code dbname="POC.PORT_CODE" )
 column=(type=char(35) updatewhereclause=yes name=ports_port_n dbname="PORTS.PORT_N" )
 column=(type=datetime updatewhereclause=yes name=poc_port_arr_dt dbname="POC.PORT_ARR_DT" )
 column=(type=datetime updatewhereclause=yes name=poc_port_dept_dt dbname="POC.PORT_DEPT_DT" )
 retrieve="  SELECT POC.VOYAGE_NR,   
         POC.PORT_CODE,   
         PORTS.PORT_N,   
         POC.PORT_ARR_DT,   
         POC.PORT_DEPT_DT  
    FROM POC,   
         PORTS  
   WHERE POC.PORT_CODE = PORTS.PORT_CODE 
	AND POC.VESSEL_NR = :vessel     
	AND ((POC.PORT_ARR_DT >= DATEADD(DD, -30, :activity_period) AND POC.PORT_ARR_DT <= DATEADD(DD, 30, :activity_period))
	OR (POC.PORT_DEPT_DT >= DATEADD(DD, -30, :activity_period) AND POC.PORT_DEPT_DT <= DATEADD(DD, 30, :activity_period)))
ORDER BY POC.PORT_ARR_DT


" arguments=(("vessel", number),("activity_period", datetime)) )
