 retrieve="  SELECT NTC_EST_OC_EXP.PAYMENT_ID,   
         NTC_EST_OC_EXP.CURR_CODE,   
         NTC_EST_OC_EXP.INCOME,   
         NTC_EST_OC_EXP.COMMENT,   
         NTC_EST_OC_EXP.AMOUNT,   
         NTC_EST_OC_EXP.EX_RATE_TC,
	   NTC_EST_OC_EXP.OWNER_EXP,   
			(SELECT TC_HIRE_IN FROM NTC_TC_CONTRACT WHERE CONTRACT_ID = :contract_id ) AS TC_HIRE_IN  
    FROM NTC_EST_OC_EXP,   
         NTC_PAYMENT  
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_EST_OC_EXP.PAYMENT_ID ) and  
         (	( NTC_EST_OC_EXP.PAYMENT_ID = :payment_id ) 
          OR ( NTC_PAYMENT.CONTRACT_ID = :contract_id AND 
					NTC_EST_OC_EXP.PAYMENT_ID >= :payment_id_low AND 
         		NTC_EST_OC_EXP.PAYMENT_ID <= :payment_id ) )    
" arguments=(("payment_id", number),("payment_id_low", number),("contract_id", number)) )
