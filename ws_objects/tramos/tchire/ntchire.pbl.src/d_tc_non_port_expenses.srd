 retrieve="  SELECT NTC_NON_PORT_EXP.INCOME,   
         NTC_NON_PORT_EXP.EXP_TYPE_ID,   
         NTC_EXP_TYPE.TYPE_DESC,   
         NTC_NON_PORT_EXP.COMMENT,   
         NTC_NON_PORT_EXP.FINAL_HIRE,   
         NTC_NON_PORT_EXP.AMOUNT,   
         NTC_NON_PORT_EXP.CURR_CODE,   
         NTC_NON_PORT_EXP.EX_RATE_TC,   
         NTC_PAYMENT.SETTLE_DATE,   
         NTC_NON_PORT_EXP.USE_IN_VAS,   
         NTC_TC_CONTRACT.TC_HIRE_IN,   
         NTC_NON_PORT_EXP.PAYMENT_ID,   
         NTC_NON_PORT_EXP.NON_PORT_ID,   
         NTC_PAYMENT.PAYMENT_STATUS,   
         NTC_NON_PORT_EXP.TRANS_TO_CODA,   
         NTC_NON_PORT_EXP.ACTIVITY_PERIOD,   
         NTC_NON_PORT_EXP.ADDRESS_COMMISSION_PCT,   
         NTC_NON_PORT_EXP.SET_OFF_BROKER_COMM_PCT,   
         NTC_NON_PORT_EXP.SET_OFF_BROKER_COMM,   
         NTC_NON_PORT_EXP.ADDRESS_COMMISSION,
	   (SELECT CASE COUNT(*) WHEN 0 THEN 0 ELSE 1 END FROM NTC_NON_PORT_EXP_BROKER_COMM WHERE  NTC_NON_PORT_EXP_BROKER_COMM.NON_PORT_ID = NTC_NON_PORT_EXP.NON_PORT_ID) AS COUNT_BROKER_COMM
    FROM NTC_EXP_TYPE,   
         NTC_NON_PORT_EXP,   
         NTC_PAYMENT,   
         NTC_TC_CONTRACT  
   WHERE ( NTC_NON_PORT_EXP.EXP_TYPE_ID = NTC_EXP_TYPE.EXP_TYPE_ID ) and  
         ( NTC_PAYMENT.PAYMENT_ID = NTC_NON_PORT_EXP.PAYMENT_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( ( NTC_TC_CONTRACT.CONTRACT_ID = :contract_id ) )   
ORDER BY NTC_PAYMENT.SETTLE_DATE ASC   
" update="NTC_NON_PORT_EXP" updatewhere=0 updatekeyinplace=no arguments=(("contract_id", number)) )
