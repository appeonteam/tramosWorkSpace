global m_tramosmain m_tramosmain

type variables
// Used in operations to hold reference to current dw
// from where the vessel_nr and voyage_nr are picked
// when linking to calculation.

datawindow idw_current
string is_vessel, is_voyage

u_jump_tchire	inv_jump_tchire

end variables

forward prototypes
public subroutine mf_setcalclink (boolean ab_enabled)
public subroutine mf_setcalclink (ref datawindow adw_link, string as_vessel_fieldname, string as_voyage_fieldname, boolean ab_enabled)
public subroutine mf_link (string as_link_to)
public function integer mf_set_menu_hidden (ref menu arg_menu)
public subroutine documentation ()
end prototypes

public subroutine mf_setcalclink (boolean ab_enabled);if ab_enabled then
//	m_menutop4.m_calculated.enabled = True
//	m_menutop4.m_estimated.enabled = True
	m_menutop4.enabled = True
else
//	m_menutop4.m_calculated.enabled = False
//	m_menutop4.m_estimated.enabled = False
	m_menutop4.enabled = False
end if
return
end subroutine

public subroutine mf_setcalclink (ref datawindow adw_link, string as_vessel_fieldname, string as_voyage_fieldname, boolean ab_enabled);window lw_activesheet
m_tramosmain lm_menu

if adw_link.classname() = "dw_port_of_call_list" then
	lm_menu = w_port_of_call_list.menuid
else
	lm_menu = w_tramos_main.menuid
end if
lm_menu.idw_current = adw_link
lm_menu.is_vessel = as_vessel_fieldname
lm_menu.is_voyage = as_voyage_fieldname
if ab_enabled then
	lm_menu.m_menutop4.enabled = True
	
	//CR2530/2319 Added by LGX001 on 20/09/2011.
	if adw_link.classname() <> "dw_port_of_call_list" then
      lw_activesheet = w_tramos_main.getactivesheet()
		if isvalid(lw_activesheet) then
			if lw_activesheet.classname( ) = 'w_port_of_call'  then 
				lm_menu.m_menutop4.m_portofcall0.enabled = false  // shortcut key = F3
			else
            lm_menu.m_menutop4.m_portofcall0.enabled = true
			end if
			
			if lw_activesheet.classname( ) = 'w_fin_controlpanel'  then 
				lm_menu.m_menutop4.m_finance.enabled = false      // shortcut key = F4
			else
            lm_menu.m_menutop4.m_finance.enabled = true
			end if
				
		end if
	end if	
else
	lm_menu.m_menutop4.enabled = False
end if	

return







end subroutine

public subroutine mf_link (string as_link_to);// if as_Link_To = "Calc" link to Calculated Calculation
// if as_Link_To = "Est" link to Estimated Calculation
// if as_Link_To = "VAS" link to VAS Report
// if as_Link_To = "EXT" link to CALC MEMO EXTENDED Report
// if as_Link_To = "CAL" link to CALC MEMO Report
// if as_Link_To = "LOD" link to VAS Report (LoadLoad)
// if as_Link_To = "Load" link to LoadLoad Calculation
// if as_Link_To = "TCHire" link to TCHire contract
// if as_Link_To = "VNP" link to Voyage Noepad
// otherwise do nothing

integer li_vessel, li_pcn, li_tcin, li_status, li_counter
string  ls_voyage, ls_portcode, ls_tcout_voyage
long    ll_calc_id, ll_fix_id, ll_voyage_type, ll_contract_id, ll_row, ll_period_id
boolean lb_find_contract

u_jump_poc                  luo_jump_poc
s_opencalc_parm             lstr_opencalc_parm
w_calc_calculation          lw_calculation
u_jump_voyage_notepad       lnv_jump_vnp
u_jump_finance_control      luo_jump_fc
n_calcwindowcontrol         lnv_calcwincontrol
w_atobviac_calc_calculation lw_calc

// Get the current row. If it's above zero, then get the CALC_ID into a 
// S_OPENCALC_PARM structure and open the calculation with that

if not isValid(idw_current) then return

/*		
	When user use menu or shortcut key F9/F10 to link to Calculation, this will popup a message and 
	ask user to save the modified data first. In this case:
	1.)if user chooses Yes, and data is saved successfully, the calculation window will open
	2.)if user chooses Yes, but data saving is failed, it will popup whatever message it needs for user, and the calculation window will not open.
	3.)if user chooses No, data will not be saved, calculation window will open.	
*/
if as_link_to = "Est" or as_link_to = "Calc" then
	if isvalid(w_claims) then
		if not w_claims.wf_allow_linkcalculation() then
			w_claims.setfocus()
			return
		end if
	end if

	if isvalid(w_actions_transactions) then
		if not w_actions_transactions.wf_allow_linkcalculation() then
			w_actions_transactions.setfocus()
			return
		end if
	end if
end if

ll_Row = idw_current.GetRow()

If ll_Row > 0 Then
	li_vessel = idw_current.GetItemNumber(ll_row, is_vessel)
	ls_voyage = idw_current.GetItemString(ll_row, is_voyage)
	choose case as_link_to
		case "TCHire"
			if len(ls_voyage) = 7 then
				li_tcin = 0
			elseif  len(ls_voyage) = 5 then
				li_tcin = 1
			end if
			
			
			SELECT NTC_TC_CONTRACT.CONTRACT_ID, NTC_TC_PERIOD.TC_PERIODE_ID INTO :ll_contract_id, :ll_period_id
			FROM   NTC_TC_CONTRACT, NTC_TC_PERIOD
			WHERE  VESSEL_NR = :li_vessel AND NTC_TC_CONTRACT.TC_HIRE_IN =:li_tcin AND
			       NTC_TC_CONTRACT.CONTRACT_ID = NTC_TC_PERIOD.CONTRACT_ID AND
			       (NTC_TC_PERIOD.PERIODE_START <= (SELECT MIN(dateadd(mi, POC1.LT_TO_UTC_DIFFERENCE*60, POC1.PORT_ARR_DT)) FROM POC POC1 WHERE POC1.VESSEL_NR = :li_vessel AND POC1.VOYAGE_NR = :ls_voyage)) AND   
			       (NTC_TC_PERIOD.PERIODE_END   >= (SELECT MIN(dateadd(mi, POC2.LT_TO_UTC_DIFFERENCE*60, POC2.PORT_ARR_DT)) FROM POC POC2 WHERE POC2.VESSEL_NR = :li_vessel AND POC2.VOYAGE_NR = :ls_voyage));
			
			if ll_contract_id > 0 then
				
				SELECT COUNT(*) INTO :li_counter 
				FROM   POC, NTC_TC_CONTRACT
				WHERE  PURPOSE_CODE = 'DEL'  AND NTC_TC_CONTRACT.CONTRACT_ID = POC.CONTRACT_ID AND 
				       POC.VESSEL_NR = :li_vessel AND NTC_TC_CONTRACT.CONTRACT_ID = :ll_contract_id;
				
				if li_counter > 0 then			
					if li_tcin = 1 then
						lb_find_contract = true
					else
						
						SELECT ISNULL(TCOUT_VOYAGE_NR, '') INTO :ls_tcout_voyage
						FROM   NTC_TC_PERIOD
						WHERE  NTC_TC_PERIOD.TC_PERIODE_ID = :ll_period_id;
						
						if ls_tcout_voyage = "" or ls_tcout_voyage = left(ls_voyage, 5) then
							lb_find_contract = true
						end if
					end if			
				end if
				
			end if
			
			IF not lb_find_contract THEN
				MessageBox("Information", "There is no TC contract for this voyage!")	
			ELSE
				if isvalid(inv_jump_tchire) then destroy inv_jump_tchire
				inv_jump_tchire = create u_jump_tchire
				inv_jump_tchire.of_open_tchire(li_vessel, ll_contract_id)
			END IF
			
		case "POC"
			luo_jump_poc = CREATE u_jump_poc
			luo_jump_poc.of_open_poc(li_vessel, ls_voyage)
			DESTROY luo_jump_poc		
		case "FCP"
			luo_jump_fc = CREATE u_jump_finance_control
			luo_jump_fc.of_open_control(li_vessel, ls_voyage)
			DESTROY luo_jump_fc	
		case "VNP"
			lnv_jump_vnp = create u_jump_voyage_notepad
			lnv_jump_vnp.of_open_voyage_notepad(li_vessel, ls_voyage)
			destroy lnv_jump_vnp
		case else
			SELECT CAL_CALC_ID, VOYAGE_TYPE INTO :ll_calc_id, :ll_voyage_type FROM VOYAGES WHERE VESSEL_NR = :li_vessel AND VOYAGE_NR = :ls_voyage;
		
		if ll_calc_id > 1 or ((ll_voyage_type = 2 or ll_voyage_type = 7) and (as_link_to ="VAS" or as_link_to = "CAL" or as_link_to = "EXT")) then
			choose case as_link_to
				case "Calc", "Est", "Load"   //link to calculation
					if as_link_to = "Calc" then
						SELECT CAL_CALC_FIX_ID INTO :ll_fix_id FROM CAL_CALC WHERE CAL_CALC_ID = :ll_calc_id;
						commit;
						SELECT CAL_CALC_ID INTO :ll_calc_id FROM CAL_CALC WHERE CAL_CALC_FIX_ID = :ll_fix_id and CAL_CALC_STATUS = 5;
						commit;
						li_status = 5
					elseif 	 as_link_to = "Est" then
						SELECT CAL_CALC_FIX_ID INTO :ll_fix_id FROM CAL_CALC WHERE CAL_CALC_ID = :ll_calc_id;
						commit;
						li_status = 6
					else
						SELECT CAL_CALC_FIX_ID, CAL_CALC_STATUS INTO :ll_fix_id, :li_status FROM CAL_CALC WHERE CAL_CALC_ID = :ll_calc_id;
						commit;
						SELECT CAL_CALC_ID INTO :ll_calc_id FROM CAL_CALC WHERE CAL_CALC_FIX_ID = :ll_fix_id and CAL_CALC_STATUS = 7;
						if isNull(ll_calc_id) or sqlca.sqlcode <> 0 then
							commit;
							return
						end if
						commit;
					end if
					
					/* check existing calculation instances */
					lnv_calcwincontrol = create n_calcwindowcontrol		
					integer li_retval
					lw_calc = lnv_calcwincontrol.of_getavailablecalc( ll_calc_id, ll_fix_id, li_status, li_retval)
					
					if isvalid(lw_calc) then
						/* same calc */
						lw_calc.setfocus()
					else
						if li_retval = 2 then /* modified open */
							lw_calc.setfocus()
						else /* closed unmodified or not open already */
							SetPointer(hourglass!)
							lstr_opencalc_parm.l_calc_id = ll_calc_id
							if f_AtoBviaC_used(li_vessel, ls_voyage) then
								/* If not already active create instance */
								if NOT isValid(gnv_atobviac) then gnv_atobviac = create n_atobviac
								/* If not open open tables - can take several seconds */
								if NOT gnv_atobviac.of_getTableOpen( ) then
									open(w_startup_screen)
									gnv_AtoBviaC.of_OpenTable()
									close(w_startup_screen)
								end if 
								OpenSheetWithParm(lw_calc, lstr_opencalc_parm,w_tramos_main,0,Original!)
							else	
								OpenSheetWithParm(lw_calculation, lstr_opencalc_parm,w_tramos_main,0,Original!)
							end if
						end if	
					end if

					
					destroy lnv_calcwincontrol
					/* end calculation instance validation*/
					
					
				case "VAS"
					if isValid(w_actions_transactions) then parentwindow.setFocus()
					if isValid(w_super_vas_reports) then close(w_super_vas_reports)
					opensheetwithparm( w_super_vas_reports, "VAS"+string(li_vessel,"0000") + mid(ls_voyage,1,5), w_tramos_main, 7, original!)
				case "EXT"
					if isValid(w_actions_transactions) then parentwindow.setFocus()
					if isValid(w_super_vas_reports) then close(w_super_vas_reports)
					opensheetwithparm( w_super_vas_reports, "EXT"+string(li_vessel,"0000") + mid(ls_voyage,1,5), w_tramos_main, 7, original!)
				case "CAL"
					if isValid(w_actions_transactions) then parentwindow.setFocus()
					if isValid(w_super_vas_reports) then close(w_super_vas_reports)
					opensheetwithparm( w_super_vas_reports, "CAL"+string(li_vessel,"0000") + mid(ls_voyage,1,5), w_tramos_main, 7, original!)
				case "LOD"
					SELECT CAL_CALC_FIX_ID INTO :ll_fix_id FROM CAL_CALC WHERE CAL_CALC_ID = :ll_calc_id;
					commit;
					SELECT CAL_CALC_ID INTO :ll_calc_id FROM CAL_CALC WHERE CAL_CALC_FIX_ID = :ll_fix_id and CAL_CALC_STATUS = 7;
					if isNull(ll_calc_id) or sqlca.sqlcode <> 0 then
						commit;
						return
					end if
					commit;
					if isValid(w_actions_transactions) then parentwindow.setFocus()
					if isValid(w_super_vas_reports) then close(w_super_vas_reports)
					opensheetwithparm( w_super_vas_reports, "LOD"+string(li_vessel,"0000") + mid(ls_voyage,1,5), w_tramos_main, 7, original!)
			end choose
		else
			MessageBox("Information", "This voyage is not allocated to a calculation!")		
		end if
		end choose
End if
Return
end subroutine

public function integer mf_set_menu_hidden (ref menu arg_menu);//CR2530 Added by LGX001 on 06/09/2011.
// this funcation is called from w_port_of_call_list to implement shortcut key (Links)
//
//////////////////////////////////////////////////////////////////////////////////////

long ll_menuitem_count, ll_loop
menu lm_temp
if arg_menu.classname() = 'm_menutop4' and arg_menu.text ='&Links' then
	arg_menu.visible = false
	arg_menu.enabled = true
	arg_menu.menuimage = ""
	arg_menu.toolbaritemvisible = false
	ll_menuitem_count = UpperBound(arg_menu.item[])
	for ll_loop = 1 to ll_menuitem_count
		lm_temp = arg_menu.item[ll_loop]
		lm_temp.visible = false
		lm_temp.menuimage=''
		lm_temp.toolbaritemvisible = false
	next
else
	arg_menu.visible = false
	arg_menu.enabled = false
	arg_menu.menuimage = ""
	arg_menu.toolbaritemvisible = false
	ll_menuitem_count = UpperBound(arg_menu.item[])
	if ll_menuitem_count > 0 then
		for ll_loop = 1 to ll_menuitem_count
			lm_temp = arg_menu.item[ll_loop]
			mf_set_menu_hidden(lm_temp)
		next
	end if
end if

return 0
end function

