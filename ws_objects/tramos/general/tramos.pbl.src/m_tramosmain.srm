event clicked;string ls_userid, ls_username

/* Administrator and Finance Users */
IF uo_global.ii_access_level = 3 or uo_global.ii_user_profile = 3 THEN
	//
ELSE
	this.Enabled = FALSE
	MessageBox("Infomation","Only Administrator and Finance profile has access to this functionality.")
	return
END IF

/* Find out if window already open. If so prevent user from opening, and tell who has it open */
SELECT IMPORT_USER
	INTO :ls_userid
	FROM ELECTRONIC_IN_PROGRESS ;
commit;	
if isNull(ls_userid) then	
	mf_opensheet(w_expense_import)
else
	SELECT isNull(FIRST_NAME, " ") + " " +ISNULL(LAST_NAME, " ")
		INTO :ls_username
		FROM USERS
		WHERE USERID = :ls_userid;
	commit;
	MessageBox("Information", "Only one user at the time can have this window open.~n~r~n~r"+&
						"Already opened by "+ls_userid+" - "+ls_username)
	return					
END IF

end event

