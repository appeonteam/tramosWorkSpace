type variables
// Used in operations to hold reference to current dw
// from where the vessel_nr and voyage_nr are picked
// when linking.
datawindow idw_current
string is_vessel, is_contract

n_weekly_fixture_report iuo_wf

end variables

forward prototypes
public subroutine mf_tchiretrigger (string as_event)
public subroutine mf_link (string as_link_to)
public subroutine mf_setcalclink (boolean d)
public subroutine mf_setlink (datawindow adw_link, string as_vessel_fieldname, boolean ab_enabled)
public subroutine documentation ()
end prototypes

public subroutine mf_tchiretrigger (string as_event);window					lw_test
w_tc_sheet_ancestor 	lw_activesheet

lw_test = w_tramos_main.GetActiveSheet()

if isValid(lw_test) then
	if "w_tc_contract" <> lw_test.className() then 
		return
	end if
else
	return
end if


lw_activesheet = lw_test
lw_activesheet.Event ue_eventroute(as_event)


end subroutine

public subroutine mf_link (string as_link_to);// if as_Link_To = "POC" link to Port of Call
// if as_Link_To = "FCP" link to Finance Control Panel
// otherwise do nothing  (CR1923)

integer 	li_vessel
Long 		ll_row, ll_contract_id
string	ls_voyage

u_jump_poc luo_jump_poc
u_jump_finance_control luo_jump_fc

if not isValid(idw_current) then return

ll_Row = idw_current.GetRow()

If ll_Row > 0 Then
	li_vessel = idw_current.GetItemNumber(ll_row, is_vessel)
	choose case as_link_to
		case "POC"
			ll_contract_id = idw_current.getitemnumber(ll_row, "contract_id")
			if not isnull(ll_contract_id) then
				SELECT TOP 1 VOYAGE_NR INTO :ls_voyage FROM POC WHERE VESSEL_NR = :li_vessel AND CONTRACT_ID = :ll_contract_id ORDER BY PORT_ARR_DT DESC;
			end if
			
			luo_jump_poc = CREATE u_jump_poc
			luo_jump_poc.of_open_poc(li_vessel, ls_voyage)
			DESTROY luo_jump_poc		
		case "FCP"
			luo_jump_fc = CREATE u_jump_finance_control
			luo_jump_fc.of_open_control(li_vessel, "")
			DESTROY luo_jump_fc				
	end choose
End if
Return
end subroutine

public subroutine mf_setcalclink (boolean d);
end subroutine

public subroutine mf_setlink (datawindow adw_link, string as_vessel_fieldname, boolean ab_enabled);idw_current = adw_link
is_vessel = as_vessel_fieldname
if ab_enabled then
//	m_menutop4.m_calculated.enabled = True
//	m_menutop4.m_estimated.enabled = True
	m_menutop4.enabled = True
else
//	m_menutop4.m_calculated.enabled = False
//	m_menutop4.m_estimated.enabled = False
	m_menutop4.enabled = False
end if	

return
end subroutine

