event clicked;window					lw_test
w_tc_sheet_ancestor 	lw_activesheet
Double ld_tc_id
Integer li_row, li_vessel

lw_test = w_tramos_main.GetActiveSheet()

if isValid(lw_test) then
	if "w_tc_contract" <> lw_test.className() then 
		return
	end if
else
	return
end if

li_row = w_tc_contract.dw_contract_list.GetRow()
IF NOT(li_row > 0) THEN
	MessageBox("Information","You must select a TC Contract first !")
	Return
END IF

ld_tc_id = w_tc_contract.dw_contract_list.GetItemNumber(li_row,"contract_id")
li_vessel = w_tc_contract.dw_contract_list.GetItemNumber(li_row,"ntc_tc_contract_vessel_nr")

IF NOT(w_tc_contract.dw_contract_list.GetItemNumber(li_row,"tc_hire_in") = 0) THEN
	MessageBox("Information","You can only select a TC OUT Contract for Weekly Fixture !")
	Return
END IF

iuo_wf = CREATE n_weekly_fixture_report

iuo_wf.of_report_fixture(li_vessel,ld_tc_id,0)

Destroy iuo_wf

end event

