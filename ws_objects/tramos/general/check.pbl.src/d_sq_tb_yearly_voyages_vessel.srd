table(column=(type=long update=yes updatewhereclause=yes name=vessel_nr dbname="POC.VESSEL_NR" )
 column=(type=char(7) update=yes updatewhereclause=yes name=voyage_nr dbname="POC.VOYAGE_NR" )
 column=(type=char(10) update=yes updatewhereclause=yes name=port_code dbname="POC.PORT_CODE" )
 column=(type=long update=yes updatewhereclause=yes name=pcn dbname="POC.PCN" )
 column=(type=datetime update=yes updatewhereclause=yes name=port_arr_dt dbname="POC.PORT_ARR_DT" )
 column=(type=datetime update=yes updatewhereclause=yes name=port_dept_dt dbname="POC.PORT_DEPT_DT" )
 column=(type=char(3) updatewhereclause=yes name=purpose_code dbname="POC.PURPOSE_CODE" )
 retrieve="  SELECT POC.VESSEL_NR,   
         POC.VOYAGE_NR,   
         POC.PORT_CODE,   
         POC.PCN,   
         POC.PORT_ARR_DT,   
         POC.PORT_DEPT_DT,
		POC.PURPOSE_CODE  
    FROM POC, PROCEED 
   WHERE POC.VESSEL_NR = PROCEED.VESSEL_NR AND
		POC.VOYAGE_NR = PROCEED.VOYAGE_NR AND
		POC.PORT_CODE = PROCEED.PORT_CODE AND
		POC.PCN = PROCEED.PCN AND
		POC.VESSEL_NR = :vessel  AND  
         ((POC.PORT_ARR_DT >= :start  and POC.PORT_ARR_DT < :end) OR
         (POC.PORT_DEPT_DT >= :start  and POC.PORT_DEPT_DT < :end))
ORDER BY PROCEED.VESSEL_NR,
		SUBSTRING(PROCEED.VOYAGE_NR+~"00~",1,7) ASC,   
		POC.PORT_ARR_DT ASC   
" arguments=(("vessel", number),("start", datetime),("end", datetime)) )
