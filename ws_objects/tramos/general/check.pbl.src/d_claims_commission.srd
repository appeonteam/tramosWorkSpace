table(column=(type=long update=yes updatewhereclause=yes name=chart_nr dbname="CLAIMS.CHART_NR" )
 column=(type=long update=yes updatewhereclause=yes name=vessel_nr dbname="CLAIMS.VESSEL_NR" )
 column=(type=char(7) update=yes updatewhereclause=yes name=voyage_nr dbname="CLAIMS.VOYAGE_NR" )
 column=(type=long update=yes updatewhereclause=yes name=claim_nr dbname="CLAIMS.CLAIM_NR" )
 column=(type=char(10) updatewhereclause=yes name=chart_chart_sn dbname="CHART.CHART_SN" )
 column=(type=char(5) updatewhereclause=yes name=vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 retrieve="  SELECT CLAIMS.CHART_NR,   
         CLAIMS.VESSEL_NR,   
         CLAIMS.VOYAGE_NR,   
         CLAIMS.CLAIM_NR,
			CHART.CHART_SN,
			VESSELS.VESSEL_REF_NR  
    FROM CLAIMS,
			CHART,
			VESSELS  
   WHERE  not exists (  SELECT *
								FROM COMMISSIONS  
                         WHERE COMMISSIONS.CLAIM_NR = CLAIMS.CLAIM_NR AND
										COMMISSIONS.VOYAGE_NR = CLAIMS.VOYAGE_NR AND
										COMMISSIONS.VESSEL_NR = CLAIMS.VESSEL_NR AND
										COMMISSIONS.CHART_NR = CLAIMS.CHART_NR)  AND
   SUBSTRING(CLAIMS.VOYAGE_NR,1,2) = :YEAR_SPECIFICATION AND
	CLAIMS.CHART_NR = CHART.CHART_NR AND
	CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR AND
	VESSELS.PC_NR = :pc_nr AND
	CLAIMS.BROKER_COM > 0 AND
	(CLAIMS.CLAIM_AMOUNT_USD>0 OR CLAIMS.CLAIM_TYPE= 'FRT')
	ORDER BY CLAIMS.VESSEL_NR,CLAIMS.VOYAGE_NR,CLAIMS.CLAIM_NR " arguments=(("year_specification", string),("pc_nr", number)) )
