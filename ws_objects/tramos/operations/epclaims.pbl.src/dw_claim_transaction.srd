 retrieve="  SELECT CLAIM_TRANSACTION.CHART_NR,   
         CLAIM_TRANSACTION.VESSEL_NR,   
         CLAIM_TRANSACTION.VOYAGE_NR,   
         CLAIM_TRANSACTION.CLAIM_NR,   
         CLAIM_TRANSACTION.C_TRANS_VAL_DATE,   
         CLAIM_TRANSACTION.C_TRANS_COMMENT,   
         CLAIM_TRANSACTION.C_TRANS_AMOUNT,   
         CLAIM_TRANSACTION.C_TRANS_CODE,   
         CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD,    
         CASE C_TRANS_CODE WHEN 'W' THEN C_TRANS_COMMENT END AS 'WRITE_OFF_REASONS', 
         CASE C_TRANS_CODE WHEN 'A' THEN C_TRANS_COMMENT END AS 'TRANS_REASON_TYPE',   
         CLAIM_TRANSACTION.TRANS_REASON_SUBTYPE,    
         CLAIM_TRANSACTION.C_TRANS_CREATED_BY,   
         CLAIM_TRANSACTION.C_TRANS_SEQ,
			CLAIM_TRANSACTION.EXCHANGE_RATE
    FROM CLAIM_TRANSACTION  
   WHERE ( CLAIM_TRANSACTION.VESSEL_NR = :vessel_nr ) AND  
         ( CLAIM_TRANSACTION.VOYAGE_NR = :voyage_nr ) AND  
         ( CLAIM_TRANSACTION.CHART_NR = :chart_nr ) AND  
         ( CLAIM_TRANSACTION.CLAIM_NR = :claim_nr )    
" update="CLAIM_TRANSACTION" updatewhere=0 updatekeyinplace=yes arguments=(("vessel_nr", number),("voyage_nr", string),("chart_nr", number),("claim_nr", number))  sort="c_trans_val_date A c_trans_seq A " )
