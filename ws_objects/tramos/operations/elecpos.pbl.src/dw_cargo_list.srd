 retrieve="  SELECT CARGO.VESSEL_NR,   
         CARGO.VOYAGE_NR,   
         CARGO.PORT_CODE,   
         CARGO.PCN,   
         CARGO.AGENT_NR,   
         CARGO.CHART_NR,   
         CARGO.CAL_CERP_ID,   
         CAL_CERP.CAL_CERP_DESCRIPTION,
		AGENTS.AGENT_SN,
		 CHART.CHART_SN
    FROM CARGO,
			AGENTS,
			CHART,
         CAL_CERP
   WHERE  CHART.CHART_NR = CARGO.CHART_NR AND
		AGENTS.AGENT_NR = CARGO.AGENT_NR AND	
		( CAL_CERP.CAL_CERP_ID = CARGO.CAL_CERP_ID ) and  
         ( ( CARGO.VESSEL_NR = :vessel_nr ) AND  
         ( CARGO.VOYAGE_NR = :voyage_nr ) AND  
         ( CARGO.PORT_CODE = :port_code ) AND  
         ( CARGO.PCN = :pcn ) )   
ORDER BY CHART.CHART_SN" update="CARGO" updatewhere=0 updatekeyinplace=no arguments=(("vessel_nr", number),("voyage_nr", string),("port_code", string),("pcn", number))  sort="chart_chart_sn A " )
