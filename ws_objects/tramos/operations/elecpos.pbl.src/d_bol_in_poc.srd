table(column=(type=char(35) updatewhereclause=yes name=cd_layout dbname="LAYOUT" )
 column=(type=char(35) updatewhereclause=yes name=cd_grade_name dbname="GRADE_NAME" )
 column=(type=datetime updatewhereclause=yes name=bol_nor_dt dbname="NOR_DT" )
 column=(type=datetime updatewhereclause=yes name=bol_all_fast_dt dbname="ALL_FAST_DT" )
 column=(type=datetime updatewhereclause=yes name=bol_departure_dt dbname="DEPARTURE_DT" )
 column=(type=number updatewhereclause=yes name=bol_l dbname="BOL_L" )
 column=(type=number updatewhereclause=yes name=bol_d dbname="BOL_D" )
 column=(type=char(50) updatewhereclause=yes name=cargo_desc dbname="CARGO_DESC" )
 retrieve="  select B.LAYOUT,B.GRADE_NAME,B.NOR_DT,B.ALL_FAST_DT,B.DEPARTURE_DT,B.BOL_L,B.BOL_D ,
        isnull((SELECT CAL_CARG.CAL_CARG_DESCRIPTION  
    		    FROM CAL_CAIO,   CAL_CARG  
		        WHERE 	 CAL_CARG.CAL_CARG_ID = CAL_CAIO.CAL_CARG_ID  and  CAL_CAIO.CAL_CAIO_ID = B.CAL_CAIO_ID),'No Calculation') as CARGO_DESC 
  from (SELECT MIN(CD.GRADE_NAME) AS GRADE_NAME,   
         		BOL.NOR_DT,   
         		BOL.ALL_FAST_DT,   
         		BOL.DEPARTURE_DT,
		 			MIN(CD.LAYOUT) LAYOUT,
		 			SUM(CASE BOL.L_D WHEN 1 THEN BOL.BOL_QUANTITY ELSE 0 END) AS BOL_L,
		 			SUM(CASE BOL.L_D WHEN 0 THEN BOL.BOL_QUANTITY ELSE 0 END) AS BOL_D,CD.CAL_CAIO_ID
    				FROM 	BOL,   
         				CD  
   				WHERE ( CD.CARGO_DETAIL_ID = BOL.CARGO_DETAIL_ID ) and  
         				( ( BOL.VESSEL_NR = :VESSEL_NR ) AND  
         				( BOL.VOYAGE_NR = :VOYAGE_NR ) AND  
         				( BOL.PORT_CODE = :PORT_CODE ) AND  
         				( BOL.PCN = :PCN ) )    
	GROUP BY  BOL.CARGO_DETAIL_ID,BOL.NOR_DT,  
         BOL.ALL_FAST_DT,   
         BOL.DEPARTURE_DT,CD.CAL_CAIO_ID) B




