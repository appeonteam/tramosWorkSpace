 retrieve="  SELECT POC.PORT_CODE,   
         POC.VESSEL_NR,   
         POC.PCN,   
         POC.PORT_OFF,   
         POC.TUGS_IN,   
         POC.TUGS_OUT,   
         POC.TUGS_SHIFTING,   
         POC.ARR_HFO,   
         POC.ARR_DO,   
         POC.ARR_GO,   
         POC.DEPT_HFO,   
         POC.DEPT_DO,   
         POC.DEPT_GO,   
         POC.LIFT_HFO,   
         POC.LIFT_DO,   
         POC.LIFT_GO,   
         POC.DISCH,   
         POC.COMMENTS,   
         POC.PORT_ARR_DT,   
         POC.PURPOSE_CODE,   
         POC.DT_V_3,   
         POC.DT_V_2,   
         POC.DT_V_1,   
         POC.DT_T_3,   
         POC.DT_T_2,   
         POC.DT_T_1,   
         POC.SLOP,   
         POC.VOYAGE_NR,   
         POC.PORT_DEPT_DT,   
         0.0000 as ORDERED_HFO,   
         0.0000 as ORDERED_DO,   
         0.0000 as ORDERED_GO,   
         POC.AGENT_NR,    
         AGENTS.AGENT_SN,   
         POC.ARR_FOR_DRAFT,   
         POC.ARR_AFT_DRAFT,   
         POC.DEPT_AFT_DRAFT,   
         POC.DEPT_FOR_DRAFT,   
         VOYAGES.VOYAGE_FINISHED,
	  VOYAGES.VOYAGE_FINISHED_DATE,
	   VOYAGES.VOYAGE_TYPE,
	   POC.PORT_BERTHING_TIME,
	   PROFIT_C.POC_SLOP_MANDATORY,
	   POC.CONTRACT_ID,
	   POC.LT_TO_UTC_DIFFERENCE,
	   POC.ARR_LSHFO,
	   POC.DEPT_LSHFO,
	   POC.LIFT_LSHFO,
	   0.0000 as ORDERED_LSHFO,
	   POC.LOCAL_DATES_UPDATED,
	   POC.COMMENTS_UPDATED,
	   POC.DATES_UPDATED_BY,
	   POC.COMMENTS_UPDATED_BY,
      POC.POC_PERFORMING_AGENT_NR,
		POC.OIL,
		POC.TANK_NAME,
		POC.LOPS_ISSUED,
		POC.LOPS_RECEIVED,
		POC.NOR_TENDERED,
		POC.FRESH_WATER,
		POC.MSPS_TFV,
		UPPER(P_AGENTS.COMPANY_SN) AS PERFORMING_AGENT_SN,
        	(SELECT count(*)
			FROM BP_DETAILS
			WHERE PORT_CODE =POC.PORT_CODE AND VESSEL_NR = POC.VESSEL_NR AND 
			VOYAGE_NR = POC.VOYAGE_NR AND PCN = POC.PCN) as BP_COUNT,
	   (SELECT COUNT(*) FROM CD WHERE VESSEL_NR = :vessel_nr AND VOYAGE_NR = :voyage_nr AND PORT_CODE = :port_code AND PCN = :pcn) AS CD_COUNT,
		POC.AUTOSCHEDULE_STATUS,
		getdate() AS PREVIOUS_ETD,
		getdate() AS CALC_PETD, 
		POC.AUTO_ARR_STATUS, 
		POC.AUTO_BER_STATUS, 
		POC.AUTO_DEPT_STATUS,
		(SELECT FIXTURE_PROFIT_MARGIN_PR_DAY FROM VAS_LOG_POC WHERE VAS_LOG_POC.VESSEL_NR = VOYAGES.VESSEL_NR AND VAS_LOG_POC.VOYAGE_NR = VOYAGES.VOYAGE_NR 
			AND VAS_LOG_POC.LOG_ID = (SELECT MAX(LOG_ID) FROM VAS_LOG_POC WHERE VESSEL_NR = VOYAGES.VESSEL_NR AND VOYAGE_NR = VOYAGES.VOYAGE_NR)) AS FIXTURE_PROFIT_MARGIN_PR_DAY,
		(SELECT EST_ACT_PROFIT_MARGIN_PR_DAY FROM VAS_LOG_POC WHERE VAS_LOG_POC.VESSEL_NR = VOYAGES.VESSEL_NR AND VAS_LOG_POC.VOYAGE_NR = VOYAGES.VOYAGE_NR 
			AND VAS_LOG_POC.LOG_ID = (SELECT MAX(LOG_ID) FROM VAS_LOG_POC WHERE VESSEL_NR = VOYAGES.VESSEL_NR AND VOYAGE_NR = VOYAGES.VOYAGE_NR)) AS EST_ACT_PROFIT_MARGIN_PR_DAY,
		PROFIT_C.TCE_PR_DAY_IN_POC,
		VOYAGES.CAL_CALC_ID,
      POC.WD_CODE
    FROM POC,
		   COMPANIES P_AGENTS,
          AGENTS,   
         VOYAGES,
	   PROFIT_C,
	   VESSELS  
   WHERE   
		 ( P_AGENTS.COMPANY_ID =* POC.POC_PERFORMING_AGENT_NR) and          
          ( AGENTS.AGENT_NR =* POC.AGENT_NR) and  
         ( POC.VESSEL_NR = VOYAGES.VESSEL_NR ) and  
         ( POC.VOYAGE_NR = VOYAGES.VOYAGE_NR ) and
	   ( POC.VESSEL_NR = VESSELS.VESSEL_NR) and  
         ( VESSELS.PC_NR = PROFIT_C.PC_NR) and
         ( ( POC.VESSEL_NR = :vessel_nr ) AND  
         ( POC.VOYAGE_NR = :voyage_nr ) AND  
         ( POC.PORT_CODE = :port_code ) AND  
         ( POC.PCN = :pcn ) )    
" update="POC" updatewhere=0 updatekeyinplace=no arguments=(("vessel_nr", number),("voyage_nr", string),("port_code", string),("pcn", number)) )
