 retrieve="SELECT 
		~"TYPE~" as category,
		VOYAGE_DOC_TYPES.DOC_TYPE as type_id,   
         	VOYAGE_DOC_TYPES.DOC_TYPE_DESC as description 
    FROM VOYAGE_DOC_TYPES, VESSELS   
WHERE (VESSEL_NR = :vessel) and 
(VESSELS.PC_NR=VOYAGE_DOC_TYPES.PC_NR) and 
(VOYAGE_DOC_TYPES.DOC_TYPE_ACTIVE=1)
UNION
SELECT ~"VOY~" as category,
	0 as type_id,
	VOYAGE_NR as description
FROM VOYAGES
WHERE VESSEL_NR = :vessel and 
(SUBSTRING(VOYAGE_NR,1,2) = SUBSTRING(convert(char,YEAR(getDate())+1),3,2) or 
SUBSTRING(VOYAGE_NR,1,2) = SUBSTRING(convert(char,YEAR(getDate())),3,2) or 
SUBSTRING(VOYAGE_NR,1,2) = SUBSTRING(convert(char,YEAR(getDate())-1),3,2))" arguments=(("vessel", number))  sort="category A type_id A description D " )
