 column=(type=char(35) updatewhereclause=yes name=port_n dbname="PORTS.PORT_N" dbalias=".PORT_N" )
 column=(type=long updatewhereclause=yes name=pcn dbname="POC_TASK_LIST.PCN" dbalias="A.PCN" )
