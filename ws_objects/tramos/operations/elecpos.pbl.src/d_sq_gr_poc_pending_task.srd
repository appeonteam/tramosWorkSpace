 retrieve="SELECT A.VOYAGE_NR, A.PORT_CODE, PORT_N, A.PCN, DESCRIPTION
From POC_TASK_LIST A, POC_TASKS B, PORTS , POC_TASKS_CONFIG_PC C, POC, VOYAGES
WHERE A.TASK_ID = B.TASK_ID
AND A.PORT_CODE = PORTS.PORT_CODE
and A.VESSEL_NR = :al_vessel_nr 
AND A.VOYAGE_NR = :as_voyage_nr
AND A.VESSEL_NR =  POC.VESSEL_NR
AND A.VOYAGE_NR =  POC.VOYAGE_NR
AND A.PORT_CODE =  POC.PORT_CODE
AND A.PCN =  POC.PCN
AND C.TASK_ID = A.TASK_ID
AND C.PURPOSE_CODE = POC.PURPOSE_CODE
AND C.PC_NR = B.PC_NR 
AND C.TASK_USE_FINISH = 1
AND A.TASK_NA = 0 
AND A.TASK_DONE = 0
AND POC.VESSEL_NR = VOYAGES.VESSEL_NR
AND POC.VOYAGE_NR = VOYAGES.VOYAGE_NR
AND ((VOYAGES.VOYAGE_TYPE <>2 AND C.TASK_USE_TCOUT = 0) OR (VOYAGES.VOYAGE_TYPE=2))
ORDER BY POC.PORT_ARR_DT,  A.PORT_CODE, A.PCN" arguments=(("al_vessel_nr", number),("as_voyage_nr", string)) )
