 retrieve="  SELECT DISTINCT RTRIM(LTRIM(UPC.USERID)) USERID,
         USERS.FIRST_NAME,   
         USERS.LAST_NAME,   
         USERS.DELETED,   
         USERS.LOCKED
    FROM USERS,   
         USERS_PROFITCENTER UPC  
   WHERE (UPC.USERID = USERS.USERID) and
			(UPC.PC_NR IN (SELECT UPC.PC_NR FROM USERS_PROFITCENTER UPC WHERE UPC.USERID = :as_userid)) and
