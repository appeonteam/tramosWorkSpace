 retrieve="  SELECT BOL.VESSEL_NR,   
         BOL.VOYAGE_NR,   
         BOL.PORT_CODE,   
         BOL.PCN,   
         BOL.CAL_CERP_ID,   
         BOL.CHART_NR,   
         BOL.AGENT_NR,   
         BOL.LAYOUT,   
         BOL.GRADE_GROUP,   
         BOL.GRADE_NAME,   
         BOL.BOL_NR,   
         BOL.LAYOUT_BL,   
         BOL.DENSITY,   
         BOL.L_D,   
         BOL.CARGO_TEMP,   
         BOL.RATE,   
         BOL.RATE_TYPE,   
         BOL.ROB,   
         BOL.NOR_DT,   
         BOL.DEPARTURE_DT,   
         BOL.ALL_FAST_DT,   
         BOL.NR_ISSUED,   
         BOL.GEAR,   
         BOL.TERMINAL,   
         BOL.DESTINATION,   
         BOL.RECIEVED,   
         BOL.BOL_QUANTITY,   
         BOL.TEMP_TYPE,   
         BOL.SAFE_MEMO,   
         BOL.BOL_ID,   
         BOL.CARGO_DETAIL_ID,   
         BOL.MSPS_CARGO_ID,
		(CASE WHEN BOL.L_D = 1 THEN (SELECT PORTS.PORT_N FROM PORTS WHERE PORT_CODE = BOL.PORT_CODE)
		ELSE (SELECT  PORTS.PORT_N FROM PORTS, CD CD1, CD CD2 WHERE PORTS.PORT_CODE = CD1.PORT_CODE 
				AND CD2.CARGO_DETAIL_ID = BOL.CARGO_DETAIL_ID AND CD1.CARGO_DETAIL_ID = CD2.LOAD_CARGO_DETAIL_ID) END) AS PORT_NAME,
		(SELECT LOAD_CARGO_DETAIL_ID FROM CD WHERE CARGO_DETAIL_ID = BOL.CARGO_DETAIL_ID) AS LOAD_CARGO_DETAIL_ID,
		0 as DUMMY_CD_ID,
		(SELECT VALIDATE_CARGO_TEMP FROM PROFIT_C, VESSELS WHERE PROFIT_C.PC_NR = VESSELS.PC_NR AND VESSELS.VESSEL_NR = :al_vessel_nr) AS VALIDATE_CARGO_TEMP,
		(SELECT DIRTY_PRODUCT FROM GROUPS WHERE GRADE_GROUP = BOL.GRADE_GROUP) AS DIRTY_PRODUCT
    FROM BOL   
WHERE  BOL.VESSEL_NR = :al_vessel_nr 
 AND     BOL.VOYAGE_NR = :as_voyage_nr
 AND     BOL.PORT_CODE = :as_port_code
 AND     BOL.PCN = :ai_pcn
ORDER BY BOL.BOL_NR" update="BOL" updatewhere=1 updatekeyinplace=no arguments=(("al_vessel_nr", number),("as_voyage_nr", string),("as_port_code", string),("ai_pcn", number))  sort="bol_nr A " )
