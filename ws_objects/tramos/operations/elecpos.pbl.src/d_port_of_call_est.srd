 retrieve="  SELECT POC_EST.PORT_CODE,   
         POC_EST.VESSEL_NR,   
         POC_EST.PCN,   
         POC_EST.VOYAGE_NR,   
         POC_EST.COMMENTS,   
         POC_EST.PORT_ARR_DT,   
         POC_EST.PURPOSE_CODE,   
     	POC_EST.PORT_DEPT_DT,
         	POC_EST.AGENT_NR,   
        	AGENTS.AGENT_SN,
		POC_EST.PORT_BERTHING_TIME,
		VOYAGES.VOYAGE_TYPE,
		POC_EST.LOCAL_DATES_UPDATED,
		POC_EST.DATES_UPDATED_BY,
		POC_EST.COMMENTS_UPDATED_BY,
		POC_EST.COMMENTS_UPDATED,
         POC_EST.POC_EST_PERFORMING_AGENT_NR,
         UPPER(P_AGENTS.COMPANY_SN) AS PERFORMING_AGENT_SN,
         POC_EST.AUTOSCHEDULE_STATUS,
			POC_EST.OIL,
			POC_EST.TANK_NAME,
			POC_EST.FRESH_WATER,
			POC_EST.SLOP,
			POC_EST.MSPS_TFV,
         0 AS PRECEEDING_TIME,
         getdate() AS PREVIOUS_ETD,
			getdate() AS CALC_PETD,
			0 AS SHIPTYPE_PORTSTAY,
			0.00 AS POC_STEAMINGTIME,
			0.00 AS POC_PORTSTAY,
			USERS.POC_PORTSTAY_UNIT,
			USERS.STEAMINGTIME_UNIT,
			USERS.POC_ENABLE_AUTO_SCHEDULE, 
			POC_EST.AUTO_ARR_STATUS, 
			POC_EST.AUTO_BER_STATUS, 
			POC_EST.AUTO_DEPT_STATUS,
			POC_EST.ROB_LATEST_DATE,
			POC_EST.ROB_HFO,
			POC_EST.ROB_DO,
			POC_EST.ROB_GO,
			POC_EST.ROB_LSHFO,
			POC_EST.ARR_HFO,
			POC_EST.ARR_DO,
			POC_EST.ARR_GO,
			POC_EST.ARR_LSHFO,
			POC_EST.DEPT_HFO,
			POC_EST.DEPT_DO,
			POC_EST.DEPT_GO,
			POC_EST.DEPT_LSHFO,
			0.0000 as ORDERED_HFO,   
			0.0000 as ORDERED_DO,   
			0.0000 as ORDERED_GO,
			0.0000 as ORDERED_LSHFO,
			PROFIT_C.TCE_PR_DAY_IN_POC,
			PROFIT_C.POC_SLOP_MANDATORY,
			(SELECT FIXTURE_PROFIT_MARGIN_PR_DAY FROM VAS_LOG_POC WHERE VAS_LOG_POC.VESSEL_NR = VOYAGES.VESSEL_NR AND VAS_LOG_POC.VOYAGE_NR = VOYAGES.VOYAGE_NR 
			AND VAS_LOG_POC.LOG_ID = (SELECT MAX(LOG_ID) FROM VAS_LOG_POC WHERE VESSEL_NR = VOYAGES.VESSEL_NR AND VOYAGE_NR = VOYAGES.VOYAGE_NR)) AS FIXTURE_PROFIT_MARGIN_PR_DAY,
			(SELECT EST_ACT_PROFIT_MARGIN_PR_DAY FROM VAS_LOG_POC WHERE VAS_LOG_POC.VESSEL_NR = VOYAGES.VESSEL_NR AND VAS_LOG_POC.VOYAGE_NR = VOYAGES.VOYAGE_NR 
			AND VAS_LOG_POC.LOG_ID = (SELECT MAX(LOG_ID) FROM VAS_LOG_POC WHERE VESSEL_NR = VOYAGES.VESSEL_NR AND VOYAGE_NR = VOYAGES.VOYAGE_NR)) AS EST_ACT_PROFIT_MARGIN_PR_DAY,
			(SELECT count(*)
				FROM BP_DETAILS
				WHERE PORT_CODE =POC_EST.PORT_CODE AND VESSEL_NR = POC_EST.VESSEL_NR AND 
				VOYAGE_NR = POC_EST.VOYAGE_NR AND PCN = POC_EST.PCN) as BP_COUNT,
			VOYAGES.CAL_CALC_ID
    FROM POC_EST  ,
         COMPANIES P_AGENTS, 
         AGENTS,   
         VOYAGES,
			USERS,
		VESSELS,
		PROFIT_C
   WHERE   
         ( P_AGENTS.COMPANY_ID =* POC_EST.POC_EST_PERFORMING_AGENT_NR) and  
         ( AGENTS.AGENT_NR =* POC_EST.AGENT_NR) and  
         ( POC_EST.VESSEL_NR = VOYAGES.VESSEL_NR ) and  
         ( POC_EST.VOYAGE_NR = VOYAGES.VOYAGE_NR ) and  
         ( ( POC_EST.VESSEL_NR = :vessel_nr ) AND  
         ( POC_EST.VOYAGE_NR = :voyage_nr ) AND  
         ( POC_EST.PORT_CODE = :port_code ) AND  
         ( POC_EST.PCN = :pcn ) ) AND
			( USERS.USERID = :user_id)
		AND VESSELS.VESSEL_NR = POC_EST.VESSEL_NR
		AND VESSELS.PC_NR = PROFIT_C.PC_NR
" update="POC_EST" updatewhere=0 updatekeyinplace=no arguments=(("vessel_nr", number),("voyage_nr", string),("port_code", string),("pcn", number),("user_id", string)) )
