 retrieve="  SELECT 
	    VOYAGE_DOCUMENT.DOC_NAME,
         VOYAGE_DOCUMENT.DOC_TYPE,   
         VOYAGE_DOC_TYPES.PC_NR,   
         VOYAGE_DOCUMENT.VOYAGE_NR,   
         VOYAGE_DOCUMENT.VESSEL_NR,   
         VOYAGE_DOCUMENT.DOCUMENT_ID,   
         VOYAGE_DOCUMENT.DESCRIPTION,
         VOYAGE_DOCUMENT.LAST_UPDATED_BY,
         VOYAGE_DOCUMENT.FILE_UPDATED_DATE,   
         VOYAGE_DOC_TYPES.DOC_TYPE_ACTIVE,   
         VOYAGE_DOC_TYPES.DOC_TYPE_DESC,
         VOYAGE_DOC_TYPES.DOC_TYPE,
         0 as insert_index, 
         PROFIT_C.PC_NAME, 
         0 as CURRENT_PC,
			VOYAGE_DOCUMENT.DB_NAME
    FROM VOYAGE_DOCUMENT, VOYAGE_DOC_TYPES, PROFIT_C
   WHERE ( VOYAGE_DOCUMENT.DOC_TYPE =* VOYAGE_DOC_TYPES.DOC_TYPE )  AND 
  			( VOYAGE_DOCUMENT.VESSEL_NR = :vessel ) AND
         ( VOYAGE_DOCUMENT.VOYAGE_NR = :voyage ) AND
         ( VOYAGE_DOC_TYPES.PC_NR = PROFIT_C.PC_NR) AND
			VOYAGE_DOC_TYPES.PC_NR = (SELECT VESSELS.PC_NR FROM VESSELS WHERE VESSELS.VESSEL_NR=  :vessel )    

union

  SELECT 
	    VOYAGE_DOCUMENT.DOC_NAME,
         VOYAGE_DOCUMENT.DOC_TYPE,   
         VOYAGE_DOC_TYPES.PC_NR,   
         VOYAGE_DOCUMENT.VOYAGE_NR,   
         VOYAGE_DOCUMENT.VESSEL_NR,   
         VOYAGE_DOCUMENT.DOCUMENT_ID,   
         VOYAGE_DOCUMENT.DESCRIPTION,
         VOYAGE_DOCUMENT.LAST_UPDATED_BY,
         VOYAGE_DOCUMENT.FILE_UPDATED_DATE,   
         VOYAGE_DOC_TYPES.DOC_TYPE_ACTIVE,   
         VOYAGE_DOC_TYPES.DOC_TYPE_DESC,
         VOYAGE_DOC_TYPES.DOC_TYPE,
         0 as insert_index, 
         PROFIT_C.PC_NAME, 
         1 as CURRENT_PC,
			VOYAGE_DOCUMENT.DB_NAME
    FROM VOYAGE_DOCUMENT, VOYAGE_DOC_TYPES, PROFIT_C
   WHERE ( VOYAGE_DOCUMENT.DOC_TYPE = VOYAGE_DOC_TYPES.DOC_TYPE )  AND 
  			( VOYAGE_DOCUMENT.VESSEL_NR = :vessel ) AND  
         ( VOYAGE_DOCUMENT.VOYAGE_NR = :voyage ) AND
         ( VOYAGE_DOC_TYPES.PC_NR = PROFIT_C.PC_NR) AND
			VOYAGE_DOC_TYPES.PC_NR <> (SELECT VESSELS.PC_NR FROM VESSELS WHERE VESSELS.VESSEL_NR=  :vessel )    
" update="VOYAGE_DOCUMENT" updatewhere=0 updatekeyinplace=no filter=" doc_type_active = 1 or not isNull( file_id )"arguments=(("vessel", number),("voyage", string))  sort="pc_nr A category A file_updated_date D " )
