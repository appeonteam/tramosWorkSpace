 retrieve="  SELECT POC.VESSEL_NR,   
         POC.VOYAGE_NR,   
         POC.PORT_CODE,   
         POC.PCN,   
         POC.PURPOSE_CODE,   
         CARGO.CAL_CERP_ID,   
         CARGO.CHART_NR,   
         CD.LAYOUT,   
         CD.GRADE_GROUP,   
         CD.GRADE_NAME,   
         CD.SHIPS_FIG,   
         CD.SURVEYOR_FIG,   
         CD.TOTAL_BOL_LOAD_QTY,   
         CD.TOTAL_BOL_DISCH_QTY,   
         CD.CD_MTBE_ETBE,   
         CD.CD_MTBE_ETBE_STATUS,   
         CD.CD_BIO,   
         CD.CD_BIO_STATUS,   
         CD.CD_DYE_MARKED,   
         BOL.LAYOUT,   
         BOL.GRADE_GROUP,   
         BOL.GRADE_NAME,   
         BOL.BOL_NR,   
         BOL.DENSITY,   
         BOL.L_D,   
         BOL.RATE,   
         BOL.ROB,   
         BOL.DEPARTURE_DT,   
         BOL.NOR_DT,   
         BOL.ALL_FAST_DT,   
         BOL.GEAR,   
         BOL.TERMINAL,   
         BOL.DESTINATION,   
         BOL.TEMP_TYPE,   
         BOL.BOL_QUANTITY,   
         BOL.RECIEVED  
    FROM BOL,   
         CARGO,   
         CD,   
         POC  
   WHERE ( BOL.VESSEL_NR = CARGO.VESSEL_NR ) and  
         ( CD.CARGO_DETAIL_ID = BOL.CARGO_DETAIL_ID ) and  
         ( POC.VESSEL_NR = CARGO.VESSEL_NR ) and  
         ( POC.VOYAGE_NR = CARGO.VOYAGE_NR ) and  
         ( POC.PORT_CODE = CARGO.PORT_CODE ) and  
         ( POC.PCN = CARGO.PCN ) and 
         ( CARGO.VESSEL_NR = CD.VESSEL_NR ) and  
         ( CARGO.VOYAGE_NR = CD.VOYAGE_NR ) and  
         ( CARGO.PORT_CODE = CD.PORT_CODE ) and  
         ( CARGO.PCN = CD.PCN ) and 
         ( CD.VESSEL_NR = BOL.VESSEL_NR ) and  
         ( CD.VOYAGE_NR = BOL.VOYAGE_NR ) and  
         ( CD.PORT_CODE = BOL.PORT_CODE ) and  
         ( CD.PCN = BOL.PCN ) and 
			( POC.VESSEL_NR = :l_vessel_nr) and
			( left(ltrim(POC.VOYAGE_NR),2) = ~"16~")
ORDER BY POC.VESSEL_NR ASC,   
         POC.VOYAGE_NR DESC   
" arguments=(("l_vessel_nr", number))  sort="poc_voyage_nr A " )
