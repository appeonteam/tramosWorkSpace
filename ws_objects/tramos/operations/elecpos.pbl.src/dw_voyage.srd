 column=(type=decimal(4) updatewhereclause=yes name=idle_est_bunker_consumption dbname="IDLE_VOYAGE_VAS_FIGURES.IDLE_EST_BUNKER_CONSUMPTION" )
 column=(type=decimal(2) updatewhereclause=yes name=idle_est_port_exp_usd dbname="IDLE_VOYAGE_VAS_FIGURES.IDLE_EST_PORT_EXP_USD" )
 column=(type=long updatewhereclause=yes name=mismatch_flag dbname="mismatch_flag" initial="0" )
 retrieve="  SELECT VOYAGES.VESSEL_NR,   
         VOYAGES.VOYAGE_TYPE,   
         VOYAGES.VOYAGE_NR,   
         VOYAGES.CAL_CALC_ID,   
         VOYAGES.VOYAGE_COMMENTS,   
         VESSELS.VESSEL_REF_NR,
			VESSELS.VESSEL_NAME,
		IDLE_EST_BUNKER_CONSUMPTION,
		IDLE_EST_PORT_EXP_USD,
		0 as mismatch_flag
    FROM VOYAGES,   
         VESSELS,
IDLE_VOYAGE_VAS_FIGURES  
   WHERE ( VOYAGES.VESSEL_NR = VESSELS.VESSEL_NR ) and  
		VOYAGES.VESSEL_NR*= IDLE_VOYAGE_VAS_FIGURES.VESSEL_NR and
		VOYAGES.VOYAGE_NR*= IDLE_VOYAGE_VAS_FIGURES.VOYAGE_NR and
         ( ( VOYAGES.VESSEL_NR = :vessel_nr ) AND  
         ( VOYAGES.VOYAGE_NR = :voyage_nr ) )    
" update="VOYAGES" updatewhere=0 updatekeyinplace=no arguments=(("vessel_nr", number),("voyage_nr", string)) )
