 retrieve="SELECT DISTINCT PROCEED.VOYAGE_NR, 
PROCEED.PORT_CODE, 
PROCEED.PCN, 
PROCEED.PROC_DATE, 
PROCEED.PROC_TEXT, 
PROCEED.CANCEL, 
PROCEED.VESSEL_NR,
(SELECT ISNULL(COUNT(*),0) FROM DISB_EXPENSES DE WHERE DE.VESSEL_NR = PROCEED.VESSEL_NR AND DE.VOYAGE_NR = PROCEED.VOYAGE_NR AND DE.PORT_CODE = PROCEED.PORT_CODE AND DE.PCN = PROCEED.PCN) AS COUNT_DE,
(SELECT ISNULL(COUNT(*),0) FROM DISB_PAYMENTS DP WHERE DP.VESSEL_NR = PROCEED.VESSEL_NR AND DP.VOYAGE_NR = PROCEED.VOYAGE_NR AND DP.PORT_CODE = PROCEED.PORT_CODE AND DP.PCN = PROCEED.PCN) AS COUNT_DP  
FROM PROCEED, 
DISBURSEMENTS 
WHERE ( PROCEED.VESSEL_NR *= DISBURSEMENTS.VESSEL_NR) 
and ( PROCEED.VOYAGE_NR *= DISBURSEMENTS.VOYAGE_NR) 
and ( PROCEED.PORT_CODE *= DISBURSEMENTS.PORT_CODE) 
and ( PROCEED.PCN *= DISBURSEMENTS.PCN) 
and ( ( PROCEED.VESSEL_NR = :vessel_nr ) 
AND ( PROCEED.PORT_CODE <> ~" ~" )) 
ORDER BY PROCEED.VOYAGE_NR ASC, 
PROCEED.PROC_DATE ASC " filter=" (  proceed_cancel = 0 )  OR  (  proceed_cancel = 1 AND  ( count_de >0 OR  count_dp >0) ) "arguments=(("vessel_nr", number))  sort="sort_year_voyage A proceed_voyage_nr A proceed_proc_date A " )
