 retrieve="  SELECT SPECIAL_CLAIM.SPECIAL_CLAIM_ID,
		VESSELS.VESSEL_REF_NR,
		VESSELS.VESSEL_NAME,
         SPECIAL_CLAIM.VOYAGE_NR,   
         SPECIAL_CLAIM.INCOME,   
         CHART.CHART_N_1,   
         TCOWNERS.TCOWNER_N_1,   
         BROKERS.BROKER_NAME,   
         AGENTS.AGENT_N_1,   
         SPECIAL_CLAIM.THIRD_PARTY_NAME,   
         SPECIAL_CLAIM.SPECIAL_CLAIM_TYPE,   
         SPECIAL_CLAIM.TIMEBAR_DATE,   
         SPECIAL_CLAIM.FORWARDING_DATE,   
         SPECIAL_CLAIM.CURR_CODE,   
         SPECIAL_CLAIM.AMOUNT,
	    SPECIAL_CLAIM.RESPONSIBLE_PERSON,   
         USERS.FIRST_NAME+ ~" ~"+USERS.LAST_NAME as RESPONSIBLE,
		(SPECIAL_CLAIM.AMOUNT - ISNULL((SELECT SUM(AMOUNT) FROM SPECIAL_CLAIM_TRANSACTION WHERE SPECIAL_CLAIM_TRANSACTION.SPECIAL_CLAIM_ID = SPECIAL_CLAIM.SPECIAL_CLAIM_ID),0)) as  CLAIM_BALANCE ,
		OFFICES.OFFICE_SN AS RESPONSIBLE_OFFICE, 
		SPECIAL_CLAIM.RESPONSIBLE_OFFICE AS OFFICE_NR, 
		(SELECT MAX(ACTION_DATE) FROM SPECIAL_CLAIM_ACTION WHERE SPECIAL_CLAIM_ACTION.SPECIAL_CLAIM_ID = SPECIAL_CLAIM.SPECIAL_CLAIM_ID) AS LAST_ACTION,
   		SPECIAL_CLAIM.VESSEL_IN_POOL_BY AS IN_POOL_BY
    FROM OFFICES,   
         SPECIAL_CLAIM LEFT JOIN AGENTS ON SPECIAL_CLAIM.AGENT_NR = AGENTS.AGENT_NR
                       LEFT JOIN BROKERS ON SPECIAL_CLAIM.BROKER_NR = BROKERS.BROKER_NR
                       LEFT JOIN CHART ON SPECIAL_CLAIM.CHART_NR = CHART.CHART_NR
                       LEFT JOIN TCOWNERS ON TCOWNERS.TCOWNER_NR = SPECIAL_CLAIM.TCOWNER_NR,   
         USERS,
		VESSELS,
		USERS_PROFITCENTER
   WHERE ( SPECIAL_CLAIM.RESPONSIBLE_OFFICE = OFFICES.OFFICE_NR ) and  
         ( USERS.USERID = SPECIAL_CLAIM.RESPONSIBLE_PERSON )  and 
	    SPECIAL_CLAIM.VESSEL_NR = VESSELS.VESSEL_NR and
		VESSELS.PC_NR = USERS_PROFITCENTER.PC_NR and
		USERS_PROFITCENTER.USERID = suser_name()
ORDER BY  SPECIAL_CLAIM.SPECIAL_CLAIM_ID " )
