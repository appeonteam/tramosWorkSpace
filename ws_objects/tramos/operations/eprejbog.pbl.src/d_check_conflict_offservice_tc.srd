table(column=(type=decimal(0) updatewhereclause=yes identity=yes name=contract_id dbname="NTC_TC_CONTRACT.CONTRACT_ID" )
 column=(type=number updatewhereclause=yes name=ntc_tc_contract_tc_hire_in dbname="NTC_TC_CONTRACT.TC_HIRE_IN" values="in	1/out	0/" )
 column=(type=datetime updatewhereclause=yes name=ntc_tc_contract_tc_hire_cp_date dbname="NTC_TC_CONTRACT.TC_HIRE_CP_DATE" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=periode_id dbname="NTC_TC_PERIOD.TC_PERIODE_ID" )
 column=(type=datetime updatewhereclause=yes name=periode_start dbname="NTC_TC_PERIOD.PERIODE_START" )
 column=(type=datetime updatewhereclause=yes name=periode_end dbname="NTC_TC_PERIOD.PERIODE_END" )
 retrieve="SELECT   NTC_TC_CONTRACT.CONTRACT_ID,
			NTC_TC_CONTRACT.TC_HIRE_IN,
			NTC_TC_CONTRACT.TC_HIRE_CP_DATE,
			NTC_TC_PERIOD.TC_PERIODE_ID,   
         NTC_TC_PERIOD.PERIODE_START,   
         NTC_TC_PERIOD.PERIODE_END  
    FROM NTC_TC_CONTRACT,   
         NTC_TC_PERIOD  
   WHERE ( NTC_TC_PERIOD.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID ) and  
         (( :startdate < NTC_TC_PERIOD.PERIODE_START and 
								:startdate <= NTC_TC_PERIOD.PERIODE_END and
								:enddate >= NTC_TC_PERIOD.PERIODE_END) OR
         ( :enddate > NTC_TC_PERIOD.PERIODE_START and 
								:enddate <= NTC_TC_PERIOD.PERIODE_END and
								:startdate < NTC_TC_PERIOD.PERIODE_START) OR
         ( :enddate > NTC_TC_PERIOD.PERIODE_END and 
								:startdate < NTC_TC_PERIOD.PERIODE_END)) and
			( NTC_TC_CONTRACT.VESSEL_NR = :vessel )
	ORDER BY NTC_TC_CONTRACT.CONTRACT_ID,
			NTC_TC_PERIOD.PERIODE_START   
         
   " arguments=(("vessel", number),("startdate", datetime),("enddate", datetime)) )
