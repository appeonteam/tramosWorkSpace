/* Set the structure to be returned to the actual prices in the fields */
istr_fuel.sd_price_hfo 	= round(double(em_price_hfo.text),4)
istr_fuel.sd_price_do 		= round(double(em_price_do.text),4)
istr_fuel.sd_price_go 		= round(double(em_price_go.text),4)
istr_fuel.sd_price_lshfo 	= round(double(em_price_lshfo.text),4)
/* close window with structure as return value */
closewithreturn( parent , istr_fuel )
end event

