 column=(type=char(7) updatewhereclause=yes name=off_services_voyage_nr dbname="OFF_SERVICES.VOYAGE_NR" )
 column=(type=char(10) updatewhereclause=yes name=off_services_port_code dbname="OFF_SERVICES.PORT_CODE" )
 column=(type=char(35) updatewhereclause=yes name=ports_port_n dbname="PORTS.PORT_N" )
 column=(type=datetime updatewhereclause=yes name=off_services_off_start dbname="OFF_SERVICES.OFF_START" )
 column=(type=datetime updatewhereclause=yes name=off_services_off_end dbname="OFF_SERVICES.OFF_END" )
