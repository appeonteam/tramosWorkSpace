 column=(type=char(200) updatewhereclause=yes name=off_services_off_description dbname="OFF_SERVICES.OFF_DESCRIPTION" )
 column=(type=long updatewhereclause=yes name=adjusted_days dbname="ADJUSTED_DAYS" )
 column=(type=long updatewhereclause=yes name=adjusted_hours dbname="ADJUSTED_HOURS" )
 column=(type=long updatewhereclause=yes name=adjusted_minutes dbname="ADJUSTED_MINUTES" )
 column=(type=decimal(0) updatewhereclause=yes name=trans_to_tcin dbname="TRANS_TO_TCIN" )
 column=(type=long updatewhereclause=yes name=tcin_settled dbname="TCIN_SETTLED" )
 column=(type=decimal(0) updatewhereclause=yes name=trans_to_tcout dbname="TRANS_TO_TCOUT" )
 column=(type=long updatewhereclause=yes name=tcout_settled dbname="TCOUT_SETTLED" )
 column=(type=long updatewhereclause=yes name=claim_office_office_nr dbname="OFFICE_NR" )
 retrieve="  SELECT VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,   
         OFF_SERVICES.VOYAGE_NR,   
         OFF_SERVICES.PORT_CODE,   
         PORTS.PORT_N,   
         OFF_SERVICES.OFF_START,   
         OFF_SERVICES.OFF_END,   
         OFF_SERVICES.OFF_TIME_DAYS,   
         OFF_SERVICES.OFF_TIME_HOURS,   
         OFF_SERVICES.OFF_TIME_MINUTES,   
         OFF_SERVICES.OFF_DESCRIPTION,
		0 AS ADJUSTED_DAYS, 
		0 AS ADJUSTED_HOURS,
		0 AS ADJUSTED_MINUTES,
         (SELECT MAX(NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID) 
		    	FROM NTC_OFF_SERVICE,   
         			NTC_PAYMENT,   
         			NTC_TC_CONTRACT  
   			WHERE  NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID  and  
	          	NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID  and  
   	   		    	NTC_TC_CONTRACT.TC_HIRE_IN = 1 and
				NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TRANS_TO_TCIN,   
         (SELECT isNull(MAX(NTC_OFF_SERVICE.TRANS_TO_CODA),0) 
		    	FROM NTC_OFF_SERVICE,   
         			NTC_PAYMENT,   
         			NTC_TC_CONTRACT  
   			WHERE  NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID  and  
	          	NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID  and  
   	   		    	NTC_TC_CONTRACT.TC_HIRE_IN = 1 and
				NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TCIN_SETTLED,
         (SELECT MAX(NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID) 
		    	FROM NTC_OFF_SERVICE,   
         			NTC_PAYMENT,   
         			NTC_TC_CONTRACT  
   			WHERE  NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID  and  
	          	NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID  and  
   	   		    	NTC_TC_CONTRACT.TC_HIRE_IN = 0 and
				NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TRANS_TO_TCOUT,   
         (SELECT isNull(MAX(NTC_OFF_SERVICE.TRANS_TO_CODA),0) 
		    	FROM NTC_OFF_SERVICE,   
         			NTC_PAYMENT,   
         			NTC_TC_CONTRACT  
   			WHERE  NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID  and  
	          	NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID  and  
   	   		    	NTC_TC_CONTRACT.TC_HIRE_IN = 0 and
				NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TCOUT_SETTLED,
				
				CLAIM_OFFICE.OFFICE_NR


    FROM OFF_SERVICES,   
         PORTS,   
         VESSELS,
			(select DISTINCT VESSEL_NR, VOYAGE_NR, OFFICE_NR from CLAIMS where CLAIM_TYPE=~"FRT~" and OFFICE_NR is not null) CLAIM_OFFICE
  
   WHERE ( PORTS.PORT_CODE = OFF_SERVICES.PORT_CODE ) and  
         ( OFF_SERVICES.VESSEL_NR = VESSELS.VESSEL_NR ) and  
			(CLAIM_OFFICE.VESSEL_NR=*OFF_SERVICES.VESSEL_NR and CLAIM_OFFICE.VOYAGE_NR=*OFF_SERVICES.VOYAGE_NR) and
         ( (( OFF_SERVICES.OFF_START <= :adt_startdate ) AND  
         ( OFF_SERVICES.OFF_END >= :adt_enddate )) OR  
         (( OFF_SERVICES.OFF_START <= :adt_startdate ) AND  
         ( OFF_SERVICES.OFF_END >= :adt_startdate )) OR  
         (( OFF_SERVICES.OFF_START >= :adt_startdate ) AND  
         ( OFF_SERVICES.OFF_END <= :adt_enddate )) OR  
         (( OFF_SERVICES.OFF_START <= :adt_enddate ) AND  
         ( OFF_SERVICES.OFF_END >= :adt_enddate ))) AND  
         ( VESSELS.PC_NR in (:aint_pcnr))

UNION

select VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,
			~"AE~" as VOYAGE_NR,
			~"n/a~" as PORT_CODE,
			~"~" as PORT_N,
			ACTUAL_EARNINGS.STARTDATE,
		  	ACTUAL_EARNINGS.ENDDATE,
			0 as ACTUAL_EARNING_DAYS,
			0 as ACTUAL_EARNING_HOURS,
			0 as ACTUAL_EARNING_MINUTES,
			ACTUAL_EARNINGS.COMMENT,
			0 as ADJUSTED_DAYS,
			0 as ADJUSTED_HOURS,
			0 as ADJUSTED_MINUTES,
			0 as TRANS_TO_TCIN,
			0 as TCIN_SETTLED,
			0 as TRANS_TO_TCOUT,
			0 as TCOUT_SETTLED,
			0 as OFFICE_NR 
from 
		VESSELS, 
		ACTUAL_EARNINGS 
where
			ACTUAL_EARNINGS.VESSEL_NR=VESSELS.VESSEL_NR and
			( VESSELS.PC_NR in (:aint_pcnr) ) and  
			( (( ACTUAL_EARNINGS.STARTDATE <= :adt_startdate ) AND  
         ( ACTUAL_EARNINGS.ENDDATE >= :adt_enddate )) OR  
         (( ACTUAL_EARNINGS.STARTDATE <= :adt_startdate ) AND  
         ( ACTUAL_EARNINGS.ENDDATE >= :adt_startdate )) OR  
         (( ACTUAL_EARNINGS.STARTDATE >= :adt_startdate ) AND  
         ( ACTUAL_EARNINGS.ENDDATE <= :adt_enddate )) OR  
         (( ACTUAL_EARNINGS.STARTDATE <= :adt_enddate ) AND  
         ( ACTUAL_EARNINGS.ENDDATE >= :adt_enddate ))
			OR
			(ACTUAL_EARNINGS.ENDDATE is null and 
			ACTUAL_EARNINGS.STARTDATE <= :adt_enddate)
			)

 ORDER BY VESSELS.VESSEL_REF_NR ASC,   
         OFF_SERVICES.OFF_START ASC  
" arguments=(("adt_startdate", datetime),("adt_enddate", datetime),("aint_pcnr", numberlist)) )
