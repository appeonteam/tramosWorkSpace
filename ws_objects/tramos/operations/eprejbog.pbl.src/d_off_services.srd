 retrieve="  SELECT OFF_SERVICES.VESSEL_NR,   
         OFF_SERVICES.VOYAGE_NR,   
         OFF_SERVICES.PORT_CODE,   
         OFF_SERVICES.OFF_START,   
         OFF_SERVICES.OFF_END,   
         OFF_SERVICES.OFF_TIME_DAYS,   
         OFF_SERVICES.OFF_TIME_HOURS,   
         OFF_SERVICES.OFF_FUEL_OIL_USED,   
         OFF_SERVICES.OFF_DIESEL_OIL_USED,   
         OFF_SERVICES.OFF_GAS_OIL_USED,   
         OFF_SERVICES.OFF_LSHFO_OIL_USED,   
         OFF_SERVICES.HFO_STOCK_START,   
         OFF_SERVICES.HFO_STOCK_END,   
         OFF_SERVICES.DO_STOCK_START,   
         OFF_SERVICES.DO_STOCK_END,   
         OFF_SERVICES.GO_STOCK_START,   
         OFF_SERVICES.GO_STOCK_END,   
         OFF_SERVICES.LSHFO_STOCK_START,   
         OFF_SERVICES.LSHFO_STOCK_END,   
         OFF_SERVICES.OFF_DESCRIPTION,   
         OFF_SERVICES.OFF_TIME_MINUTES,   
         OFF_SERVICES.OFF_SCHEDULED,   
         (SELECT COUNT(*)
		FROM NTC_OFF_SERVICE NOS1, NTC_PAYMENT P1, NTC_TC_CONTRACT C1
		WHERE NOS1.PAYMENT_ID = P1.PAYMENT_ID
		AND P1.CONTRACT_ID = C1.CONTRACT_ID
		AND C1.TC_HIRE_IN = 1
		AND NOS1.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TRANS_TO_IN,   
         (SELECT COUNT(*)
		FROM NTC_OFF_SERVICE NOS2, NTC_PAYMENT P2, NTC_TC_CONTRACT C2
		WHERE NOS2.PAYMENT_ID = P2.PAYMENT_ID
		AND P2.CONTRACT_ID = C2.CONTRACT_ID
		AND C2.TC_HIRE_IN = 0
		AND NOS2.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID) as TRANS_TO_OUT,   
	CASE WHEN EXISTS(
		SELECT *
		FROM NTC_OFF_SERVICE
		INNER JOIN NTC_PAYMENT ON NTC_OFF_SERVICE.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID
		INNER JOIN NTC_TC_CONTRACT ON NTC_PAYMENT.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID
		WHERE NTC_OFF_SERVICE.OPS_OFF_SERVICE_ID = OFF_SERVICES.OPS_OFF_SERVICE_ID
			AND NTC_PAYMENT.PAYMENT_STATUS >= 3						
						) THEN 1 ELSE 0 END as TRANS_TO_CODA,
	   	OFF_SERVICES.OPS_OFF_SERVICE_ID,
		VESSELS.VESSEL_REF_NR,
	   	OFF_SERVICES.COMMENTS
    FROM OFF_SERVICES,   
			VESSELS   
   WHERE OFF_SERVICES.VESSEL_NR = VESSELS.VESSEL_NR AND
			OFF_SERVICES.VESSEL_NR = :vessel_nr   
ORDER BY OFF_SERVICES.OFF_START ASC  
" arguments=(("vessel_nr", number)) )
