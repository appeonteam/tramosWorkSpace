 retrieve="  SELECT DISTINCT CAL_CARG.CAL_CARG_EXRATE_USD,   
         CAL_CARG.CAL_CARG_CURR_CODE,   
         CAL_CARG.FIXED_EXRATE_ENABLED,   
         CAL_CALC.CAL_CALC_FIX_ID,   
         CAL_CERP.CAL_CERP_ID,   
         CAL_CERP.CHART_NR,   
         CAL_CARG.FIXED_EXRATE,   
         CAL_CARG.CLAIM_CURR  
    FROM CAL_CALC,   
         CAL_CARG,   
         CAL_CERP  
   WHERE ( CAL_CARG.CAL_CALC_ID = CAL_CALC.CAL_CALC_ID ) and  
         ( CAL_CERP.CAL_CERP_ID = CAL_CARG.CAL_CERP_ID ) and  
         ( ( CAL_CALC.CAL_CALC_VESSEL_ID = :vessel_nr ) AND  
         ( CAL_CALC.CAL_CALC_ID = :calc_id ) AND  
         ( CAL_CALC.CAL_CALC_STATUS = 6 ) AND  
         ( CAL_CERP.CHART_NR = :chart_nr ) )    
" arguments=(("vessel_nr", number),("calc_id", number),("chart_nr", number)) )
