E_NR ) and ( LAYTIME_STATEMENTS.PORT_CODE = POC.PORT_CODE ) and ( LAYTIME_STATEMENTS.PCN = POC.PCN ) and ( ( LAYTIME_STATEMENTS.VESSEL_NR = :VESSEL_NR ) AND ( LAYTIME_STATEMENTS.VOYAGE_NR = :VOYAGE_NR ) AND ( LAYTIME_STATEMENTS.CHART_NR = :CHART_NR ) ) AND DEM_DES_CLAIMS.DEM_DES_SETTLED <> 0 ORDER BY POC.PORT_ARR_DT ASC, LAY_DEDUCTIONS.REASON_START ASC " arguments=(("vessel_nr", number),("voyage_nr", string),("chart_nr", number)) )
