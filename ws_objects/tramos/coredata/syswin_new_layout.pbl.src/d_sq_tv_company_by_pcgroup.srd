 retrieve="

  SELECT PF_COMPANY.COMPANYID,   
         PF_COMPANY.COMPANY,   
         PF_COMPANY.SHORTNAME,   
		PF_COMPANYCONTACTS.FULLNAME as CONTACT,
         PF_COMPANYCONTACTS.EMAIL ,
		PF_COMPANYCONTACTS.CONTACTSID,
		0 AS SELECTEMAIL,
		PF_COMPANY.DEACTIVATED,
		PF_COMPANYCONTACTS.DEACTIVATED,
		PF_COMPANY.PCGROUP_ID
    FROM PF_COMPANY,   
         PF_COMPANYCONTACTS  
   WHERE ( PF_COMPANYCONTACTS.COMPANYID =* PF_COMPANY.COMPANYID ) 
		AND PF_COMPANY.PCGROUP_ID = :pcgroupid 
		//AND PF_COMPANY.DEACTIVATED = 0 
		//AND PF_COMPANY.CATEGORYID IS NOT NULL
		AND PF_COMPANYCONTACTS.FULLNAME <> ~"login_info~"

