		 PF_COMPANY.COMPANY,  
         PF_COMPANY.SHORTNAME,   
		 ~"_Chart. email~" as CONTACT,
         PF_COMPANY.EMAIL2 as EMAIL
    FROM PF_COMPANY  
   WHERE PF_COMPANY.PCGROUP_ID = :pcgroupid  AND DEACTIVATED = 0 AND CATEGORYID IS NOT NULL
union

  SELECT PF_COMPANY.COMPANYID,   
		 PF_COMPANY.COMPANY,  
         PF_COMPANY.SHORTNAME,   
		~"_Oper. email~" as CONTACT,
         PF_COMPANY.EMAIL as EMAIL  
    FROM PF_COMPANY  
   WHERE PF_COMPANY.PCGROUP_ID = :pcgroupid AND DEACTIVATED = 0 AND CATEGORYID IS NOT NULL

union*/" arguments=(("pcgroupid", number))  sort="company A companyid A contact A " )
