 retrieve="
  SELECT PF_COMPANY.COMPANYID,   
         PF_COMPANY.COMPANY,   
         PF_COMPANY.SHORTNAME,   
		PF_COMPANYCONTACTS.FULLNAME as CONTACT,
         PF_COMPANYCONTACTS.EMAIL AS EMAIL,
		0 AS SELECTEMAIL,
		PF_COMPANYCONTACTS.STAFFRESPONSIBILITY AS DEPARTMENT,
		PF_COMPANYCONTACTS.CONTACTSID AS CONTACTID,
		PF_COMPANY.DEACTIVATED,
		PF_COMPANYCONTACTS.DEACTIVATED AS CONTACT_DEACTIVATED
    FROM PF_COMPANY,   
         PF_COMPANYCONTACTS  
   WHERE ( PF_COMPANYCONTACTS.COMPANYID =* PF_COMPANY.COMPANYID ) 
		AND PF_COMPANY.PCGROUP_ID = :pcgroupid 
		//AND PF_COMPANYCONTACTS.DEACTIVATED = 0 
		AND PF_COMPANYCONTACTS.FULLNAME <> ~"login_info~"
		AND  PF_COMPANYCONTACTS.EMAIL IS NOT NULL
" update="PF_COMPANY" updatewhere=0 updatekeyinplace=no arguments=(("pcgroupid", number))  sort="company A companyid A contact A " )
