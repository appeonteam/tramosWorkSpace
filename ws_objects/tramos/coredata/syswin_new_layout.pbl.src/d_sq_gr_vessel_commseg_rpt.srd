 column=(type=char(50) updatewhereclause=yes name=comm_segment dbname="COMM_SEGMENT.COMM_SEGMENT_NAME" )
 column=(type=char(8) updatewhereclause=yes name=active dbname="ACTIVE" )
 retrieve="  SELECT VESSELS.VESSEL_REF_NR,   
         VESSELS.VESSEL_NAME,
			PROFIT_C.PC_NAME,   
         COMM_SEGMENT.COMM_SEGMENT_NAME,   
         (CASE VESSELS.VESSEL_ACTIVE WHEN 1 THEN 'Active' WHEN 0 THEN 'Inactive' END) ACTIVE
    FROM VESSELS LEFT OUTER JOIN COMM_SEGMENT ON VESSELS.COMM_SEGMENT_ID = COMM_SEGMENT.COMM_SEGMENT_ID ,
			PROFIT_C
   WHERE ( VESSELS.PC_NR = PROFIT_C.PC_NR) AND
         ( VESSELS.PC_NR IN (SELECT PROFIT_C.PC_NR FROM PROFIT_C, USERS_PROFITCENTER
                                WHERE PROFIT_C.PC_NR = USERS_PROFITCENTER.PC_NR 
                                  AND USERS_PROFITCENTER.USERID = :as_userid))  
 
" arguments=(("as_userid", string)) )
