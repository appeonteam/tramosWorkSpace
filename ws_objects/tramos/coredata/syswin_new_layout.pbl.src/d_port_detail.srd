 retrieve="  SELECT PORTS.COUNTRY_ID,   
         PORTS.PORT_CODE,   
         PORTS.PORT_N,   
         PORTS.PORT_UNCTAD,   
         PORTS.DISB_PORT_CODE,   
         PORTS.PORT_S,   
         PORTS.VIA_POINT,   
         PORTS.PORT_ACTIVE,   
         PORTS.GMT_DIFF,   
         PORTS.HYPERLINK,   
         PORTS.ABC_PORTID,   
         ATOBVIAC_PORT.ABC_PORTNAME,   
         CAL_UNCT.CAL_UNCT_PORT_NAME,   
         PORTS.RKST_PORTID,   
         PORTS.CREATED,   
         PORTS.CREATEDBY,   
         PORTS.VETT_REDFLAG,   
         PORTS.VETT_NOTES,   
         PORTS.DETAILS
    FROM ATOBVIAC_PORT RIGHT OUTER JOIN PORTS ON ATOBVIAC_PORT.ABC_PORTID = PORTS.ABC_PORTID LEFT OUTER JOIN CAL_UNCT ON PORTS.PORT_UNCTAD = CAL_UNCT.CAL_UNCT_UNCTADCODE  
   WHERE PORTS.PORT_CODE = :as_port_code    
" update="PORTS" updatewhere=0 updatekeyinplace=yes arguments=(("as_port_code", string)) )
