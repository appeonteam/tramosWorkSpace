 retrieve="SELECT	POC_TASKS_CONFIG_PC.PC_NR,   
			POC_TASKS_CONFIG_PC.PURPOSE_CODE,   
			POC_TASKS.DESCRIPTION,
			POC_TASKS_CONFIG_PC.TASK_ID,   
			POC_TASKS_CONFIG_PC.TASK_SORT,
			POC_TASKS.TASK_MVV_NAME,
			PURPOSE.PURPOSE_DESC,
			POC_TASKS_CONFIG_PC.TASK_USE_TCOUT,
			POC_TASKS_CONFIG_PC.TASK_USE_FINISH
FROM		POC_TASKS_CONFIG_PC, POC_TASKS, PURPOSE
WHERE	POC_TASKS_CONFIG_PC.PC_NR = :pc_nr and   
			POC_TASKS_CONFIG_PC.PURPOSE_CODE = :purpose_code and
			( PURPOSE.PURPOSE_CODE = POC_TASKS_CONFIG_PC.PURPOSE_CODE ) and  
			POC_TASKS_CONFIG_PC.TASK_ID = POC_TASKS.TASK_ID

