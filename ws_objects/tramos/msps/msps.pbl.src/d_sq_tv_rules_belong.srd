 column=(type=long updatewhereclause=yes name=vessel_config dbname="VESSEL_CONFIG" )
 column=(type=long updatewhereclause=yes name=voyage_config dbname="VOYAGE_CONFIG" )
 retrieve="  SELECT CAL_VEST.CAL_VEST_TYPE_ID,
         CAL_VEST.CAL_VEST_TYPE_NAME, 
         VESSELS.VESSEL_NR, 
         VESSELS.VESSEL_REF_NR, 
         VESSELS.VESSEL_NAME, 
         VESSELS.VESSEL_REF_NR + ' ' + VESSELS.VESSEL_NAME AS VESSEL_DISPLAY, 
         VOYAGES.VOYAGE_NR, 
         (SELECT COUNT(*) FROM RUL_CONFIG WHERE RUL_CONFIG.CAL_VEST_TYPE_ID = CAL_VEST.CAL_VEST_TYPE_ID) AS VEST_TYPE_CONFIG, 
         (SELECT COUNT(*) FROM RUL_CONFIG WHERE RUL_CONFIG.VESSEL_NR = VESSELS.VESSEL_NR AND RUL_CONFIG.VOYAGE_NR IS NULL) AS VESSEL_CONFIG,
         (SELECT COUNT(*) FROM RUL_CONFIG WHERE RUL_CONFIG.VESSEL_NR = VESSELS.VESSEL_NR AND RUL_CONFIG.VOYAGE_NR = VOYAGES.VOYAGE_NR) AS VOYAGE_CONFIG
    FROM CAL_VEST, VESSELS, VOYAGES, VOYAGE_TYPE
   WHERE CAL_VEST.CAL_VEST_TYPE_ID = VESSELS.CAL_VEST_TYPE_ID AND VESSELS.VESSEL_ACTIVE = 1 AND
         VESSELS.VESSEL_NR = VOYAGES.VESSEL_NR AND (VOYAGES.VOYAGE_FINISHED = 0 or VOYAGES.VOYAGE_FINISHED IS NULL) AND
         VOYAGES.VOYAGE_TYPE = VOYAGE_TYPE.ID AND RUL_ENABLED = 1 AND
         VESSELS.VESSEL_NR IN (:al_vessel_nr)
ORDER BY CAL_VEST.CAL_VEST_TYPE_NAME ASC, VESSELS.VESSEL_REF_NR ASC, (CASE WHEN LEFT(VOYAGES.VOYAGE_NR, 2) > '50' THEN '19' ELSE '20' END) + VOYAGES.VOYAGE_NR ASC
" arguments=(("al_vessel_nr", numberlist)) )
