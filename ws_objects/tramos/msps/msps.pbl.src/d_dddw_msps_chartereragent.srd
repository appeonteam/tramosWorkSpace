 retrieve="  SELECT CARGO.VESSEL_NR,   
         CARGO.VOYAGE_NR,   
         CARGO.PORT_CODE,   
         CARGO.PCN,   
         CARGO.AGENT_NR,   
         CARGO.CHART_NR,   
         AGENTS.AGENT_SN,   
         CHART.CHART_SN,   
         CARGO.CAL_CERP_ID,   
         CAL_CERP.CAL_CERP_DESCRIPTION,
		 	CHART.CHART_SN + ' / ' + AGENTS.AGENT_SN as CHARTAGENT_DISP,
			CAST(CARGO.CAL_CERP_ID as VARCHAR(7)) + '@' + CAST(CARGO.CHART_NR as VARCHAR(5)) + '@' + CAST(CARGO.AGENT_NR as VARCHAR(5)) AS CHARTAGENT
    FROM CARGO,   
         AGENTS,   
         CHART,   
         CAL_CERP  
   WHERE ( AGENTS.AGENT_NR = CARGO.AGENT_NR ) and  
         ( CHART.CHART_NR = CARGO.CHART_NR ) and  
         ( CAL_CERP.CAL_CERP_ID = CARGO.CAL_CERP_ID ) and  
         ( ( CARGO.VESSEL_NR = :vessel_nr ) AND  
         ( CARGO.VOYAGE_NR = :voyage_nr ) AND  
         ( CARGO.PORT_CODE = :port_code ) AND  
         ( CARGO.PCN = :pcn ))    
" arguments=(("vessel_nr", number),("voyage_nr", string),("port_code", string),("pcn", number)) )
