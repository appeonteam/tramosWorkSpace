 retrieve="  SELECT RUL_ALERTS.ALERTS_ID,   
         RUL_ALERTS.RUL_ID,   
         RUL_ALERTS.VESSEL_IMO,   
         RUL_ALERTS.REPORT_ID,   
         RUL_ALERTS.REVISION_NO,   
         RUL_ALERTS.MSG_TYPE,   
         RUL_ALERTS.VESSEL_NR,   
         RUL_ALERTS.VOYAGE_NR,   
         RUL_ALERTS.PORT_CODE,   
         RUL_ALERTS.PCN,   
         RUL_ALERTS.SEVERITY_ID,   
         RUL_ALERTS.ALERTS_MSG,   
         RUL_ALERTS.CREATE_DATE,   
         RUL_ALERTS.ACKNOWLEDGE,   
         RUL_ALERTS.ACKN_BY,   
         RUL_ALERTS.ACKN_DATE,   
         RUL_SEVERITIES.SEVERITY_LEVEL  
    FROM RUL_ALERTS LEFT OUTER JOIN RUL_SEVERITIES ON RUL_ALERTS.SEVERITY_ID = RUL_SEVERITIES.SEVERITY_ID  
   WHERE ( RUL_ALERTS.VESSEL_IMO = :al_vesselimo ) AND  
         ( RUL_ALERTS.REPORT_ID = :al_reportid ) AND  
         ( RUL_ALERTS.REVISION_NO = :al_revisionno ) AND  
         ( RUL_ALERTS.MSG_TYPE = :as_msgtype )    
" update="RUL_ALERTS" updatewhere=0 updatekeyinplace=no arguments=(("al_vesselimo", number),("al_reportid", number),("al_revisionno", number),("as_msgtype", string)) )
