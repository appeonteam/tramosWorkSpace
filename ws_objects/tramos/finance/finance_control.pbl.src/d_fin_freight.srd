 column=(type=number updatewhereclause=yes name=claims_claim_amount_usd dbname="CLAIMS.CLAIM_AMOUNT_USD" dbalias=".CLAIM_AMOUNT_USD" )
 column=(type=long updatewhereclause=yes name=afc dbname="AFC" )
 retrieve="  SELECT CLAIMS.CHART_NR,   
         CLAIMS.CLAIM_NR,   
         CHART.CHART_N_1,   
         BROKERS.BROKER_NAME,   
         OFFICES.OFFICE_NAME,   
         CLAIMS.FORWARDING_DATE,   
         CLAIMS.CURR_CODE,   
         CLAIMS.CLAIM_AMOUNT,   
         CLAIMS.CLAIM_AMOUNT_USD,
		(SELECT max(FA.AFC_NR) FROM FREIGHT_ADVANCED FA WHERE FA.VESSEL_NR = CLAIMS.VESSEL_NR AND FA.VOYAGE_NR = CLAIMS.VOYAGE_NR AND FA.CHART_NR = CLAIMS.CHART_NR AND FA.CLAIM_NR = CLAIMS.CLAIM_NR ) AS AFC  
    FROM CHART,   
         CLAIMS,   
         BROKERS,   
         OFFICES  
   WHERE ( BROKERS.BROKER_NR =* CLAIMS.BROKER_NR) and  
         ( OFFICES.OFFICE_NR =* CLAIMS.OFFICE_NR) and  
         ( CLAIMS.CHART_NR = CHART.CHART_NR ) and  
         ( ( CLAIMS.VESSEL_NR = :ai_vessel_nr ) AND  
         ( CLAIMS.VOYAGE_NR = :as_voyage_nr ) AND  
         ( CLAIMS.CLAIM_TYPE = ~"FRT~" ) )    
" arguments=(("ai_vessel_nr", number),("as_voyage_nr", string)) )
