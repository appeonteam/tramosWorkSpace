 column=(type=number updatewhereclause=yes name=claim_adrcomm dbname="CLAIMS.CLAIM_ADRCOMM" )
 column=(type=char(5) updatewhereclause=yes name=vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 retrieve="  SELECT DISTINCT CLAIMS.VESSEL_NR,   
			VESSELS.VESSEL_NAME,
         CLAIMS.VOYAGE_NR,   
         CLAIMS.CLAIM_NR,   
         CLAIMS.CLAIM_TYPE,   
         CLAIMS.CHART_NR,
			CHART.CHART_N_1,   
			VESSELS.PC_NR,
			ACTEST_POC.DISCHARGE_DATE as DISCHARGE_DATE,
			(CLAIMS.CLAIM_AMOUNT_USD + (select isnull(sum(FREIGHT_RECEIVED.FREIGHT_RECEIVED),0)
													from FREIGHT_RECEIVED
													WHERE ( FREIGHT_RECEIVED.CHART_NR = CLAIMS.CHART_NR) and  
															( FREIGHT_RECEIVED.VESSEL_NR = CLAIMS.VESSEL_NR) and  
															( FREIGHT_RECEIVED.VOYAGE_NR = CLAIMS.VOYAGE_NR) and  
															( FREIGHT_RECEIVED.CLAIM_NR = CLAIMS.CLAIM_NR) and  
															( FREIGHT_RECEIVED.TRANS_CODE <> 'C')) + (SELECT isnull(SUM(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0) 
																																	FROM CLAIM_TRANSACTION
																																	WHERE ( CLAIM_TRANSACTION.CHART_NR = CLAIMS.CHART_NR) and  
																																		( CLAIM_TRANSACTION.VESSEL_NR = CLAIMS.VESSEL_NR) and  
																																		( CLAIM_TRANSACTION.VOYAGE_NR = CLAIMS.VOYAGE_NR) and  
																																		( CLAIM_TRANSACTION.CLAIM_NR = CLAIMS.CLAIM_NR))) as CLAIM_AMOUNT_USD, 
			(select isnull(sum(FREIGHT_RECEIVED.FREIGHT_RECEIVED),0)
				from FREIGHT_RECEIVED
			   WHERE ( FREIGHT_RECEIVED.CHART_NR = CLAIMS.CHART_NR) and  
						( FREIGHT_RECEIVED.VESSEL_NR = CLAIMS.VESSEL_NR) and  
						( FREIGHT_RECEIVED.VOYAGE_NR = CLAIMS.VOYAGE_NR) and  
						( FREIGHT_RECEIVED.CLAIM_NR = CLAIMS.CLAIM_NR)  and
						( FREIGHT_RECEIVED.TRANS_CODE = ~"R~")) as CLAIM_REC,
			(select MAX (FREIGHT_RECEIVED.FREIGHT_REC_DATE)
				from FREIGHT_RECEIVED
			   WHERE ( FREIGHT_RECEIVED.CHART_NR = CLAIMS.CHART_NR) and  
						( FREIGHT_RECEIVED.VESSEL_NR = CLAIMS.VESSEL_NR) and  
						( FREIGHT_RECEIVED.VOYAGE_NR = CLAIMS.VOYAGE_NR) and  
						( FREIGHT_RECEIVED.CLAIM_NR = CLAIMS.CLAIM_NR) and
						( FREIGHT_RECEIVED.TRANS_CODE = ~"R~")) as SETTLED_DATE,
			CLAIMS.BROKER_NR,
			CLAIMS.OFFICE_NR,
			CLAIMS.CP_DATE,
			CLAIMS.ADDRESS_COM as CLAIM_ADRCOMM,
			VESSELS.VESSEL_REF_NR 
	FROM CLAIMS LEFT JOIN (SELECT PROCEED.VOYAGE_NR, PROCEED.VESSEL_NR ,MAX(ISNULL(POC.PORT_DEPT_DT, POC_EST.PORT_DEPT_DT)) DISCHARGE_DATE
                               FROM PROCEED 
                               LEFT JOIN POC ON PROCEED.VESSEL_NR = POC.VESSEL_NR AND PROCEED.VOYAGE_NR = POC.VOYAGE_NR AND PROCEED.PORT_CODE = POC.PORT_CODE AND PROCEED.PCN = POC.PCN 
                               LEFT JOIN POC_EST ON PROCEED.VESSEL_NR = POC_EST.VESSEL_NR AND PROCEED.VOYAGE_NR = POC_EST.VOYAGE_NR AND PROCEED.PORT_CODE = POC_EST.PORT_CODE AND PROCEED.PCN = POC_EST.PCN
                               WHERE (POC.PURPOSE_CODE IN('D','L/D') OR POC_EST.PURPOSE_CODE IN ('D', 'L/D') ) AND (POC.PORT_DEPT_DT IS NOT NULL OR POC_EST.PORT_DEPT_DT IS NOT NULL) 
                               GROUP BY PROCEED.VESSEL_NR, PROCEED.VOYAGE_NR
                               ) ACTEST_POC ON ACTEST_POC.VESSEL_NR = CLAIMS.VESSEL_NR AND ACTEST_POC.VOYAGE_NR = CLAIMS.VOYAGE_NR,
			VESSELS,
			CHART
	 WHERE ( CLAIMS.CLAIM_TYPE = ~"FRT~") AND 
			( CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR) AND
			( CLAIMS.CHART_NR = CHART.CHART_NR) AND
			( ABS(CLAIMS.CLAIM_AMOUNT_USD - (SELECT isnull(SUM(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0) 
															FROM CLAIM_TRANSACTION
															WHERE ( CLAIM_TRANSACTION.CHART_NR = CLAIMS.CHART_NR) and  
																( CLAIM_TRANSACTION.VESSEL_NR = CLAIMS.VESSEL_NR) and  
																( CLAIM_TRANSACTION.VOYAGE_NR = CLAIMS.VOYAGE_NR) and  
																( CLAIM_TRANSACTION.CLAIM_NR = CLAIMS.CLAIM_NR)))) < 0.01  
	AND ACTEST_POC.DISCHARGE_DATE IS NOT NULL"  sort="claims_vessel_nr A compute_voyage_sort A claims_voyage_nr A " )
