 retrieve="SELECT DISTINCT CLAIMS.VESSEL_NR,
       VESSELS.VESSEL_REF_NR,
       VESSELS.VESSEL_NAME,
       CLAIMS.VOYAGE_NR,
       CLAIMS.CLAIM_NR,
       CLAIMS.CLAIM_TYPE,
       CLAIMS.CREATED_BY,
	   'Charterer' AS 'Opponent Type',
       CLAIMS.CHART_NR AS 'CHART/OPPONENT_NO',
       CHART.CHART_N_1 AS 'CHART/OPPONENT_NAME',
       CHART.NOM_ACC_NR AS 'CHART/OPPONENT_NOM_ACC_NO',
       CLAIMS.BROKER_NR,
       BROKERS.BROKER_NAME,
       round((round(CLAIMS.CLAIM_AMOUNT, 2) - isnull(TRANS.TRANS_AMOUNT, 0)) * 
	       (
			 dbo.FN_GET_EXCHANGERATE(
			   CAL_CARG_CURRENCY.DEM_CURR_CODE,
				CAL_CARG_CURRENCY.SET_EXRATE,
				CAL_CARG_CURRENCY.DEM_EXRATE,
				CAL_CARG_CURRENCY.FREIGHT_EXRATE,
				CAL_CARG_CURRENCY.FREIGHT_CURR_CODE,
				CLAIMS.CLAIM_TYPE,
				CLAIMS.CURR_CODE,
				LATEST_EXCHANGE_RATE.EXRATE_USD)
			 ) / 100,2) as OUT_AMOUNT_USD,
       VESSELS.PC_NR,
       CLAIMS.FORWARDING_DATE,
       DISCH_DATE =  (SELECT MAX(ISNULL(POC.PORT_DEPT_DT, POC_EST.PORT_DEPT_DT))
                     			FROM PROCEED LEFT JOIN POC ON PROCEED.VESSEL_NR = POC.VESSEL_NR AND PROCEED.VOYAGE_NR = POC.VOYAGE_NR AND PROCEED.PORT_CODE = POC.PORT_CODE AND PROCEED.PCN = POC.PCN
                                  					  LEFT JOIN POC_EST ON PROCEED.VESSEL_NR = POC_EST.VESSEL_NR AND PROCEED.VOYAGE_NR = POC_EST.VOYAGE_NR AND PROCEED.PORT_CODE = POC_EST.PORT_CODE AND PROCEED.PCN = POC_EST.PCN 
                     			WHERE CLAIMS.VESSEL_NR = PROCEED.VESSEL_NR AND CLAIMS.VOYAGE_NR = PROCEED.VOYAGE_NR
                     			AND (POC.PURPOSE_CODE IN('D','L/D') OR POC_EST.PURPOSE_CODE IN ('D', 'L/D') ) AND (POC.PORT_DEPT_DT IS NOT NULL OR POC_EST.PORT_DEPT_DT IS NOT NULL) 
                    			),
       CLAIMS.CP_DATE,
       CLAIMS.OFFICE_NR,
       (CASE CLAIMS.CLAIM_SENT WHEN 1 THEN 1 ELSE 0 END) AS CLAIM_SENT,
       CLAIMS.CLAIM_SENT_DATE,
       (select SENT_AMOUNT
          from CLAIM_SENT
         where CLAIM_SENT.CHART_NR = CLAIMS.CHART_NR 
           and CLAIM_SENT.VESSEL_NR = CLAIMS.VESSEL_NR 
           and CLAIM_SENT.VOYAGE_NR = CLAIMS.VOYAGE_NR 
           and CLAIM_SENT.CLAIM_NR = CLAIMS.CLAIM_NR 
         group by CLAIM_SENT.CHART_NR, CLAIM_SENT.VESSEL_NR, CLAIM_SENT.VOYAGE_NR, CLAIM_SENT.CLAIM_NR
        having SENT_NR = min(SENT_NR)) *
	       (
			 dbo.FN_GET_EXCHANGERATE(
			   CAL_CARG_CURRENCY.DEM_CURR_CODE,
				CAL_CARG_CURRENCY.SET_EXRATE,
				CAL_CARG_CURRENCY.DEM_EXRATE,
				CAL_CARG_CURRENCY.FREIGHT_EXRATE,
				CAL_CARG_CURRENCY.FREIGHT_CURR_CODE,
				CLAIMS.CLAIM_TYPE,
				CLAIMS.CURR_CODE,
				LATEST_EXCHANGE_RATE.EXRATE_USD)
			 ) / 100 as ORIGINAL_AMOUNT_IN_USD,   
       (CASE CLAIMS.BROKER_CONFIRMED WHEN 1 THEN 1 ELSE 0 END) AS BROKER_CONFIRMED,
       CLAIMS.BROKER_CONFIRMED_DATE,
       (CASE CLAIMS.CHARTERER_CONFIRMED WHEN 1 THEN 1 ELSE 0 END) AS  CHARTERER_CONFIRMED, 
       CLAIMS.CHARTERER_CONFIRMED_DATE,
       CLAIMS.[STATUS], 
       CLAIMS.STATUS_DATE, 
       CLAIMS.CLAIM_EMAIL,
       CLAIMS.CLAIM_COMMENT,
	  ''  AS 'Receivable/Payable',
	  '' AS 'Estimate/Actual',
 			CLAIMS.CLAIM_RESPONSIBLE
  FROM CLAIMS LEFT JOIN(
           select TR.CHART_NR, TR.VESSEL_NR, TR.VOYAGE_NR, TR.CLAIM_NR, sum(TR.C_TRANS_AMOUNT) as TRANS_AMOUNT
           from CLAIM_TRANSACTION TR
           group by TR.CHART_NR, TR.VESSEL_NR, TR.VOYAGE_NR, TR.CLAIM_NR
       ) TRANS ON (CLAIMS.CHART_NR = TRANS.CHART_NR and CLAIMS.VESSEL_NR = TRANS.VESSEL_NR and CLAIMS.VOYAGE_NR = TRANS.VOYAGE_NR and CLAIMS.CLAIM_NR = TRANS.CLAIM_NR)
       LEFT JOIN BROKERS ON(CLAIMS.BROKER_NR = BROKERS.BROKER_NR)
		 LEFT JOIN VOYAGES ON (VOYAGES.VESSEL_NR = CLAIMS.VESSEL_NR AND VOYAGES.VOYAGE_NR = CLAIMS.VOYAGE_NR)
		 LEFT JOIN CAL_CARG_CURRENCY ON (CAL_CARG_CURRENCY.CAL_CERP_ID = CLAIMS.CAL_CERP_ID AND CAL_CARG_CURRENCY.CAL_CALC_ID = VOYAGES.CAL_CALC_ID)
		 LEFT JOIN LATEST_EXCHANGE_RATE ON (LATEST_EXCHANGE_RATE.CURR_CODE = CLAIMS.CURR_CODE),
       VESSELS,
       CHART
 WHERE ( CLAIMS.CLAIM_TYPE NOT IN ('DEM', 'FRT')) AND 
       ( CLAIMS.INVOICE_NR IS NOT NULL) AND
		 ( VESSELS.PC_NR IN (:aan_pcnr)) AND
       ( CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR) AND
       ( CLAIMS.CHART_NR = CHART.CHART_NR) AND
       ( round(round(CLAIMS.CLAIM_AMOUNT,2) - isnull(TRANS.TRANS_AMOUNT, 0) ,2) > 0)

UNION ALL

	SELECT SPECIAL_CLAIM.VESSEL_NR,
       VESSELS.VESSEL_REF_NR,
       VESSELS.VESSEL_NAME,
       SPECIAL_CLAIM.VOYAGE_NR,
       SPECIAL_CLAIM.SPECIAL_CLAIM_ID,
       'SPEC',
     (SELECT ISNULL(USERS.FIRST_NAME, '') + ' ' + ISNULL(USERS.LAST_NAME, '') FROM SPECIAL_CLAIM_ACTION, USERS WHERE SPECIAL_CLAIM_ID = SPECIAL_CLAIM.SPECIAL_CLAIM_ID AND SPECIAL_CLAIM_ACTION.CREATED_BY = USERS.USERID AND SPECIAL_CLAIM_ACTION.DESCRIPTION = 'Claim Created!') AS CREATED_BY,
	   (CASE DEBTOR_CREDITOR WHEN 1 THEN 'Charterer' WHEN 2 THEN 'TC Owner' WHEN 3 THEN 'Head Owner' WHEN 4 THEN 'Agent' WHEN 5 THEN 'Broker' WHEN 6 THEN '3rd Party' END),
       (CASE DEBTOR_CREDITOR WHEN 1 THEN SPECIAL_CLAIM.CHART_NR WHEN 2 THEN SPECIAL_CLAIM.TCOWNER_NR WHEN 3 THEN SPECIAL_CLAIM.TCOWNER_NR WHEN 4 THEN SPECIAL_CLAIM.AGENT_NR WHEN 5 THEN SPECIAL_CLAIM.BROKER_NR WHEN 6 THEN NULL END),
	   (CASE DEBTOR_CREDITOR WHEN 1 THEN CHART.CHART_N_1 WHEN 2 THEN TCOWNERS.TCOWNER_N_1 WHEN 3 THEN TCOWNERS.TCOWNER_N_1 WHEN 4 THEN AGENTS.AGENT_N_1 WHEN 5 THEN  BROKERS.BROKER_NAME WHEN 6 THEN THIRD_PARTY_NAME END),
       CASE DEBTOR_CREDITOR WHEN 1 THEN CHART.NOM_ACC_NR WHEN 2 THEN TCOWNERS. NOM_ACC_NR WHEN 3 THEN TCOWNERS. NOM_ACC_NR WHEN 4 THEN AGENTS.NOM_ACC_NR WHEN 5 THEN BROKERS.NOM_ACC_NR  ELSE NULL END,
       NULL,
       NULL,
       ROUND(SPECIAL_CLAIM.AMOUNT_USD - ISNULL((SELECT SUM(AMOUNT_USD) FROM SPECIAL_CLAIM_TRANSACTION WHERE SPECIAL_CLAIM_ID = SPECIAL_CLAIM.SPECIAL_CLAIM_ID), 0), 2),
       VESSELS.PC_NR,
       SPECIAL_CLAIM.FORWARDING_DATE,
       DISCH_DATE =  (SELECT MAX(ISNULL(POC.PORT_DEPT_DT, POC_EST.PORT_DEPT_DT))
                     			FROM PROCEED LEFT JOIN POC ON PROCEED.VESSEL_NR = POC.VESSEL_NR AND PROCEED.VOYAGE_NR = POC.VOYAGE_NR AND PROCEED.PORT_CODE = POC.PORT_CODE AND PROCEED.PCN = POC.PCN
                                  					  LEFT JOIN POC_EST ON PROCEED.VESSEL_NR = POC_EST.VESSEL_NR AND PROCEED.VOYAGE_NR = POC_EST.VOYAGE_NR AND PROCEED.PORT_CODE = POC_EST.PORT_CODE AND PROCEED.PCN = POC_EST.PCN 
                     			WHERE SPECIAL_CLAIM.VESSEL_NR = PROCEED.VESSEL_NR AND SPECIAL_CLAIM.VOYAGE_NR = PROCEED.VOYAGE_NR
                     			AND (POC.PURPOSE_CODE IN('D','L/D') OR POC_EST.PURPOSE_CODE IN ('D', 'L/D') ) AND (POC.PORT_DEPT_DT IS NOT NULL OR POC_EST.PORT_DEPT_DT IS NOT NULL) 
                    			),
       dbo.FN_GET_CPDATEBYVOYAGE(SPECIAL_CLAIM.VESSEL_NR, SPECIAL_CLAIM.VOYAGE_NR, (CASE SPECIAL_CLAIM.DEBTOR_CREDITOR WHEN 1 THEN SPECIAL_CLAIM.CHART_NR ELSE 0 END)),
       RESPONSIBLE_OFFICE,
       NULL,
       NULL,
       NULL,
       NULL,
       NULL,
       NULL,
       NULL,
       CASE WHEN SPECIAL_CLAIM.FORWARDING_DATE IS NULL and dateadd(day, 7, getdate()) >= SPECIAL_CLAIM.TIMEBAR_DATE AND ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL(TRANS.TRANS_AMOUNT, 0), 2) <> 0  THEN 'Claim already due or due within the next week. Please forward claim before ' + CONVERT(UNIVARCHAR(10), SPECIAL_CLAIM.TIMEBAR_DATE, 104)
       WHEN SPECIAL_CLAIM.FORWARDING_DATE IS NULL and dateadd(day, 7, getdate()) < SPECIAL_CLAIM.TIMEBAR_DATE and dateadd(day, 14, getdate()) >= SPECIAL_CLAIM.TIMEBAR_DATE AND ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL(TRANS.TRANS_AMOUNT, 0), 2) <> 0  THEN 'Claim due within the next two weeks. Please forward claim before ' + CONVERT(UNIVARCHAR(10), SPECIAL_CLAIM.TIMEBAR_DATE, 104)
       WHEN ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL(TRANS.TRANS_AMOUNT, 0), 2) = 0 THEN 'Claim settled'
       WHEN SPECIAL_CLAIM.FORWARDING_DATE IS NULL AND DATEADD(DAY, 14, GETDATE()) < SPECIAL_CLAIM.TIMEBAR_DATE AND ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL(TRANS.TRANS_AMOUNT, 0), 2) <> 0 THEN 'More than two weeks until timebar date (' + CONVERT(UNIVARCHAR(10), SPECIAL_CLAIM.TIMEBAR_DATE, 104) + ')'
       WHEN SPECIAL_CLAIM.FORWARDING_DATE IS NOT NULL AND ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL(TRANS.TRANS_AMOUNT, 0), 2) <> 0 THEN 'Claim already forwarded (' + CONVERT(UNIVARCHAR(10), SPECIAL_CLAIM.FORWARDING_DATE, 104) + ')' 
       ELSE NULL END,
       NULL,
       NULL,
       SPECIAL_CLAIM.SPECIAL_CLAIM_TYPE,
	  CASE SPECIAL_CLAIM.INCOME WHEN 1 THEN 'Receivable' ELSE 'Payable' END,
	  CASE SPECIAL_CLAIM.ESTIMATE WHEN 0 THEN 'Actual' ELSE 'Estimate' END,
     SPECIAL_CLAIM.RESPONSIBLE_PERSON	
FROM SPECIAL_CLAIM LEFT JOIN CHART ON SPECIAL_CLAIM.CHART_NR = CHART.CHART_NR
                     LEFT JOIN TCOWNERS ON SPECIAL_CLAIM.TCOWNER_NR = TCOWNERS.TCOWNER_NR
                      LEFT JOIN AGENTS ON SPECIAL_CLAIM.AGENT_NR = AGENTS.AGENT_NR
                     LEFT JOIN  BROKERS ON SPECIAL_CLAIM.BROKER_NR = BROKERS.BROKER_NR
				LEFT JOIN ( SELECT SPECIAL_CLAIM_ID, SUM(AMOUNT) TRANS_AMOUNT FROM SPECIAL_CLAIM_TRANSACTION GROUP BY SPECIAL_CLAIM_ID) TRANS ON SPECIAL_CLAIM.SPECIAL_CLAIM_ID = TRANS.SPECIAL_CLAIM_ID,
    VESSELS
WHERE SPECIAL_CLAIM.VESSEL_NR = VESSELS.VESSEL_NR 
AND ( VESSELS.PC_NR IN (:aan_pcnr)) 
AND ROUND(SPECIAL_CLAIM.AMOUNT - ISNULL((SELECT SUM(AMOUNT) FROM SPECIAL_CLAIM_TRANSACTION WHERE SPECIAL_CLAIM_ID = SPECIAL_CLAIM.SPECIAL_CLAIM_ID), 0), 2) > 0
