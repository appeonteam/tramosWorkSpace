 retrieve="SELECT V.VESSEL_NR, V.VESSEL_NAME, C.DELIVERY, CH.CHART_N_1, P.EST_DUE_DATE, V.VESSEL_REF_NR
		FROM NTC_CONT_BROKER_COMM BC, 
			NTC_PAYMENT P, 
			NTC_TC_CONTRACT C,
			VESSELS V,
			CHART CH
		WHERE BC.CONTRACT_ID = P.CONTRACT_ID
		AND P.CONTRACT_ID = C.CONTRACT_ID
		AND C.VESSEL_NR = V.VESSEL_NR
		AND C.CHART_NR = CH.CHART_NR
		AND V.PC_NR IN (:profitcenter) 
		AND C.TC_HIRE_IN = 0
		AND BC.BROKER_NR = :broker
		AND SUBSTRING(CONVERT(VARCHAR, DATEPART(YY, P.EST_DUE_DATE)),3,4) = :year
		AND P.PAYMENT_STATUS = 5
		AND P.PAYMENT_ID NOT IN 
		(SELECT PAYMENT_ID FROM NTC_COMMISSION WHERE BROKER_NR = :broker)" arguments=(("year", string),("profitcenter", numberlist),("broker", number)) )
