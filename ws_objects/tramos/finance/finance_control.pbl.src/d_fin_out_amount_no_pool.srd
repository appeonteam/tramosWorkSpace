 column=(type=char(5) updatewhereclause=yes name=vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" dbalias=".VESSEL_REF_NR" )
 retrieve="  SELECT CLAIMS.VESSEL_NR,   
			VESSELS.VESSEL_NAME,
         CLAIMS.VOYAGE_NR,   
         CLAIMS.CLAIM_NR,   
         CLAIMS.CLAIM_TYPE,   
         CLAIMS.CHART_NR,
			CHART.CHART_N_1,   
         CLAIMS.BROKER_NR,
			BROKERS.BROKER_NAME,
			PROFIT_C.PC_NAME,   
         CLAIMS.CLAIM_AMOUNT_USD -
			(select isnull(sum(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0)
				from CLAIM_TRANSACTION
			   WHERE ( CLAIMS.CHART_NR = CLAIM_TRANSACTION.CHART_NR) and  
						( CLAIMS.VESSEL_NR = CLAIM_TRANSACTION.VESSEL_NR) and  
						( CLAIMS.VOYAGE_NR = CLAIM_TRANSACTION.VOYAGE_NR) and  
						( CLAIMS.CLAIM_NR = CLAIM_TRANSACTION.CLAIM_NR)) as compute_0011, CLAIMS.CP_DATE,
			0 as CONTRACT_ID,
			VESSELS.VESSEL_REF_NR  
  
	FROM CLAIMS,
			VESSELS,
			CHART,
			PROFIT_C,
			BROKERS
   WHERE ( substring(CLAIMS.VOYAGE_NR,1,2) >= ~"00~" and substring(CLAIMS.VOYAGE_NR,1,2) < ~"50~") and
			( CLAIMS.CLAIM_TYPE <> ~"FRT~" ) AND 
			( CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR) AND
			( VESSELS.PC_NR = PROFIT_C.PC_NR) AND
			( CLAIMS.CHART_NR = CHART.CHART_NR) AND
			( CLAIMS.BROKER_NR *= BROKERS.BROKER_NR) AND
			( CLAIMS.CLAIM_AMOUNT_USD - 
			(select isnull(sum(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0)
				from CLAIM_TRANSACTION
			   WHERE ( CLAIMS.CHART_NR = CLAIM_TRANSACTION.CHART_NR) and  
						( CLAIMS.VESSEL_NR = CLAIM_TRANSACTION.VESSEL_NR) and  
						( CLAIMS.VOYAGE_NR = CLAIM_TRANSACTION.VOYAGE_NR) and  
						( CLAIMS.CLAIM_NR = CLAIM_TRANSACTION.CLAIM_NR)) > 0.01) and
			(VESSELS.PC_NR <> 4 )
			 						
UNION


  SELECT CLAIMS.VESSEL_NR,   
			VESSELS.VESSEL_NAME,
         CLAIMS.VOYAGE_NR,   
         CLAIMS.CLAIM_NR,   
         CLAIMS.CLAIM_TYPE,   
         CLAIMS.CHART_NR,
			CHART.CHART_N_1,   
         CLAIMS.BROKER_NR,
			BROKERS.BROKER_NAME,
			PROFIT_C.PC_NAME,   
         CLAIMS.CLAIM_AMOUNT_USD,
			CLAIMS.CP_DATE,
			0 as CONTRACT_ID,
			VESSELS.VESSEL_REF_NR  
    FROM CLAIMS,
			VESSELS,
			CHART,
			PROFIT_C,
			BROKERS
   WHERE ( substring(CLAIMS.VOYAGE_NR,1,2) >= ~"00~" and substring(CLAIMS.VOYAGE_NR,1,2) < ~"50~") and
			( CLAIMS.CLAIM_TYPE = ~"FRT~" ) AND 
			( CLAIMS.VESSEL_NR = VESSELS.VESSEL_NR) AND
			( VESSELS.PC_NR = PROFIT_C.PC_NR) AND
			( CLAIMS.CHART_NR = CHART.CHART_NR) AND
			( CLAIMS.BROKER_NR *= BROKERS.BROKER_NR) AND
			( CLAIMS.CLAIM_AMOUNT_USD - 
			(select isnull(sum(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0)
				from CLAIM_TRANSACTION
			   WHERE ( CLAIMS.CHART_NR = CLAIM_TRANSACTION.CHART_NR) and  
						( CLAIMS.VESSEL_NR = CLAIM_TRANSACTION.VESSEL_NR) and  
						( CLAIMS.VOYAGE_NR = CLAIM_TRANSACTION.VOYAGE_NR) and  
						( CLAIMS.CLAIM_NR = CLAIM_TRANSACTION.CLAIM_NR)) > 0.01) and
			(VESSELS.PC_NR <> 4 )
	
UNION
  SELECT NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
 			NTC_TC_PERIOD.TCOUT_VOYAGE_NR,
			NTC_TC_CONTRACT.TCOWNER_NR,
			~"TC-OUT~",
         CHART.CHART_NR,
         CHART.CHART_N_1,
         0,
			~"~",   
			PROFIT_C.PC_NAME,
         NTCP.PAYMENT_BALANCE,
			NTC_TC_CONTRACT.TC_HIRE_CP_DATE,
			NTC_TC_CONTRACT.CONTRACT_ID,
			VESSELS.VESSEL_REF_NR
    FROM NTC_PAYMENT NTCP,   
         NTC_TC_CONTRACT,   
         CHART,   
         TCOWNERS,   
         VESSELS,   
         NTC_TC_PERIOD,
			PROFIT_C
   WHERE ( CHART.CHART_NR =* NTC_TC_CONTRACT.CHART_NR) and  
         ( TCOWNERS.TCOWNER_NR =* NTC_TC_CONTRACT.TCOWNER_NR) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTCP.CONTRACT_ID ) and  
			( VESSELS.PC_NR = PROFIT_C.PC_NR) AND
         ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and  
         ( NTC_TC_PERIOD.CONTRACT_ID = NTC_TC_CONTRACT.CONTRACT_ID ) and 
         ( NTC_TC_PERIOD.PERIODE_START <= NTCP.EST_DUE_DATE ) and
         ( NTC_TC_PERIOD.PERIODE_END > NTCP.EST_DUE_DATE ) and
			( NTC_TC_CONTRACT.TC_HIRE_IN = 0) AND        
		   ( NTCP.PAYMENT_BALANCE <> 0 ) AND
		   ( NTCP.EST_DUE_DATE < :adt_today) AND
			( NTC_TC_CONTRACT.CHART_NR NOT IN (1202, 280, 1180)) and
			(VESSELS.PC_NR <> 4 )
         
 " filter=" out_amount > 0"arguments=(("adt_today", datetime))  sort="claims_vessel_nr A compute_voyage_sort A " )
