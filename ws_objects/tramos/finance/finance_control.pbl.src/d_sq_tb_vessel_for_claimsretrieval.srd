 column=(type=char(50) update=yes updatewhereclause=yes name=vessel_name dbname="VESSELS.VESSEL_NAME" )
 column=(type=char(5) updatewhereclause=yes name=vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 column=(type=number updatewhereclause=yes name=vessel_active dbname="VESSELS.VESSEL_ACTIVE" )
 retrieve="  SELECT VESSELS.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         VESSELS.VESSEL_REF_NR,
		VESSELS.VESSEL_ACTIVE  
    FROM VESSELS  
   WHERE ( VESSELS.PC_NR in ( :profitcenter ) ) AND  
         ( VESSELS.VESSEL_ACTIVE = 1 )   
UNION
SELECT DISTINCT VESSELS.VESSEL_NR,   
	VESSELS.VESSEL_NAME,   
	VESSELS.VESSEL_REF_NR,
	VESSELS.VESSEL_ACTIVE  
FROM VESSELS, CLAIMS  
WHERE VESSELS.VESSEL_NR = CLAIMS.VESSEL_NR
AND VESSELS.PC_NR in ( :profitcenter )
AND VESSELS.VESSEL_ACTIVE = 0 
ORDER BY VESSELS.VESSEL_REF_NR ASC   
" arguments=(("profitcenter", numberlist)) )
