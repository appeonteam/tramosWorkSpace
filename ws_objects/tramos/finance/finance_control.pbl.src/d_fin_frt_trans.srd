 column=(type=datetime updatewhereclause=yes name=freight_rec_date dbname="FREIGHT_RECEIVED.FREIGHT_REC_DATE" )
 column=(type=number updatewhereclause=yes name=freight_received dbname="FREIGHT_RECEIVED.FREIGHT_RECEIVED" )
 column=(type=number updatewhereclause=yes name=freight_received_local_curr dbname="FREIGHT_RECEIVED.FREIGHT_RECEIVED_LOCAL_CURR" )
 column=(type=char(10) updatewhereclause=yes name=trans_code dbname="FREIGHT_RECEIVED.TRANS_CODE" )
 column=(type=long updatewhereclause=yes name=claim_nr dbname="FREIGHT_RECEIVED.CLAIM_NR" )
 column=(type=long updatewhereclause=yes name=chart_nr dbname="FREIGHT_RECEIVED.CHART_NR" )
 retrieve="  SELECT  isnull(nullif(FREIGHT_RECEIVED.FREIGHT_COMMENTS,''), 'Received') as FREIGHT_COMMENTS ,
         FREIGHT_RECEIVED.FREIGHT_REC_DATE,   
         FREIGHT_RECEIVED.FREIGHT_RECEIVED,   
         FREIGHT_RECEIVED.FREIGHT_RECEIVED_LOCAL_CURR,   
         FREIGHT_RECEIVED.TRANS_CODE,   
         FREIGHT_RECEIVED.CLAIM_NR,   
         FREIGHT_RECEIVED.CHART_NR  
    FROM FREIGHT_RECEIVED  
   WHERE ( FREIGHT_RECEIVED.VESSEL_NR = :ai_vessel ) AND  
         ( FREIGHT_RECEIVED.VOYAGE_NR = :as_voyage ) AND  
         ( FREIGHT_RECEIVED.CHART_NR = :ai_chart ) AND  
         ( FREIGHT_RECEIVED.CLAIM_NR = :ai_claim )   
   UNION   
  SELECT CLAIM_TRANSACTION.C_TRANS_COMMENT,   
         CLAIM_TRANSACTION.C_TRANS_VAL_DATE,   
		CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD,            
		CLAIM_TRANSACTION.C_TRANS_AMOUNT,      
         CLAIM_TRANSACTION.C_TRANS_CODE,   
         CLAIM_TRANSACTION.CLAIM_NR,   
         CLAIM_TRANSACTION.CHART_NR  
    FROM CLAIM_TRANSACTION  
   WHERE ( CLAIM_TRANSACTION.VESSEL_NR = :ai_vessel ) AND  
         ( CLAIM_TRANSACTION.VOYAGE_NR = :as_voyage ) AND  
         ( CLAIM_TRANSACTION.CHART_NR = :ai_chart ) AND  
         ( CLAIM_TRANSACTION.CLAIM_NR = :ai_claim )    
UNION
  SELECT  'Received' as FREIGHT_COMMENTS ,
         AFR.AFC_RECIEVED_DATE,   
         AFR.AFC_RECIEVED,   
         AFR.AFC_RECEIVED_LOCAL_CURR,   
         AFR.TRANS_CODE,   
         AFR.CLAIM_NR,   
         AFR.CHART_NR  
    FROM FREIGHT_ADVANCED_RECIEVED AFR  
   WHERE ( AFR.VESSEL_NR = :ai_vessel ) AND  
         ( AFR.VOYAGE_NR = :as_voyage ) AND  
         ( AFR.CHART_NR = :ai_chart ) AND  
         ( AFR.CLAIM_NR = :ai_claim )   
" arguments=(("ai_vessel", number),("as_voyage", string),("ai_chart", number),("ai_claim", number)) )
