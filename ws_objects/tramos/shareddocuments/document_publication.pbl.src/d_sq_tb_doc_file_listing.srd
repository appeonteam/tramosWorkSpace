 retrieve="
SELECT DISTINCT
	DOCUMENT_PUBLICATION.DOCUMENT_ID,   
	DOCUMENT_PUBLICATION.LAST_EDITED_BY,   
	DOCUMENT_PUBLICATION.DOCUMENT_TYPE,   
	DOCUMENT_PUBLICATION.EXTERNAL_ACCESS,   
	DOCUMENT_PUBLICATION.OWNED_BY,
	DOCUMENT_PUBLICATION.CREATED_BY,
	DOCUMENT_PUBLICATION.FILE_NAME,
	0 as INSERT_INDEX,
	(SELECT ISNULL(FIRST_NAME,~"~")+~" ~"+ISNULL(LAST_NAME,~"~") FROM USERS WHERE USERID = DOCUMENT_PUBLICATION.CREATED_BY) AS CREATED_NAME,   
	(SELECT ISNULL(FIRST_NAME,~"~")+~" ~"+ISNULL(LAST_NAME,~"~") FROM USERS WHERE USERID = DOCUMENT_PUBLICATION.LAST_EDITED_BY) AS LAST_EDITED_NAME,   
	DOCUMENT_PUBLICATION.CREATED_DATE,   
	DOCUMENT_PUBLICATION.UPDATED_DATE,   
	DOCUMENT_PUBLICATION.DOCUMENT_DESCRIPTION,
	DOCUMENT_PUBLICATION.DB_NAME
    FROM DOCUMENT_PUBLICATION,   
         DOCUMENT_ACCESSIBLE_BY,   
         USERS_PROFITCENTER
 WHERE ( DOCUMENT_PUBLICATION.DOCUMENT_ID = DOCUMENT_ACCESSIBLE_BY.DOCUMENT_ID ) AND  
       ( DOCUMENT_ACCESSIBLE_BY.PC_NR = USERS_PROFITCENTER.PC_NR ) AND 
       ( ( DOCUMENT_PUBLICATION.EXTERNAL_ACCESS >= :external_access ) AND  
       ( USERS_PROFITCENTER.USERID = :current_userid ))  
ORDER BY DOCUMENT_PUBLICATION.CREATED_DATE DESC" update="DOCUMENT_PUBLICATION" updatewhere=0 updatekeyinplace=no arguments=(("external_access", number),("current_userid", string))  sort="created_date D " )
