 retrieve="  SELECT CAL_BUNKERADDITIONALS.CAL_DESCRIPTION,
			PC_BUNKERADDITIONALS.HSFO_VALUE, 
			PC_BUNKERADDITIONALS.HSGO_VALUE,
			PC_BUNKERADDITIONALS.LSGO_VALUE,
			PC_BUNKERADDITIONALS.LSFO_VALUE,
			PC_BUNKERADDITIONALS.DEFAULT_VALUE
FROM PC_BUNKERADDITIONALS, CAL_BUNKERADDITIONALS, VESSELS
WHERE PC_BUNKERADDITIONALS.CAL_DESCRIPTION = CAL_BUNKERADDITIONALS.CAL_DESCRIPTION
AND (ISNULL(PC_BUNKERADDITIONALS.HSFO_VALUE, 0) <> 0 OR ISNULL(PC_BUNKERADDITIONALS.HSGO_VALUE, 0) <> 0 OR ISNULL(PC_BUNKERADDITIONALS.LSGO_VALUE, 0) <> 0 OR ISNULL(PC_BUNKERADDITIONALS.LSFO_VALUE, 0) <> 0)
AND CAL_BUNKERADDITIONALS.CAL_ACTIVE = 1
AND VESSELS.PC_NR  = PC_BUNKERADDITIONALS.PC_NR
AND VESSELS.VESSEL_NR = :al_vesselnr
ORDER BY CAL_BUNKERADDITIONALS.CAL_ORDER
" arguments=(("al_vesselnr", number)) )
