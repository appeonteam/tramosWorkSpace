 retrieve="  SELECT CAL_CALC.CAL_CALC_START_PORT,   
         CAL_CALC.CAL_CALC_END_PORT,   
         CAL_CALC.CAL_CALC_CREATED_BY,   
         CAL_CALC.CAL_CALC_EXPIRY_DATE,   
         CAL_CALC.CAL_CALC_VESSEL_ID,   
         CAL_CALC.CAL_CALC_STATUS,   
         CAL_CALC.CAL_CALC_ID,   
         CAL_CALC.CAL_CALC_DESCRIPTION,   
         VESSEL_NAME + CAL_VEST_TYPE_NAME + CAL_CLRK_NAME  
    FROM CAL_CALC,   
         VESSELS,   
         CAL_VEST,   
         CAL_CLAR 
   WHERE ( VESSELS.VESSEL_NR =* CAL_CALC.CAL_CALC_VESSEL_ID) and  
         ( CAL_VEST.CAL_VEST_TYPE_ID =* CAL_CALC.CAL_VEST_TYPE_ID) and  
         ( CAL_CLAR.CAL_CLRK_ID =* CAL_CALC.CAL_CLRK_ID) and  
         ( VESSELS.PC_NR in (SELECT USERS_PROFITCENTER.PC_NR FROM USERS_PROFITCENTER WHERE  USERS_PROFITCENTER.USERID = :userid ) ) and  
         ( ( CAL_CALC.CAL_CALC_STATUS > 1 ) AND  
         ( CAL_CALC.CAL_CALC_EXPIRY_DATE <= :days_date ) AND  
         ( CAL_CALC.CAL_CALC_STATUS < 4 ) )    
" update="CAL_CALC" updatewhere=0 updatekeyinplace=no arguments=(("days_date", date),("userid", string)) )
