 retrieve="  SELECT CAL_CAIO.PORT_CODE,   
         CAL_CAIO.CAL_RATY_ID,   
         CAL_CAIO.CAL_CAIO_RATE_ESTIMATED,   
         CAL_CAIO.CAL_CAIO_RATE_CALCULATED,   
         CAL_CAIO.CAL_CAIO_NOTICETIME,   
         CAL_CAIO.CAL_CAIO_NUMBER_OF_UNITS,   
         CAL_CAIO.CAL_CAIO_DESPATCH,   
         CAL_CAIO.CAL_CAIO_DEMURRAGE,   
         CAL_CAIO.CAL_CAIO_TOTAL_PORT_EXPENSES,   
         CAL_CAIO.CAL_CAIO_EXPENSES,   
         CAL_CAIO.CAL_CAIO_MISC_EXPENSES,   
         CAL_CAIO.CAL_CAIO_MISC_EXPENSES_2,   
         CAL_CAIO.CAL_CAIO_LOAD_UNIT_EXPENSES,   
         CAL_CAIO.CAL_CAIO_ID,   
         CAL_CAIO.CAL_CARG_ID,   
         CAL_CAIO.CAL_CAIO_ITINERARY_NUMBER,   
         CAL_CAIO.CAL_CAIO_VIA_POINT_1,   
         CAL_CAIO.CAL_CAIO_VIA_POINT_2,   
         CAL_CAIO.CAL_CAIO_VIA_POINT_3,   
         CAL_CAIO.CAL_CAIO_DISTANCE_TO_PREVIOUS,   
         CAL_CAIO.CAL_CAIO_GEAR,   
         CAL_CAIO.CAL_CAIO_ARRIVAL,   
         CAL_CAIO.CAL_CAIO_VIA_EXPENSES_3,   
         CAL_CAIO.CAL_CAIO_VIA_EXPENSES_2,   
         CAL_CAIO.CAL_CAIO_VIA_EXPENSES_1,   
         CAL_CAIO.CAL_CAIO_DAYS_AT_CANAL,   
         CAL_CAIO.CAL_CAIO_DAYS_AT_SEA,   
         CAL_CAIO.CAL_CAIO_DAYS_CALCULATED,   
         CAL_CAIO.CAL_CAIO_LEG_SPEED,   
         CAL_CAIO.CAL_CAIO_LOAD_TERMS,   
         0 AS edit_locked,   
         0 AS locked,   
         CAL_CAIO.PURPOSE_CODE,   
         0 AS proceed_locked,   
         CAL_CAIO.CAL_CAIO_DESPATCH_LOCAL_CURR,   
         CAL_CAIO.CAL_CAIO_DEMURRAGE_LOCAL_CURR,   
         CAL_CARG.CAL_CARG_CURR_CODE,   
         CAL_CAIO.PORT_CONS_ID,   
         CAL_CAIO.SAILING_CONS_ID,
			CAL_CAIO.CAL_CAIO_DESPATCH_DEM_CURR,   
         CAL_CAIO.CAL_CAIO_DEMURRAGE_DEM_CURR,
		(CASE WHEN (SELECT COUNT(1) FROM VOYAGE_ALERTS WHERE VOYAGE_ALERTS.PORT_CODE = CAL_CAIO.PORT_CODE 
																							OR VOYAGE_ALERTS.COUNTRY_ID = (SELECT COUNTRY_ID FROM PORTS WHERE PORTS.PORT_CODE = CAL_CAIO.PORT_CODE )
						) > 0 THEN 1 ELSE 0 END) AS VOYAGE_ALERT,
         CAL_CAIO_INTERIM_PORT
    FROM CAL_CAIO,   
         CAL_CARG  
   WHERE ( CAL_CAIO.CAL_CARG_ID = CAL_CARG.CAL_CARG_ID ) and  
         ( CAL_CAIO.CAL_CARG_ID in ( :id_list ) ) AND  
         ( CAL_CAIO.CAL_CAIO_NUMBER_OF_UNITS >= 0 )    
" update="CAL_CAIO" updatewhere=0 updatekeyinplace=yes arguments=(("id_list", numberlist))  sort="cal_caio_itinerary_number A " )
