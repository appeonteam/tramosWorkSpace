 retrieve="SELECT CAL_CERP.CAL_CERP_DATE, 
CAL_CERP.CAL_CERP_DESCRIPTION, 
CAL_CERP.CHART_NR, 
CAL_CARG.CAL_CARG_ID, 
CAL_CARG.CAL_CALC_ID, 
CAL_CARG.CAL_CERP_ID, 
CAL_CARG.CAL_CARG_FREIGHT_TYPE, 
CAL_CARG.CAL_CARG_FREIGHT_RATE, 
CAL_CARG.CAL_CARG_LUMPSUM, 
CAL_CARG.CAL_CARG_WS_RATE, 
CAL_CARG.CAL_CARG_FLATRATE, 
CAL_CARG.CAL_CARG_LAYCAN_START, 
CAL_CARG.CAL_CARG_LAYCAN_END, 
CAL_CARG.CAL_CARG_TOTAL_UNITS, 
CAL_CARG.CAL_CARG_MIN_1, 
CAL_CARG.CAL_CARG_MIN_2, 
CAL_CARG.CAL_CARG_OVERAGE_1, 
CAL_CARG.CAL_CARG_OVERAGE_2, 
CAL_CARG.CAL_CARG_ADR_COMMISION, 
CAL_CARG.CAL_CARG_TEMP_COMISSION, 
CAL_CARG.CAL_CARG_ADD_DAYS_LADEN, 
CAL_CARG.CAL_CARG_ADD_DAYS_BALLAST, 
CAL_CARG.CAL_CARG_ADD_DAYS_OTHER, 
CAL_CARG.CAL_CARG_ADD_FO, 
CAL_CARG.CAL_CARG_ADD_DO, 
CAL_CARG.CAL_CARG_ADD_MGO, 
( SELECT SUM(CAL_LUMP_ADD_LUMPSUM) FROM CAL_LUMP WHERE CAL_LUMP.CAL_CARG_ID = CAL_CARG.CAL_CARG_ID) as CAL_CARG_ADD_LUMPSUM,   
CAL_CARG.CAL_CARG_ADD_EXPENSES, 
CAL_CARG.CAL_CARG_DESCRIPTION, 
CAL_CARG.CAL_CARG_STATUS, 
CAL_CARG.CAL_CARG_ORDER, 
CAL_CARG.CAL_CARG_REVERSIBLE, 
0 as CAL_CARG_ADR_COM_LUMP, 
CAL_CARG.CAL_CARG_REV_CERP, 
CAL_CARG.CAL_CARG_REV_FREIGHT, 
CAL_CARG.CAL_CARG_ADD_DAYS_TYPE, 
CAL_CARG.CAL_CARG_IDLE_DAYS, 
CAL_CARG.CAL_CARG_BUNKERING_DAYS, 
CAL_CARG.CAL_CARG_ADD_DAYS_LADEN_PCNT, 
CAL_CARG.CAL_CARG_ADD_DAYS_BALLAST_PCNT, 
CAL_CARG.CAL_CARG_ADD_DAYS_VARIO_PCNT, 
CAL_CARG.CAL_CARG_TOTAL_GROSS_FREIGHT, 
CAL_CARG.CAL_CARG_TOTAL_DEMURRAGE, 
CAL_CARG.CAL_CARG_TOTAL_DESPATCH, 
CAL_CARG.CAL_CARG_TOTAL_COMMISSION, 
CAL_CARG.LADEN_CONS_ID,
CAL_CARG.BALLAST_CONS_ID,
~" ~" as compute_0046, 
-1 as locked, 
CAL_CARG.CAL_CARG_LOCAL_FLATRATE, 
CAL_CARG.CAL_CARG_MISC_INCOME, 
CAL_CARG.CAL_CARG_ADD_DAYS_LADEN_SPD, 
CAL_CARG.CAL_CARG_ADD_DAYS_BALLAST_SPD, 
(  SELECT count(*)  
			FROM CAL_CERP CP2,   
				CHART  C
			WHERE  C.CHART_NR = CP2.CHART_NR  and  
				 CP2.CAL_CERP_ID = CAL_CERP.CAL_CERP_ID and
				 char_length (C.CHART_COMMENT) > 0 ) as COMMENT,
CAL_CARG.BUNKER_ESCALATION,
CAL_CARG.CAL_CARG_CURR_CODE,
CAL_CARG.CAL_CARG_EXRATE_USD,
CAL_CARG.CAL_CARG_LUMPSUM_LOCAL_CURR,
CAL_CARG.CAL_CARG_FREIGHT_RATE_LOCAL_CURR,
CAL_CARG.CAL_CARG_BUNKER_ESCALATION_LOCAL_CURR,
CAL_CARG.CAL_CARG_EXRATE_DATE,
CAL_CARG.FIXED_EXRATE_ENABLED,
CAL_CARG.FIXED_EXRATE,
CAL_CARG.CLAIM_CURR, 
CAL_CARG.BUNKER_CONS_ID,
CAL_CARG.IDLE_CONS_ID,
CAL_CARG.VARIOUS_CONS_ID,
CAL_CARG.CAL_CARG_ADD_LSFO,
CAL_CARG.CAL_CARG_DEM_CURR_CODE,
CAL_CARG.CAL_CARG_DEM_EXRATE_DATE,
CAL_CARG.CAL_CARG_DEM_EXRATE_USD,
CAL_CARG.CAL_CARG_SET_EX_RATE
FROM CAL_CARG, CAL_CERP 
WHERE ( CAL_CARG.CAL_CERP_ID *= CAL_CERP.CAL_CERP_ID) 
and ( ( CAL_CARG.CAL_CALC_ID = :calc_id ) ) 
ORDER BY CAL_CARG.CAL_CARG_ORDER ASC " arguments=(("calc_id", number)) )
