 retrieve="  SELECT CAL_CLMI.CLAIM_TYPE,   
         CAL_CLMI.CAL_CLMI_AMOUNT,   
         CAL_CLMI.CAL_CARG_ID,   
         CAL_CLMI.CAL_CLMI_ADDRESS_COMMISION,   
         CAL_CLMI.CAL_CLMI_BROKER_COMMISSION,   
         CAL_CLMI.CAL_CLMI_ID,   
         CAL_CLMI.CAL_CLMI_ORDER,   
         0 as locked,   
         ~"~" as dummy1,
         CLAIM_TYPES.POOL_MANAGER_COMMISSION,   
         CLAIM_TYPES.BROKER_COMMISSION  
    FROM CAL_CLMI,   
         CAL_CARG,
CLAIM_TYPES
   WHERE ( CAL_CARG.CAL_CARG_ID = CAL_CLMI.CAL_CARG_ID ) and  
         ( ( CAL_CARG.CAL_CALC_ID = :cal_calc_id ) )     and 
         ( CAL_CLMI.CLAIM_TYPE = CLAIM_TYPES.CLAIM_TYPE ) " update="CAL_CLMI" updatewhere=0 updatekeyinplace=yes arguments=(("cal_calc_id", number))  sort="claim_type A " )
