 column=(type=char(7) updatewhereclause=yes name=disb_expenses_voyage_nr dbname="VOYAGE_NR" )
 column=(type=datetime updatewhereclause=yes name=disbursements_disb_dept_dt dbname="DISB_DEPT_DT" )
 column=(type=number updatewhereclause=yes name=exp_amount_usd dbname="EXP_AMOUNT_USD" )
 column=(type=char(50) updatewhereclause=yes name=vessels_vessel_name dbname="VESSELS.VESSEL_NAME" )
 column=(type=char(35) updatewhereclause=yes name=cal_vest_type_name dbname="CAL_VEST.CAL_VEST_TYPE_NAME" )
 column=(type=number updatewhereclause=yes name=vessels_cal_sdwt dbname="VESSELS.CAL_SDWT" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 retrieve="SELECT MAINEXP.VESSEL_NR,
	MAINEXP.VOYAGE_NR,
	MAINEXP.DISB_DEPT_DT, 
	MAINEXP.EXP_AMOUNT_USD, 
	VESSELS.VESSEL_NAME, 
	CAL_VEST.CAL_VEST_TYPE_NAME,
	VESSELS.CAL_SDWT, 
	VESSELS.VESSEL_REF_NR   FROM 
(SELECT DE1.VESSEL_NR, DE1.VOYAGE_NR, D1.DISB_DEPT_DT, SUM(DE1.EXP_AMOUNT_USD)  AS EXP_AMOUNT_USD
	FROM DISB_EXPENSES DE1, 
		DISBURSEMENTS D1,
		VOYAGES, 
		POC,
		VOUCHERS  
	WHERE DE1.VESSEL_NR = POC.VESSEL_NR  and 
		DE1.VOYAGE_NR = POC.VOYAGE_NR  and 
		DE1.PORT_CODE = POC.PORT_CODE  and 
		DE1.PCN = POC.PCN and 
		DE1.VESSEL_NR = D1.VESSEL_NR  and 
		DE1.VOYAGE_NR = D1.VOYAGE_NR  and 
		DE1.PORT_CODE = D1.PORT_CODE  and 
		DE1.PCN = D1.PCN and  
		DE1.AGENT_NR = D1.AGENT_NR AND    
		VOYAGES.VESSEL_NR = DE1.VESSEL_NR and   
		VOYAGES.VOYAGE_NR = DE1.VOYAGE_NR and   
		DE1.VOUCHER_NR = VOUCHERS.VOUCHER_NR and
		POC.PURPOSE_CODE = :Purpose and 
		DE1.PORT_CODE = :Port_code and 
		VOYAGES.VOYAGE_TYPE = 1 and
		VOUCHERS.PORT_EXPENSE = 1 AND   
		D1.DISB_DEPT_DT > dateadd(year, :NrofYears * -1, getdate()) AND
		0 = (SELECT COUNT(*) FROM DISBURSEMENTS D2 WHERE D2.DISB_FINISH_DT IS NULL
					AND D2.VESSEL_NR = DE1.VESSEL_NR AND D2.VOYAGE_NR = DE1.VOYAGE_NR 
					AND D2.PORT_CODE = DE1.PORT_CODE AND D2.PCN = DE1.PCN )
GROUP BY DE1.VESSEL_NR, DE1.VOYAGE_NR, D1.DISB_DEPT_DT) MAINEXP
LEFT JOIN VESSELS ON MAINEXP.VESSEL_NR = VESSELS.VESSEL_NR
LEFT JOIN CAL_VEST ON VESSELS.CAL_VEST_TYPE_ID = CAL_VEST.CAL_VEST_TYPE_ID
where VESSELS.CAL_VEST_TYPE_ID in (:shiptype)  
" arguments=(("shiptype", numberlist),("Port_code", string),("Purpose", string),("NrofYears", number))  sort="disbursements_disb_dept_dt D " )
