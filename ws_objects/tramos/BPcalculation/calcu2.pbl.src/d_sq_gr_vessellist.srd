 retrieve="  SELECT VESSELS.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         VESSELS.PC_NR,   
         VESSELS.VESSEL_REF_NR,
			PROFIT_C.PC_NAME,
			PRIMARY_PROFITCENTER,
			VESSELS.VESSEL_ACTIVE  
    FROM VESSELS,
			PROFIT_C,   
         USERS_PROFITCENTER  
   WHERE ( VESSELS.PC_NR = USERS_PROFITCENTER.PC_NR ) and
			(PROFIT_C.PC_NR = VESSELS.PC_NR) AND
			(VESSELS.VESSEL_ACTIVE = 1) AND
			(PRIMARY_PROFITCENTER > :an_flag) and   
         ( ( USERS_PROFITCENTER.USERID = :as_userid ))  
" arguments=(("as_userid", string),("an_flag", number))  sort="vessel_name A " )
