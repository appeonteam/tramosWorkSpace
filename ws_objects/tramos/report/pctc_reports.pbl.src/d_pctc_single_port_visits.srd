 retrieve="  SELECT POC.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         POC.VOYAGE_NR,   
         POC.PORT_CODE,   
         PORTS.PORT_N,   
         BOL.GRADE_GROUP,   
         BOL.GRADE_NAME,   
         BOL.L_D,   
         BOL.BOL_QUANTITY,   
         PURPOSE.PURPOSE_DESC,   
         POC.PORT_ARR_DT,   
         POC.PORT_DEPT_DT,   
         POC.PCN,
			VESSELS.VESSEL_REF_NR  
    FROM BOL,   
         POC,   
         VESSELS,   
         PORTS,   
         PURPOSE  
   WHERE BOL.VESSEL_NR =* POC.VESSEL_NR and  
         POC.VOYAGE_NR *= BOL.VOYAGE_NR and  
         POC.PORT_CODE *= BOL.PORT_CODE and  
         POC.PCN *= BOL.PCN and  
         POC.VESSEL_NR = VESSELS.VESSEL_NR and  
         POC.PORT_CODE = PORTS.PORT_CODE and  
         PURPOSE.PURPOSE_CODE = POC.PURPOSE_CODE and  
         POC.VESSEL_NR in ( :vessels ) AND  
         ((POC.PORT_ARR_DT >= :startdate AND POC.PORT_ARR_DT < :enddate ) or
         (POC.PORT_DEPT_DT >= :startdate AND POC.PORT_DEPT_DT < :enddate )) AND			   
         POC.PORT_CODE = :portcode    
ORDER BY VESSELS.VESSEL_REF_NR ASC,   
         POC.PORT_ARR_DT ASC   
" arguments=(("vessels", numberlist),("startdate", datetime),("enddate", datetime),("portcode", string))  sort="vessels_vessel_ref_nr A poc_voyage_nr A poc_port_code A poc_pcn A " )
