 retrieve="  SELECT DISTINCT VESSELS.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         VOYAGES.VOYAGE_NR,   
         PORTS.PORT_N, 
			COUNTRY.COUNTRY_NAME,
         POC.PURPOSE_CODE,   
         PURPOSE.PURPOSE_DESC,   
         PORTS.PORT_CODE,
			BOL.GRADE_GROUP,
			BOL.GRADE_NAME,
			BOL.BOL_QUANTITY,
			BOL.L_D,
			VESSELS.VESSEL_REF_NR 
    FROM BOL,   
         VOYAGES,   
         VESSELS,   
         POC,   
         PORTS,   
			COUNTRY,
         PURPOSE
   WHERE BOL.VESSEL_NR = POC.VESSEL_NR and  
         BOL.VOYAGE_NR = POC.VOYAGE_NR and  
			BOL.PORT_CODE = POC.PORT_CODE and
			BOL.PCN = POC.PCN and
			BOL.VESSEL_NR = VOYAGES.VESSEL_NR and
         BOL.VOYAGE_NR = VOYAGES.VOYAGE_NR and 
         BOL.VESSEL_NR  = VESSELS.VESSEL_NR and  
         BOL.PORT_CODE = PORTS.PORT_CODE and 
			PORTS.COUNTRY_ID = COUNTRY.COUNTRY_ID and 
         POC.PURPOSE_CODE = PURPOSE.PURPOSE_CODE and 
         ((POC.PORT_ARR_DT >= :startdate AND POC.PORT_ARR_DT < :enddate ) or
         (POC.PORT_DEPT_DT >= :startdate AND POC.PORT_DEPT_DT < :enddate )) AND
			VESSELS.VESSEL_NR in ( :vessel_nr )  
	ORDER BY	COUNTRY.COUNTRY_NAME,
	    	PORTS.PORT_N, 
         VESSELS.VESSEL_REF_NR,   
         VOYAGES.VOYAGE_NR    
" arguments=(("vessel_nr", numberlist),("startdate", datetime),("enddate", datetime))  sort="country_country_name A ports_port_n A vessels_vessel_ref_nr A voyages_voyage_nr A " )
