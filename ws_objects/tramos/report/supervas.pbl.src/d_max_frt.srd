 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"CLAIMS~" )  TABLE(NAME=~"FREIGHT_RECEIVED~" ) COLUMN(NAME=~"CLAIMS.CHART_NR~") COLUMN(NAME=~"CLAIMS.CLAIM_AMOUNT_USD~") COMPUTE(NAME=~"frt_received = SUM(FREIGHT_RECEIVED.FREIGHT_RECEIVED)~") COLUMN(NAME=~"CLAIMS.BROKER_NR~")    JOIN (LEFT=~"CLAIMS.VESSEL_NR~"    OP =~"=~"RIGHT=~"FREIGHT_RECEIVED.VESSEL_NR~"    OUTER1 =~"CLAIMS.VESSEL_NR~" )    JOIN (LEFT=~"CLAIMS.VOYAGE_NR~"    OP =~"=~"RIGHT=~"FREIGHT_RECEIVED.VOYAGE_NR~"    OUTER1 =~"CLAIMS.VOYAGE_NR~" )    JOIN (LEFT=~"CLAIMS.CHART_NR~"    OP =~"=~"RIGHT=~"FREIGHT_RECEIVED.CHART_NR~"    OUTER1 =~"CLAIMS.CHART_NR~" )    JOIN (LEFT=~"CLAIMS.CLAIM_NR~"    OP =~"=~"RIGHT=~"FREIGHT_RECEIVED.CLAIM_NR~"    OUTER1 =~"CLAIMS.CLAIM_NR~" )WHERE(    EXP1 =~"( ( CLAIMS.VESSEL_NR~"   OP =~"=~"    EXP2 =~":vessel )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( CLAIMS.VOYAGE_NR~"   OP =~"=~"    EXP2 =~":voyage ) )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"CLAIMS.CLAIM_TYPE~"   OP =~"=~"    EXP2 =~"~~~"FRT~~~"~" )  GROUP(NAME=~"CLAIMS.VESSEL_NR~") GROUP(NAME=~"CLAIMS.VOYAGE_NR~") GROUP(NAME=~"CLAIMS.CHART_NR~") GROUP(NAME=~"CLAIMS.CLAIM_NR~")) ARG(NAME = ~"vessel~" TYPE = number)  ARG(NAME = ~"voyage~" TYPE = string) " arguments=(("vessel", number),("voyage", string))  sort="sum D " )
