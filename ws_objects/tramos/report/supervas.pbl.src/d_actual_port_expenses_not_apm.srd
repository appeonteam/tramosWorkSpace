 retrieve="SELECT PROCEED.PORT_CODE,   
         PROCEED.PCN,   
         (SELECT ISNULL(sum(DE.EXP_AMOUNT_USD),0)
			FROM DISB_EXPENSES DE, VOUCHERS V
			WHERE  DE.VOUCHER_NR = V.VOUCHER_NR AND
				PROCEED.PORT_CODE = DE.PORT_CODE and  
				PROCEED.VESSEL_NR = DE.VESSEL_NR and  
				PROCEED.VOYAGE_NR = DE.VOYAGE_NR and  
				PROCEED.PCN = DE.PCN and  
				V.PORT_EXPENSE = 1  AND  
				V.VAS_REPORT = 1 AND
				V.VOUCHER_SHOW_AS_INCOME = 0) as amount_usd,   
         PROCEED.PROC_DATE,   
         disb_finished = null,   
         calc_port_expenses = 0, 	
		POC.PURPOSE_CODE  
    FROM  PROCEED, POC
   WHERE  PROCEED.VESSEL_NR *= POC.VESSEL_NR and  
		PROCEED.VOYAGE_NR *= POC.VOYAGE_NR and 
		PROCEED.PORT_CODE *= POC.PORT_CODE and   
		PROCEED.PCN *= POC.PCN and  
		PROCEED.VESSEL_NR = :ai_vessel_nr AND  
         PROCEED.VOYAGE_NR = :as_voyage_nr AND  
         PROCEED.CANCEL <> 1   
ORDER BY PROCEED.PROC_DATE ASC " arguments=(("AI_VESSEL_NR", number),("AS_VOYAGE_NR", string)) )
