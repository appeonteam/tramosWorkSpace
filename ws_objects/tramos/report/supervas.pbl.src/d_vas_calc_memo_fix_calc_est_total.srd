					IsNull(CAL_CALC.CAL_CALC_TOTAL_ADR_COMMISSION,0) AS TOTAL_ADR_COMM,
					IsNull(CAL_CALC.CAL_CALC_GROSS_FREIGHT,0) AS GROSS_FREIGHT,   
