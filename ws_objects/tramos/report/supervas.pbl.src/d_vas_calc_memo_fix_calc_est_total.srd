					IsNull(CAL_CALC.CAL_CALC_FO_EXPENSES,0) AS BUNKER_FO,   
					IsNull(CAL_CALC.CAL_CALC_DO_EXPENSES,0) AS BUNKER_DO,   
					IsNull(CAL_CALC.CAL_CALC_MGO_EXPENSES,0) AS BUNKER_MGO,   
					IsNull(CAL_CALC.CAL_CALC_MISC_EXPENSES,0) AS MISC_EXP,   
					IsNull(CAL_CALC.CAL_CALC_MISC_INCOME,0) AS MISC_INCO,
					IsNull(CAL_CALC.CAL_CALC_PORT_EXPENSES,0) AS PORT_EXP,
					IsNull(CAL_CALC.CAL_CALC_CHANAL_EXPENSES,0) AS CHANAL_EXP,
					 IsNull(CAL_CALC.CAL_CALC_LSFO_EXPENSES,0) AS BUNKER_LSFO
FROM   CAL_CALC  
WHERE  CAL_CALC.CAL_CALC_STATUS = :li_status  AND  
