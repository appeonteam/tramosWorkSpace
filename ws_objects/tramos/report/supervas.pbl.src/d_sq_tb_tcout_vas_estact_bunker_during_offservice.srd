 retrieve="SELECT isnull((NTC_OFF_SERVICE.HFO_TON * NTC_OFF_SERVICE.HFO_PRICE),0) 
	+ isnull((NTC_OFF_SERVICE.DO_TON * NTC_OFF_SERVICE.DO_PRICE),0) 
	+ isnull((NTC_OFF_SERVICE.GO_TON * NTC_OFF_SERVICE.GO_PRICE),0) 
	+ isnull((NTC_OFF_SERVICE.LSHFO_TON * NTC_OFF_SERVICE.LSHFO_PRICE),0) AS AMOUNT,
	NTC_OFF_SERVICE.TRANS_TO_CODA
FROM NTC_OFF_SERVICE,   
	NTC_PAYMENT  
WHERE  (NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID ) 
and ( NTC_PAYMENT.CONTRACT_ID in (:contractID) ) 
AND ( NTC_OFF_SERVICE.USE_IN_VAS = 1 ) 
AND (NOT (NTC_OFF_SERVICE.START_DATE < :voyage_start AND NTC_OFF_SERVICE.END_DATE <= :voyage_start ) 
AND NOT (NTC_OFF_SERVICE.START_DATE >= :voyage_end AND NTC_OFF_SERVICE.END_DATE > :voyage_end))
" arguments=(("contractID", numberlist),("voyage_start", datetime),("voyage_end", datetime)) )
