 column=(type=char(50) updatewhereclause=yes name=vessels_vessel_name dbname="VESSELS.VESSEL_NAME" )
 column=(type=char(7) updatewhereclause=yes name=voyage_nr dbname="VOYAGES.VOYAGE_NR" )
 column=(type=char(35) updatewhereclause=yes name=office dbname="OFFICE" )
 column=(type=char(5) updatewhereclause=yes name=vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 retrieve="SELECT VO.VESSEL_NR,   
	VE.VESSEL_NAME,   
	VO.VOYAGE_NR,
	(SELECT MAX(O.OFFICE_NAME) 
			FROM CAL_CALC CA, CAL_CARG CG, CAL_CERP CP, OFFICES O 
			WHERE CA.CAL_CALC_ID = CG.CAL_CALC_ID
			AND CG.CAL_CERP_ID = CP.CAL_CERP_ID
			AND CP.CAL_CERP_OFFICE_NR = O.OFFICE_NR
			AND CA.CAL_CALC_ID = VO.CAL_CALC_ID) AS OFFICE,
	VE.VESSEL_REF_NR 
FROM VOYAGES VO,   
	VESSELS VE,
	USERS_PROFITCENTER UP 
WHERE VE.VESSEL_NR = VO.VESSEL_NR and 
	VE.PC_NR = UP.PC_NR and   
	VO.VOYAGE_TYPE = 1 and 
	VO.CAL_CALC_ID > 1 and 
	SUBSTRING(VO.VOYAGE_NR,1,2) = :year and 
	UP.USERID = :userid and
	0 = (SELECT COUNT(*) 
				FROM CLAIMS CL 
				WHERE CL.VESSEL_NR = VO.VESSEL_NR
				AND CL.VOYAGE_NR = VO.VOYAGE_NR
				AND CL.CLAIM_TYPE = ~"FRT~" )
ORDER BY VO.VESSEL_NR, VO.VOYAGE_NR
             
" arguments=(("year", string),("userid", string))  sort="vessel_ref_nr A voyage_nr A " )
