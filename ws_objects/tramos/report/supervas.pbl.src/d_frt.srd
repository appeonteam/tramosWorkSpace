 retrieve="  SELECT distinct IsNull(CLAIMS.CLAIM_AMOUNT_USD,0),   
         CLAIMS.CHART_NR,   
         ( SELECT IsNull(sum(FREIGHT_RECEIVED.FREIGHT_RECEIVED),0) 
					FROM FREIGHT_RECEIVED 
					WHERE CLAIMS.VESSEL_NR = FREIGHT_RECEIVED.VESSEL_NR and  
         			CLAIMS.VOYAGE_NR = FREIGHT_RECEIVED.VOYAGE_NR and  
         			CLAIMS.CHART_NR = FREIGHT_RECEIVED.CHART_NR and  
         			CLAIMS.CLAIM_NR = FREIGHT_RECEIVED.CLAIM_NR and 
				FREIGHT_RECEIVED.TRANS_CODE = ~"R~"),   
         ( SELECT isNull(sum(CLAIM_TRANSACTION.C_TRANS_AMOUNT_USD),0)  
					FROM CLAIM_TRANSACTION
					WHERE CLAIMS.CHART_NR = CLAIM_TRANSACTION.CHART_NR and  
         			CLAIMS.VESSEL_NR = CLAIM_TRANSACTION.VESSEL_NR and  
         			CLAIMS.VOYAGE_NR = CLAIM_TRANSACTION.VOYAGE_NR and  
         			CLAIMS.CLAIM_NR = CLAIM_TRANSACTION.CLAIM_NR and 
			         CLAIM_TRANSACTION.C_TRANS_CODE in (~"A~", ~"W~")) ,
		(SELECT isnull(sum(FREIGHT_ADVANCED_RECIEVED.AFC_RECIEVED),0)
			 FROM FREIGHT_ADVANCED_RECIEVED   
			WHERE  FREIGHT_ADVANCED_RECIEVED.VESSEL_NR = CLAIMS.VESSEL_NR AND 
						FREIGHT_ADVANCED_RECIEVED.VOYAGE_NR = CLAIMS.VOYAGE_NR  AND 
						FREIGHT_ADVANCED_RECIEVED.CHART_NR = CLAIMS.CHART_NR AND 
						FREIGHT_ADVANCED_RECIEVED.CLAIM_NR = CLAIMS.CLAIM_NR AND 
						FREIGHT_ADVANCED_RECIEVED.TRANS_CODE = ~"R~")
    FROM CLAIMS   
   WHERE CLAIMS.CLAIM_TYPE = 'FRT'  AND  
         CLAIMS.VESSEL_NR = :vessel  AND  
         CLAIMS.VOYAGE_NR = :voyage    
" arguments=(("vessel", number),("voyage", string)) )
