 retrieve="  SELECT  POC.VESSEL_NR ,           
	VESSELS.VESSEL_NAME ,           
	POC.VOYAGE_NR ,           
	POC.PORT_CODE ,           
	PORTS.PORT_N ,           
	isnull(BP_DETAILS.LIFTED_HFO,0) as LIFTED_HSFO ,           
	isnull(BP_DETAILS.PRICE_HFO,0) as PRICE_HSFO ,           
	POC.PORT_ARR_DT,
	VESSELS.VESSEL_REF_NR,
	isnull(BP_DETAILS.LIFTED_LSHFO,0) as LIFTED_LSFO ,           
	isnull(BP_DETAILS.PRICE_LSHFO,0) as PRICE_LSFO            
FROM 	BP_DETAILS ,           
	POC ,           
	VESSELS ,           
	PORTS ,           
	VOYAGES     
WHERE POC.VESSEL_NR = BP_DETAILS.VESSEL_NR 
and POC.VOYAGE_NR = BP_DETAILS.VOYAGE_NR  
and POC.PORT_CODE = BP_DETAILS.PORT_CODE  
and POC.PCN = BP_DETAILS.PCN  
and POC.VESSEL_NR = VESSELS.VESSEL_NR 
and POC.PORT_CODE = PORTS.PORT_CODE 
and VOYAGES.VESSEL_NR = POC.VESSEL_NR 
and VOYAGES.VOYAGE_NR = POC.VOYAGE_NR  
and VOYAGES.VOYAGE_TYPE <> 2 
And VESSELS.PC_NR in ( :profitcenter ) 
And POC.PORT_ARR_DT >= :startdate 
And POC.PORT_DEPT_DT < :enddate 
and ((isnull(BP_DETAILS.PRICE_HFO,0) <> 0 and isnull(BP_DETAILS.LIFTED_HFO,0) <> 0)
or (isnull(BP_DETAILS.PRICE_LSHFO,0) <> 0 and isnull(BP_DETAILS.LIFTED_LSHFO,0) <> 0))   " arguments=(("profitcenter", numberlist),("startdate", datetime),("enddate", datetime))  sort="poc_vessel_nr A poc_port_arr_dt A " )
