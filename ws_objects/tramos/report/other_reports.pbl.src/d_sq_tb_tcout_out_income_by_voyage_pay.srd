 column=(type=decimal(0) updatewhereclause=yes identity=yes name=ntc_tc_contract_contract_id dbname="NTC_TC_CONTRACT.CONTRACT_ID" )
 retrieve="  SELECT NTC_TC_CONTRACT.VESSEL_NR,   
         NTC_PAYMENT_DETAIL.PERIODE_START,   
         NTC_PAYMENT_DETAIL.PERIODE_END,   
         NTC_PAYMENT_DETAIL.MONTHLY_RATE,   
         NTC_PAYMENT_DETAIL.QUANTITY,   
         NTC_PAYMENT_DETAIL.RATE  ,
 NTC_TC_CONTRACT.CONTRACT_ID
    FROM NTC_PAYMENT,   
         NTC_PAYMENT_DETAIL,   
         NTC_TC_CONTRACT  
   WHERE ( NTC_PAYMENT_DETAIL.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  NTC_TC_CONTRACT.VESSEL_NR=:VESSEL and 
          ( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) AND  
        ( ( NTC_PAYMENT_DETAIL.PERIODE_START >= :STARTDATE   and NTC_PAYMENT_DETAIL.PERIODE_START <= :ENDDATE) or
          ( NTC_PAYMENT_DETAIL.PERIODE_END >= :STARTDATE and  NTC_PAYMENT_DETAIL.PERIODE_END<= :ENDDATE ) or      
          ( NTC_PAYMENT_DETAIL.PERIODE_START <= :STARTDATE and  NTC_PAYMENT_DETAIL.PERIODE_END>= :ENDDATE ) )" arguments=(("VESSEL", number),("STARTDATE", datetime),("ENDDATE", datetime))  sort="periode_start A " )
