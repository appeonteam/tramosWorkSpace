 retrieve="SELECT 
		DEPARTMENT_CONFIG.DEPARTMENT_NAME AS DEPARTMENT_COLUMN,   
		DEPARTMENT_CONFIG.DEPARTMENT_SORT,
		OWNER_POC.DEPARTMENT_NAME,
		OWNER_POC.OWNER_DEPARTMENT_ID,
		CASE WHEN OWNER_POC.ENABLED = 1 THEN 'Yes' ELSE '' END AS DEPARTMENT_VALUE,
		OWNER_POC.PC_NR,
		OWNER_POC.PC_NAME,
		OWNER_POC.VESSEL_NR,
		OWNER_POC.VESSEL_REF_NR,
		OWNER_POC.VESSEL_NAME,
		OWNER_POC.VOYAGE_NR,
		OWNER_POC.PORT_NAME,
		OWNER_POC.PORT_ARR_DT,
		OWNER_POC.PORT_DEPT_DT,
		OWNER_POC.Status
FROM	DEPARTMENT_CONFIG LEFT OUTER JOIN   
	(SELECT	V.PC_NR,
				PC.PC_NAME,
				PA.VESSEL_NR,
				V.VESSEL_REF_NR,
				V.VESSEL_NAME,
				PA.VOYAGE_NR,   
				PORTS.PORT_N AS PORT_NAME,   
				PA.PCN,
				PA.PORT_ARR_DT,
				PA.PORT_DEPT_DT,
				'Actual' AS Status,
				DEPT.OWNER_DEPARTMENT_ID,
				PA.PORT_CODE,
				CF.DEPARTMENT_NAME,
				DEPT.ENABLED
	FROM		POC PA LEFT OUTER JOIN PORTS ON PORTS.PORT_CODE = PA.PORT_CODE,       	
				OWNER_MATTERS_DEPARTMENT PSM,
				DEPARTMENT DEPT,
				DEPARTMENT_CONFIG CF,
				VESSELS V LEFT OUTER JOIN PROFIT_C PC ON PC.PC_NR = V.PC_NR  
	WHERE		PA.VESSEL_NR = PSM.VESSEL_NR AND 
				PA.VOYAGE_NR = PSM.VOYAGE_NR AND 
				PA.PORT_CODE = PSM.PORT_CODE AND
				PA.PCN = PSM.PCN				  AND
				PSM.OWNER_DEPARTMENT_ID = DEPT.OWNER_DEPARTMENT_ID AND
				DEPT.DEPARTMENT_NAME = CF.DEPARTMENT_NAME AND
				CF.ENABLED = 1 AND
				PA.VESSEL_NR = V.VESSEL_NR AND
				DATEDIFF(Second, :from_date, PA.PORT_ARR_DT) >= 0 AND
				DATEDIFF(Second, :to_date, PA.PORT_ARR_DT) <= 0 AND V.VESSEL_NR IN (:aan_vessel_nr)
	UNION ALL
 	SELECT	V2.PC_NR,
				PC2.PC_NAME,
				PE.VESSEL_NR,
				V2.VESSEL_REF_NR,
				V2.VESSEL_NAME,
				PE.VOYAGE_NR,   
				PORTS2.PORT_N AS PORT_NAME,   
				PE.PCN,
				PE.PORT_ARR_DT,
				PE.PORT_DEPT_DT,
				'Estimated' AS STATUS,
				DEPT2.OWNER_DEPARTMENT_ID,
				PE.PORT_CODE,
				CF2.DEPARTMENT_NAME,
				DEPT2.ENABLED
	FROM		POC_EST PE LEFT OUTER JOIN PORTS PORTS2 ON PORTS2.PORT_CODE = PE.PORT_CODE,       	
				OWNER_MATTERS_DEPARTMENT PSM2,
				DEPARTMENT DEPT2,
				DEPARTMENT_CONFIG CF2,
				VESSELS V2 LEFT OUTER JOIN PROFIT_C PC2 ON PC2.PC_NR = V2.PC_NR  
	WHERE		PE.VESSEL_NR = PSM2.VESSEL_NR AND 
				PE.VOYAGE_NR = PSM2.VOYAGE_NR AND 
				PE.PORT_CODE = PSM2.PORT_CODE AND
				PE.PCN = PSM2.PCN				  AND
				PSM2.OWNER_DEPARTMENT_ID = DEPT2.OWNER_DEPARTMENT_ID AND
				DEPT2.DEPARTMENT_NAME = CF2.DEPARTMENT_NAME AND
				CF2.ENABLED = 1 AND
				PE.VESSEL_NR = V2.VESSEL_NR AND
				DATEDIFF(Second, :from_date, PE.PORT_ARR_DT) >= 0 AND
				DATEDIFF(Second, :to_date, PE.PORT_ARR_DT) <= 0 AND V2.VESSEL_NR IN (:aan_vessel_nr)
			 
  ) OWNER_POC ON DEPARTMENT_CONFIG.DEPARTMENT_NAME = OWNER_POC.DEPARTMENT_NAME
	WHERE DEPARTMENT_CONFIG.ENABLED = 1 AND EXISTS(SELECT 1 from DEPARTMENT D WHERE OWNER_POC.OWNER_DEPARTMENT_ID = D.OWNER_DEPARTMENT_ID AND D.ENABLED = 1)
	ORDER BY DEPARTMENT_CONFIG.DEPARTMENT_SORT
" arguments=(("from_date", datetime),("to_date", datetime),("aan_vessel_nr", numberlist))  sort="pc_name A pc_nr A vessel_nr A vessel_ref_nr A vessel_name A voyage_nr A port_name A port_arr_dt A port_dept_dt A status A " )
