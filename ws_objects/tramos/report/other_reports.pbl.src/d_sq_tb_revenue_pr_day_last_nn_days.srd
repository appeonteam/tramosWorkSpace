table(column=(type=number updatewhereclause=yes name=demurrage dbname="DEMURRAGE" )
 column=(type=number updatewhereclause=yes name=freight dbname="FREIGHT" )
 column=(type=decimal(8) updatewhereclause=yes name=tchire dbname="TCHIRE" )
 column=(type=long updatewhereclause=yes name=pc_nr dbname="VESSELS.PC_NR" dbalias="V1.PC_NR" )
 retrieve="SELECT sum(CL1.CLAIM_AMOUNT_USD) AS DEMURRAGE ,   
	0.00 AS FREIGHT,
	0.00 AS TCHIRE,
	V1.PC_NR
FROM CLAIMS CL1,
	VESSELS V1
WHERE CL1.VESSEL_NR = V1.VESSEL_NR  and
	CL1.CLAIM_TYPE <> 'FRT'  AND
	CL1.CLAIM_AMOUNT_USD > 0   AND
	CL1.DISCHARGE_DATE < getdate() AND
	datediff(dd, CL1.DISCHARGE_DATE, getdate()) <= :revenue_days
GROUP BY V1.PC_NR   
UNION
SELECT 0.00 AS DEMURRAGE ,   
	sum(CL2.CLAIM_AMOUNT_USD + (SELECT ISNULL(sum(ISNULL(FR.FREIGHT_RECEIVED,0)),0) 
											FROM FREIGHT_RECEIVED FR 
											WHERE CL2.VESSEL_NR = FR.VESSEL_NR 
												AND CL2.VOYAGE_NR = FR.VOYAGE_NR 
												AND CL2.CHART_NR = FR.CHART_NR 
												AND  CL2.CLAIM_NR = FR.CLAIM_NR)) AS FREIGHT,
	0.00 AS TCHIRE,
	V2.PC_NR
FROM CLAIMS CL2,
	VESSELS V2
WHERE CL2.VESSEL_NR = V2.VESSEL_NR  and
	CL2.CLAIM_TYPE = 'FRT'  AND
	CL2.DISCHARGE_DATE < getdate() AND
	datediff(dd, CL2.DISCHARGE_DATE, getdate()) <= :revenue_days
GROUP BY V2.PC_NR   
UNION
SELECT 0.00 AS DEMURRAGE,
	0.00 AS FREIGHT,
	sum(PD.QUANTITY * PD.RATE ) AS TCHIRE,
	V3.PC_NR
FROM NTC_PAYMENT NTCP,   
	NTC_TC_CONTRACT NTCC, 
	NTC_PAYMENT_DETAIL PD,  
	VESSELS V3 
WHERE NTCC.CONTRACT_ID = NTCP.CONTRACT_ID and
	PD.PAYMENT_ID = NTCP.PAYMENT_ID AND  
	V3.VESSEL_NR = NTCC.VESSEL_NR and  
	NTCC.TC_HIRE_IN = 0 AND  
	NTCP.EST_DUE_DATE <= getdate() and
	datediff(dd, NTCP.EST_DUE_DATE, getdate()) < :revenue_days
GROUP BY V3.PC_NR

" arguments=(("revenue_days", number))  sort="pc_nr A " )
