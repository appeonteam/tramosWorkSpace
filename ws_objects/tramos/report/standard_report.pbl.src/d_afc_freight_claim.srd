 column=(type=number updatewhereclause=yes name=afc_bol_quantity dbname="FREIGHT_ADVANCED.AFC_BOL_QUANTITY" )
 column=(type=decimal(0) updatewhereclause=yes name=afc_ws dbname="compute_0003" )
