table(column=(type=long updatewhereclause=yes name=vessels_pc_nr dbname="VESSELS.PC_NR" dbalias=".PC_NR" )
 column=(type=char(35) updatewhereclause=yes name=profit_c_pc_name dbname="PROFIT_C.PC_NAME" dbalias=".PC_NAME" )
 column=(type=long updatewhereclause=yes name=ntc_tc_contract_vessel_nr dbname="NTC_TC_CONTRACT.VESSEL_NR" dbalias=".VESSEL_NR" )
 column=(type=char(50) updatewhereclause=yes name=vessels_vessel_name dbname="VESSELS.VESSEL_NAME" dbalias=".VESSEL_NAME" )
 column=(type=char(70) updatewhereclause=yes name=chart_chart_n_1 dbname="CHART.CHART_N_1" dbalias=".CHART_N_1" )
 column=(type=datetime updatewhereclause=yes name=ntc_payment_detail_periode_start dbname="NTC_PAYMENT_DETAIL.PERIODE_START" dbalias=".PERIODE_START" )
 column=(type=datetime updatewhereclause=yes name=ntc_payment_detail_periode_end dbname="NTC_PAYMENT_DETAIL.PERIODE_END" dbalias=".PERIODE_END" )
 column=(type=decimal(15) updatewhereclause=yes name=hire_rate dbname="HIRE_RATE" )
 column=(type=decimal(4) updatewhereclause=yes name=offhire_rate dbname="OFFHIRE_RATE" )
 column=(type=char(35) updatewhereclause=yes name=ccs_chgp_name dbname="CCS_CHGP.CCS_CHGP_NAME" dbalias=".CCS_CHGP_NAME" )
 column=(type=char(35) updatewhereclause=yes name=chart_chart_c dbname="CHART.CHART_C" dbalias=".CHART_C" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" dbalias=".VESSEL_REF_NR" )
 retrieve="  SELECT VESSELS.PC_NR,   
         PROFIT_C.PC_NAME,   
         NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         CHART.CHART_N_1,   
         NTC_PAYMENT_DETAIL.PERIODE_START,   
         NTC_PAYMENT_DETAIL.PERIODE_END,   
			case NTC_PAYMENT_DETAIL.MONTHLY_RATE 
				when 1 then (NTC_PAYMENT_DETAIL.RATE / datediff(dd, NTC_PAYMENT_DETAIL.PERIODE_START,NTC_PAYMENT_DETAIL.PERIODE_END))
				else NTC_PAYMENT_DETAIL.RATE END as HIRE_RATE,
			0.0000 as OFFHIRE_RATE, 
	  CCS_CHGP.CCS_CHGP_NAME,
	  CHART.CHART_C,
			VESSELS.VESSEL_REF_NR
    FROM CHART,
			CCS_CHGP,   
         NTC_PAYMENT,   
         NTC_PAYMENT_DETAIL,   
         NTC_TC_CONTRACT,   
         PROFIT_C,   
         VESSELS  
   WHERE ( NTC_PAYMENT_DETAIL.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( NTC_TC_CONTRACT.CHART_NR = CHART.CHART_NR ) and
			( CCS_CHGP.CCS_CHGP_PK = CHART.CCS_CHGP_PK) and
         ( VESSELS.PC_NR = PROFIT_C.PC_NR ) and  
         ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and
         ( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) and
			( VESSELS.PC_NR IN (:pc_no)) and
			(VESSELS.CAL_VEST_TYPE_ID in ( :shiptype )) and
			( VESSELS.VESSEL_NR IN (:vessels)) and
			( CCS_CHGP.CCS_CHGP_PK IN (:chart_grp)) and
			( NTC_TC_CONTRACT.CHART_NR >= :chart_min ) and
			( NTC_TC_CONTRACT.CHART_NR <= :chart_max ) and
			(not ( NTC_PAYMENT_DETAIL.PERIODE_START < :startdate and NTC_PAYMENT_DETAIL.PERIODE_END <= :startdate) and
			not ( NTC_PAYMENT_DETAIL.PERIODE_START >= :enddate and NTC_PAYMENT_DETAIL.PERIODE_END > :enddate)) 
   
UNION
  SELECT VESSELS.PC_NR,   
         PROFIT_C.PC_NAME,   
         NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         CHART.CHART_N_1,   
         NTC_OFF_SERVICE_DETAIL.START_DATE,   
         NTC_OFF_SERVICE_DETAIL.END_DATE,   
			0.0000 as HIRE_RATE,
			NTC_OFF_SERVICE_DETAIL.RATE AS OFFHIRE_RATE, 
	  CCS_CHGP.CCS_CHGP_NAME,
	  CHART.CHART_C,
			VESSELS.VESSEL_REF_NR
    FROM CHART,
			CCS_CHGP,   
         NTC_PAYMENT,   
			NTC_OFF_SERVICE, 
         NTC_OFF_SERVICE_DETAIL,   
         NTC_TC_CONTRACT,   
         PROFIT_C,   
         VESSELS  
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID) and
			( NTC_OFF_SERVICE_DETAIL.OFF_SERVICE_ID = NTC_OFF_SERVICE.OFF_SERVICE_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( NTC_TC_CONTRACT.CHART_NR = CHART.CHART_NR ) and
			( CCS_CHGP.CCS_CHGP_PK = CHART.CCS_CHGP_PK) and
         ( VESSELS.PC_NR = PROFIT_C.PC_NR ) and  
         ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and
         ( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) and
			( VESSELS.PC_NR IN (:pc_no)) and
			( VESSELS.CAL_VEST_TYPE_ID in ( :shiptype )) and
			( VESSELS.VESSEL_NR IN (:vessels)) and
			( CCS_CHGP.CCS_CHGP_PK IN (:chart_grp)) and
			( NTC_TC_CONTRACT.CHART_NR >= :chart_min ) and
			( NTC_TC_CONTRACT.CHART_NR <= :chart_max ) and
			( NTC_OFF_SERVICE_DETAIL.DAYS > 0) and
			(not ( NTC_OFF_SERVICE_DETAIL.START_DATE < :startdate and NTC_OFF_SERVICE_DETAIL.END_DATE <= :startdate) and
			not ( NTC_OFF_SERVICE_DETAIL.START_DATE >= :enddate and NTC_OFF_SERVICE_DETAIL.END_DATE > :enddate)) 
UNION
  SELECT VESSELS.PC_NR,   
         PROFIT_C.PC_NAME,   
         NTC_TC_CONTRACT.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         CHART.CHART_N_1,   
         NTC_OFF_SERVICE_DETAIL.START_DATE,   
         NTC_OFF_SERVICE_DETAIL.END_DATE,   
			0.0000 as HIRE_RATE,
			NTC_OFF_SERVICE_DETAIL.RATE * -1 AS OFFHIRE_RATE, 
	  CCS_CHGP.CCS_CHGP_NAME,
	  CHART.CHART_C,
			VESSELS.VESSEL_REF_NR
    FROM CHART,
			CCS_CHGP,   
         NTC_PAYMENT,   
			NTC_OFF_SERVICE, 
         NTC_OFF_SERVICE_DETAIL,   
         NTC_TC_CONTRACT,   
         PROFIT_C,   
         VESSELS  
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID) and
			( NTC_OFF_SERVICE_DETAIL.OFF_SERVICE_ID = NTC_OFF_SERVICE.OFF_SERVICE_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( NTC_TC_CONTRACT.CHART_NR = CHART.CHART_NR ) and
			( CCS_CHGP.CCS_CHGP_PK = CHART.CCS_CHGP_PK) and
         ( VESSELS.PC_NR = PROFIT_C.PC_NR ) and  
         ( VESSELS.VESSEL_NR = NTC_TC_CONTRACT.VESSEL_NR ) and
         ( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) and
			( VESSELS.PC_NR IN (:pc_no)) and
			( VESSELS.CAL_VEST_TYPE_ID in ( :shiptype )) and
			( VESSELS.VESSEL_NR IN (:vessels)) and
			( CCS_CHGP.CCS_CHGP_PK IN (:chart_grp)) and
			( NTC_TC_CONTRACT.CHART_NR >= :chart_min ) and
			( NTC_TC_CONTRACT.CHART_NR <= :chart_max ) and
			( NTC_OFF_SERVICE_DETAIL.DAYS < 0) and
			(not ( NTC_OFF_SERVICE_DETAIL.START_DATE < :startdate and NTC_OFF_SERVICE_DETAIL.END_DATE <= :startdate) and
			not ( NTC_OFF_SERVICE_DETAIL.START_DATE >= :enddate and NTC_OFF_SERVICE_DETAIL.END_DATE > :enddate)) 
  
" arguments=(("pc_no", numberlist),("shiptype", numberlist),("vessels", numberlist),("chart_grp", numberlist),("chart_min", number),("chart_max", number),("startdate", date),("enddate", date))  sort="vessels_pc_nr A vessels_vessel_ref_nr A ntc_payment_detail_periode_start A " )
