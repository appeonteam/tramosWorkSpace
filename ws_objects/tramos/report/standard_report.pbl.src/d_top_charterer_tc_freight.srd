table(column=(type=decimal(15) updatewhereclause=yes name=hire_rate dbname="HIRE_RATE" )
 column=(type=decimal(4) updatewhereclause=yes name=offhire_rate dbname="OFFHIRE_RATE" )
 column=(type=datetime updatewhereclause=yes name=ntc_payment_detail_periode_start dbname="NTC_PAYMENT_DETAIL.PERIODE_START" )
 column=(type=datetime updatewhereclause=yes name=ntc_payment_detail_periode_end dbname="NTC_PAYMENT_DETAIL.PERIODE_END" )
 retrieve="SELECT case NTC_PAYMENT_DETAIL.MONTHLY_RATE 
				when 1 then (NTC_PAYMENT_DETAIL.RATE / datediff(dd, NTC_PAYMENT_DETAIL.PERIODE_START,NTC_PAYMENT_DETAIL.PERIODE_END))
				else NTC_PAYMENT_DETAIL.RATE END as HIRE_RATE,
				0.0000 as OFFHIRE_RATE,
				NTC_PAYMENT_DETAIL.PERIODE_START,
				NTC_PAYMENT_DETAIL.PERIODE_END
			
FROM NTC_TC_CONTRACT, NTC_PAYMENT, NTC_PAYMENT_DETAIL   
WHERE 	( NTC_PAYMENT_DETAIL.PAYMENT_ID = NTC_PAYMENT.PAYMENT_ID ) AND	
		   ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) AND 
			(NTC_TC_CONTRACT.VESSEL_NR = :al_vessel ) AND  
			( NTC_TC_CONTRACT.CHART_NR = :al_charter ) AND
			( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) AND
			( NTC_PAYMENT.PAYMENT_STATUS > 3 ) AND
			(not ( NTC_PAYMENT_DETAIL.PERIODE_START < :adt_start and NTC_PAYMENT_DETAIL.PERIODE_END <= :adt_start) and
			not ( NTC_PAYMENT_DETAIL.PERIODE_START >= :adt_end and NTC_PAYMENT_DETAIL.PERIODE_END > :adt_end)) 
UNION
  SELECT 0.0000 as HIRE_RATE,
			NTC_OFF_SERVICE_DETAIL.RATE AS OFFHIRE_RATE,
         NTC_OFF_SERVICE_DETAIL.START_DATE,   
         NTC_OFF_SERVICE_DETAIL.END_DATE   
    FROM NTC_PAYMENT,   
			NTC_OFF_SERVICE, 
         NTC_OFF_SERVICE_DETAIL,   
         NTC_TC_CONTRACT
   WHERE ( NTC_PAYMENT.PAYMENT_ID = NTC_OFF_SERVICE.PAYMENT_ID) and
			( NTC_OFF_SERVICE_DETAIL.OFF_SERVICE_ID = NTC_OFF_SERVICE.OFF_SERVICE_ID ) and  
         ( NTC_TC_CONTRACT.CONTRACT_ID = NTC_PAYMENT.CONTRACT_ID ) and  
         ( NTC_TC_CONTRACT.CHART_NR = :al_charter ) and
			(NTC_TC_CONTRACT.VESSEL_NR = :al_vessel ) AND  
         ( NTC_TC_CONTRACT.TC_HIRE_IN = 0 ) and
			(not ( NTC_OFF_SERVICE_DETAIL.START_DATE < :adt_start and NTC_OFF_SERVICE_DETAIL.END_DATE <= :adt_start) and
			not ( NTC_OFF_SERVICE_DETAIL.START_DATE >= :adt_end and NTC_OFF_SERVICE_DETAIL.END_DATE > :adt_end)) 
  
" arguments=(("al_vessel", number),("al_charter", number),("adt_start", datetime),("adt_end", datetime)) )
