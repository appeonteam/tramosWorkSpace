 column=(type=char(7) updatewhereclause=yes name=idle_days_voyage_nr dbname="IDLE_DAYS.VOYAGE_NR" )
 column=(type=datetime updatewhereclause=yes name=idle_days_idle_start dbname="IDLE_DAYS.IDLE_START" )
 column=(type=datetime updatewhereclause=yes name=idle_days_idle_end dbname="IDLE_DAYS.IDLE_END" )
 column=(type=decimal(7) updatewhereclause=yes name=idle_days dbname="IDLE_DAYS" )
 column=(type=char(5) updatewhereclause=yes name=vessels_vessel_ref_nr dbname="VESSELS.VESSEL_REF_NR" )
 column=(type=decimal(7) updatewhereclause=yes name=report_days dbname="REPORT_DAYS" )
 column=(type=decimal(4) updatewhereclause=yes name=offservice_days dbname="OFFSERVICE_DAYS" )
 retrieve="  SELECT PROFIT_C.PC_NAME,   
         VESSELS.VESSEL_NR,   
         VESSELS.VESSEL_NAME,   
         IDLE_DAYS.VOYAGE_NR,   
         IDLE_DAYS.IDLE_START,   
         IDLE_DAYS.IDLE_END,   
         IDLE_DAYS.IDLE_TIME_DAYS + IDLE_DAYS.IDLE_TIME_HOURS/24.00 + IDLE_DAYS.IDLE_TIME_MINUTES/1440.00 AS IDLE_DAYS,
			VESSELS.VESSEL_REF_NR,
			DATEDIFF(MI, :start_date, :end_date)/1440.00 as REPORT_DAYS,
			0.0000 AS OFFSERVICE_DAYS
	FROM IDLE_DAYS,   
         VESSELS,   
         PROFIT_C
   WHERE IDLE_DAYS.VESSEL_NR = VESSELS.VESSEL_NR and  
         PROFIT_C.PC_NR = VESSELS.PC_NR  and  
         VESSELS.CAL_VEST_TYPE_ID in ( :shiptype ) AND
			VESSELS.VESSEL_NR in ( :vessel_nr ) AND
         VESSELS.PC_NR in ( :pc_nr ) AND  
         ( ( IDLE_DAYS.IDLE_START <= :start_date AND IDLE_DAYS.IDLE_END > :start_date ) OR 
			( IDLE_DAYS.IDLE_START > :start_date AND IDLE_DAYS.IDLE_END < :end_date ) OR
         ( IDLE_DAYS.IDLE_START < :end_date AND IDLE_DAYS.IDLE_END >= :end_date )) AND
			IDLE_DAYS.IDLE_START <> Null AND
			IDLE_DAYS.IDLE_END <> Null 
" arguments=(("pc_nr", numberlist),("shiptype", numberlist),("vessel_nr", numberlist),("start_date", date),("end_date", date))  sort="profit_c_pc_name A vessels_vessel_ref_nr A idle_days_voyage_nr A idle_days_idle_start A " )
