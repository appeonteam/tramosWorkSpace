 retrieve="select NOR.OPEN_DATE,
	CAL_CLAR.CAL_CLRK_NAME, 
	PF_COMPANY.COMPANY,
	PF_FIXTURE_TRADE.NAME,
	PF_FIXTURE_CARGO.NAME,
	PORTS.PORT_N,
	CAL_CLAR.NEXT_OPEN_DATE AS DATE
from PF_NEXT_OPEN_REPORT NOR,
CAL_CLAR, 
PF_COMPANY, 
PF_FIXTURE, 
PF_FIXTURE_TRADE, 
PF_FIXTURE_CARGO, 
PORTS
where  NOR.NEXT_CONFIG_ID = :config_id
AND CAL_CLAR.COMPETITOR = 1
AND NOR.CAL_CLRK_ID = CAL_CLAR.CAL_CLRK_ID
and PF_FIXTURE.VESSELID_WEB = CAL_CLAR.CAL_CLRK_ID
and PF_FIXTURE.TRADEID = PF_FIXTURE_TRADE.TRADEID
and PORTS.PORT_CODE = CAL_CLAR.NEXT_OPEN_PORT
and PF_FIXTURE.CARGOID *= PF_FIXTURE_CARGO.CARGOID
and PF_COMPANY.COMPANYID =* PF_FIXTURE.OPERATORID
and PF_FIXTURE.FIXTUREREPORTED = (select max(F1.FIXTUREREPORTED) 
										from PF_FIXTURE F1 
										where F1.VESSELID_WEB = CAL_CLAR.CAL_CLRK_ID)

union
  SELECT case :config_label when ~"AG~" then PF_POSITION.AREA1 when ~"WAF~" then PF_POSITION.AREA2 end,   
         VESSELS.VESSEL_NAME,   
         PF_COMPANY.COMPANY,   
         ~"n/a~",   
         PF_POSITION.LASTCARGO,   
         PF_POSITION.OPENAREA,   
         PF_POSITION.OPENDATE  
    FROM PF_POSITION,   
         VESSELS,   
         PF_COMPANY  
   WHERE ( VESSELS.VESSEL_NR = PF_POSITION.VESSELID ) and  
         ( PF_POSITION.COMPANYID = PF_COMPANY.COMPANYID ) 
	and PF_POSITION.PCGROUP_ID = :pcgroup
 

