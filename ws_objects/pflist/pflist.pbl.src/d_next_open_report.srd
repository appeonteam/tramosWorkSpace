 column=(type=char(50) updatewhereclause=yes name=pf_company_company dbname="PF_COMPANY.COMPANY" dbalias=".COMPANY" )
 column=(type=char(50) updatewhereclause=yes name=pf_fixture_trade_name dbname="PF_FIXTURE_TRADE.NAME" dbalias=".NAME" )
 column=(type=char(180) updatewhereclause=yes name=pf_fixture_cargo_name dbname="PF_FIXTURE_CARGO.NAME" dbalias=".NAME" )
 column=(type=char(180) updatewhereclause=yes name=ports_port_n dbname="PORTS.PORT_N" dbalias=".PORT_N" )
