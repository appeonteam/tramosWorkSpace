 retrieve=" SELECT 
		
			PF_COMPANY.COMPANY,   
			PF_COMPANY.COMPANYID,
			PF_COMPANY.DEACTIVATED,
			
			COUNTRY.COUNTRY_NAME,   
			PF_COMPANYTYPE.TYPE,   
			
			PF_COMPANYCONTACTS.CONTACTSID,
			PF_COMPANYCONTACTS.FULLNAME,   
			PF_COMPANYCONTACTS.EMAIL,
			PF_COMPANYCONTACTS.MAILLIST,
			PF_COMPANYCONTACTS.STAFFRESPONSIBILITY,
			PF_COMPANYCONTACTS.DEACTIVATED,
			PF_COMPANYCONTACTS.IMO_CHECK  

    FROM PF_COMPANYCONTACTS, PF_COMPANY, COUNTRY,  
					//RIGHT OUTER JOIN PF_COMPANY ON PF_COMPANYCONTACTS.COMPANYID = PF_COMPANY.COMPANYID
   					//LEFT JOIN COUNTRY ON PF_COMPANY.COUNTRYID = COUNTRY.COUNTRY_ID, 
         PF_COMPANYTYPE

   WHERE 
		PF_COMPANYCONTACTS.COMPANYID =* PF_COMPANY.COMPANYID and
		PF_COMPANY.COUNTRYID = COUNTRY.COUNTRY_ID and
        ( PF_COMPANYTYPE.TYPEID = PF_COMPANY.TYPEID ) and  
         ( PF_COMPANY.PCGROUP_ID = :pcgroup )  AND
		  PF_COMPANYCONTACTS.FULLNAME <> ~"login_info~"

	ORDER BY PF_COMPANY.COMPANY, PF_COMPANYCONTACTS.FULLNAME



" update="PF_COMPANYCONTACTS" updatewhere=0 updatekeyinplace=yes arguments=(("pcgroup", number))  sort="pf_company_company A pf_companycontacts_fullname A " )
