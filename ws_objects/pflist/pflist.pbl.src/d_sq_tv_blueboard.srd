 retrieve="SELECT TAB1.MSGID BMSGID, IsNull(TAB2.PARENT, TAB1.MSGID) CMSGID,
       TAB1.FULLNAME BNAME, TAB2.FULLNAME CNAME,
       TAB1.MSGTIME BTIME, TAB2.MSGTIME CTIME,
       TAB1.MSGTEXT BTEXT, TAB2.MSGTEXT CTEXT
  FROM
(Select MSGID, USERS.FIRST_NAME + ' ' + USERS.LAST_NAME FULLNAME, MSGTIME, MSGTEXT from 
  BLUEBOARD Inner Join USERS On USERS.USERID = BLUEBOARD.USERID Where PARENT is Null and datediff(dd, MSGTIME, getdate()) < :MaxAge
  and PC_NR =:PCNR ) TAB1
 
Left Outer Join

(Select USERS.FIRST_NAME + ' ' + USERS.LAST_NAME FULLNAME, MSGTIME, MSGTEXT, PARENT from 
  BLUEBOARD Inner Join USERS On USERS.USERID = BLUEBOARD.USERID) TAB2

On TAB2.PARENT = TAB1.MSGID
Order By TAB1.MSGID Desc" arguments=(("PCNR", number),("MaxAge", number))  sort="btime D ctime A " )
