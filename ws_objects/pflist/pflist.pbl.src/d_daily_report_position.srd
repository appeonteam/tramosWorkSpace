 retrieve="SELECT CASE PF_CATEGORY.CATEGORYID WHEN 1 THEN 1 ELSE 0 END,  
         VESSELS.VESSEL_NAME,   
         PF_POSITION.OPENAREA,   
         PF_POSITION.OPENDATE,   
         PF_POSITION.LASTCARGO,   
         PF_POSITION.SECONDLAST,   
         PF_POSITION.THIRDLAST,   
         PF_POSITION.COMMENT,
        PROFIT_C.PC_NAME  
    FROM VESSELS 
RIGHT OUTER JOIN PF_POSITION ON VESSELS.VESSEL_NR = PF_POSITION.VESSELID 
LEFT OUTER JOIN PF_CATEGORY ON PF_POSITION.CATEGORYID = PF_CATEGORY.CATEGORYID  
LEFT OUTER JOIN PROFIT_C ON VESSELS.PC_NR = PROFIT_C.PC_NR
WHERE PF_POSITION.PCGROUP_ID = :pcgroup  AND  

((  VESSELS.PC_NR = 2 and (CONVERT(DATE,PF_POSITION.OPENDATE,103) >= CONVERT(DATE,getdate(),103)  
or CONVERT(DATE,PF_POSITION.AREA1,103) >= CONVERT(DATE,getdate(),103)   or CONVERT(DATE,PF_POSITION.AREA2,103) >= CONVERT(DATE,getdate(),103)     or 
  CONVERT(DATE,PF_POSITION.AREA3,103) >= CONVERT(DATE,getdate(),103)     or CONVERT(DATE,PF_POSITION.AREA4,103) >= CONVERT(DATE,getdate(),103)    )  
)
or  (VESSELS.PC_NR <> 2 and CONVERT(DATE,PF_POSITION.OPENDATE,103) >=CONVERT(DATE,getdate(),103) )     )

ORDER BY PROFIT_C.PC_NAME, PF_POSITION.OPENDATE" arguments=(("pcgroup", number)) )
