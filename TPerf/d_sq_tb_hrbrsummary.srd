table(column=(type=long updatewhereclause=yes name=hrbr_id dbname="TPERF_HRBR.H_ID" )
 column=(type=long updatewhereclause=yes name=tperf_hrbr_voy_id dbname="TPERF_HRBR.VOY_ID" )
 column=(type=long updatewhereclause=yes name=tperf_hrbr_h_type dbname="TPERF_HRBR.H_TYPE" values="Loading	0/Discharge	1/Load//Disch.	2/Repair//Dock	3/Other//Misc	4/" )
 column=(type=decimal(2) updatewhereclause=yes name=period dbname="PERIOD" )
 column=(type=number updatewhereclause=yes name=tperf_hrbr_is_dep dbname="TPERF_HRBR.IS_DEP" values="Arr	0/Dep	1/" )
 column=(type=char(50) updatewhereclause=yes name=tperf_hrbr_port dbname="TPERF_HRBR.PORT" )
 column=(type=char(20) updatewhereclause=yes name=tperf_hrbr_berth dbname="TPERF_HRBR.BERTH" )
 column=(type=decimal(1) updatewhereclause=yes name=con dbname="CON" )
 column=(type=decimal(1) updatewhereclause=yes name=bunk dbname="BUNK" )
 retrieve="  SELECT TPERF_HRBR.H_ID,   
         TPERF_HRBR.VOY_ID,   
         TPERF_HRBR.H_TYPE,   
         (TPERF_HRBR.PER_CARGO + TPERF_HRBR.PER_MAN + TPERF_HRBR.PER_IDLE + TPERF_HRBR.PER_MISC) AS PERIOD,   
         TPERF_HRBR.IS_DEP,   
         TPERF_HRBR.PORT,   
         TPERF_HRBR.BERTH,   
         SUM(ISNULL(CASE WHEN (TPERF_HRBRCON.CONTYPE<18) THEN (TPERF_HRBRCON.QTY_HFO + TPERF_HRBRCON.QTY_LSHFO + TPERF_HRBRCON.QTY_DO + TPERF_HRBRCON.QTY_GO) ELSE 0 END, 0)) AS CON,
         SUM(ISNULL(CASE WHEN (TPERF_HRBRCON.CONTYPE>18) THEN (TPERF_HRBRCON.QTY_HFO + TPERF_HRBRCON.QTY_LSHFO + TPERF_HRBRCON.QTY_DO + TPERF_HRBRCON.QTY_GO) ELSE 0 END, 0)) AS BUNK
    FROM TPERF_HRBR,   
         TPERF_HRBRCON  
   WHERE ( TPERF_HRBRCON.H_ID =* TPERF_HRBR.H_ID ) and  
         (( TPERF_HRBR.VOY_ID = :VoyID ))
	GROUP BY TPERF_HRBR.H_ID
" arguments=(("VoyID", number)) )
