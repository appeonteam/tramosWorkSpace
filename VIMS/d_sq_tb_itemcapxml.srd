 retrieve="  SELECT VETT_INSP.INSP_ID,
         INSPDATE,
		   VESSELIMO,
         MASTER, 
         CHENG,
			PORTS.PORT_N,
         '' as LOCATION,
         INSP_FNAME + ' ' + INSP_LNAME INSPNAME,
	      (VETT_INSPMODEL.NAME + Case When (EDITION='') or (EDITION is Null) Then '' Else ', ' End + EDITION) IMNAME,
         ITEM_ID,               
         INSPCOMM,
			'' VSLCOMM,
         VETT_ITEM.EXTNUM,
         TEXTDATA,   
         VETT_OBJ.REQTYPE,   
         VETT_OBJ.OBJNUM,
         RISK,
         CAP_STATUS,
			PAR1.OBJNUM AS P1NUM,
			PAR1.OBJTYPE AS P1TYPE,
			PAR2.OBJNUM AS P2NUM,
			PAR2.OBJTYPE AS P2TYPE,
			PAR3.OBJNUM AS P3NUM,
			PAR3.OBJTYPE AS P3TYPE,
         VETT_COMP.NAME COMPANY,
         (IsNull(CAP_GEN, 0) + 1) CAP_GEN
    FROM VETT_ITEM inner Join VETT_INSP on VETT_INSP.INSP_ID = VETT_ITEM.INSP_ID
         Inner Join VETT_INSPMODEL On VETT_INSP.IM_ID = VETT_INSPMODEL.IM_ID
			Inner Join VETT_COMP On VETT_INSP.COMP_ID = VETT_COMP.COMP_ID
         Inner Join PORTS On VETT_INSP.PORT = PORTS.PORT_CODE
	      left outer join VETT_OBJ on VETT_ITEM.OBJ_ID = VETT_OBJ.OBJ_ID
			left outer join VETT_OBJ PAR1 on PAR1.OBJ_ID = VETT_OBJ.PARENT
			left outer join VETT_OBJ PAR2 on PAR2.OBJ_ID = PAR1.PARENT
			left outer join VETT_OBJ PAR3 on PAR3.OBJ_ID = PAR2.PARENT
			left outer join VETT_TEXT on VETT_TEXT.TEXT_ID = VETT_OBJ.OBJTEXT
   WHERE (VETT_ITEM.INSP_ID = :InspID) and IS_CAP = 1 and CLOSED=0 and ((IsNull(CAP_GEN,0) = 0) Or (CAPEXPFLAG=1) Or (CAPEXPFLAG=11))
" arguments=(("InspID", number))  sort="compute_1 A " )
