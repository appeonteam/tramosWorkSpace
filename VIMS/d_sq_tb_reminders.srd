table(column=(type=long updatewhereclause=yes identity=yes name=insp_id dbname="VETT_INSP.INSP_ID" )
 column=(type=char(71) updatewhereclause=yes name=imname dbname="IMNAME" )
 column=(type=long updatewhereclause=yes name=remdays dbname="REMDAYS" )
 column=(type=long updatewhereclause=yes name=interval dbname="INTERVAL" )
 column=(type=long updatewhereclause=yes name=togo dbname="TOGO" )
 column=(type=datetime updatewhereclause=yes name=ldate dbname="LDATE" )
 column=(type=char(58) updatewhereclause=yes name=vslname dbname="VSLNAME" )
 column=(type=char(50) updatewhereclause=yes name=vessel_email dbname="VESSELS.VESSEL_EMAIL" )
 column=(type=char(20) updatewhereclause=yes name=resp dbname="VETT_INSP.RESP" )
 column=(type=char(50) updatewhereclause=yes name=deptname dbname="VETT_DEPT.DEPTNAME" )
 column=(type=char(200) updatewhereclause=yes name=deptnote dbname="VETT_DEPT.DEPTNOTE" )
 retrieve="Select INSP_ID, IMNAME, REMDAYS, INTERVAL, DateDiff(dd,getdate(),DateAdd(mm,INTERVAL,LDATE)) TOGO,LDATE,
VESSEL_REF_NR+' - '+VESSEL_NAME VSLNAME ,VESSEL_EMAIL,RESP, DEPTNAME,DEPTNOTE from VETT_INSP Inner Join
(Select VETT_INSP.IM_ID,VESSELIMO,Max(INSPDATE) LDATE, NAME+' ' + EDITION IMNAME, REMDAYS, INTERVAL
	from VETT_INSPMODEL Inner Join VETT_INSP On VETT_INSP.IM_ID=VETT_INSPMODEL.IM_ID 
	Where REMDAYS>0 And INTERVAL is not null Group By VETT_INSP.IM_ID,VESSELIMO,NAME,EDITION, REMDAYS,INTERVAL) IMSM 
On IMSM.IM_ID=VETT_INSP.IM_ID and INSPDATE=LDATE And VETT_INSP.VESSELIMO=IMSM.VESSELIMO
Inner Join VESSELS On VETT_INSP.VESSELIMO=IMO_NUMBER and VESSEL_ACTIVE=1 
Inner Join VETT_VSLTYPE On VESSELS.VETT_TYPE=VETT_VSLTYPE.TYPE_ID
Left Outer Join USERS On RESP=USERID
Left Outer Join VETT_DEPT On USERS.VETT_DEPT=DEPT_ID
Where REMINDER is null and VETT_VSLTYPE.TYPE_NAME not like '%T/C%' and VETT_VSLTYPE.TYPE_NAME not like '%3P%'
"  sort="imname A vslname A " )
